<autobuild>
    <configuration>

	<environment name="ACE_ROOT"  value="/export/home/bugzilla/FACET/ACE_wrappers" />
        <environment name="TAO_ROOT"  value="/export/home/bugzilla/FACET/ACE_wrappers/TAO"/>
        <environment name="JACE_ROOT" value="/export/home/bugzilla/FACET/RTEvent/tao-facet-adapter/jACE" />
        <environment name="LD_LIBRARY_PATH" value="/export/home/bugzilla/FACET/ACE_wrappers/ace" />

        <variable name="root"     value="/export/home/bugzilla/FACET/RTEvent" />
        <variable name="log_file" value="build.txt" />
        <variable name="log_root" value="/home/cs/group/bugzilla/.www-docs/auto_compile_logs/hokey_FACET_jACE" />
        <variable name="configs"  value="Linux" />

    </configuration>

    <command name="status"            options="on" />
    <command name="file_manipulation" options="type=mustnotexist file=.disable" />
    <command name="file_manipulation" options="type=create file=.disable output='RUNNING'" />
    <command name="log"               options="on" />

    <command name="make"              options="-C jACE/jace" />
    <command name="make"              options="-C jACE/jace install" />

    <command name="log"               options="off" />
    <command name="process_logs"      options="clean move prettify index" />
    <command name="file_manipulation" options="type=delete file=.disable" />
    <command name="status"            options="off" />
</autobuild>
