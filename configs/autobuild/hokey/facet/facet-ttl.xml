<!-- $Id$ -->
<autobuild>
    <configuration>

        <environment name="CVSROOT"         value=":ext:wustlbuilds@cvs.doc.wustl.edu:/project/cvs-repository" />
        <environment name="CVS_RSH"         value="ssh" />
	<environment name="ACE_ROOT"        value="/export/home/bugzilla/facet/ACE_wrappers" />
        <environment name="TAO_ROOT"        value="/export/home/bugzilla/facet/ACE_wrappers/TAO"/>
        <environment name="JACE_ROOT"       value="/export/home/bugzilla/facet/RTEvent/tao-facet-adapter/jACE" />
        <environment name="LD_LIBRARY_PATH" value="/export/home/bugzilla/facet/ACE_wrappers/ace" />
        <environment name="ANT_HOME"        value="/docpkg/ant" />
        <environment name="ASPECTJ_HOME"    value="/docpkg/aspectj_1.1" />

        <environment name="CLASSPATH" value="/docpkg/jacorb/lib/idl.jar"                type="replace"/>
        <environment name="CLASSPATH" value="/docpkg/jacorb/lib/jacorb.jar"             type="suffix"/>
        <environment name="CLASSPATH" value="/docpkg/dlconcurrent/lib/dlconcurrent.jar" type="suffix"/>
        <environment name="CLASSPATH" value="/docpkg/junit/junit.jar"                      type="suffix"/>
        <environment name="CLASSPATH" value="/docpkg/aspectj_1.1/lib/aspectjrt.jar"         type="suffix"/>
        <environment name="CLASSPATH" value="/docpkg/aspectj_1.1/lib/aspectj-ant.jar"       type="suffix"/>
        <environment name="CLASSPATH" value="/docpkg/aspectj_1.1/lib/aspectjtools.jar"      type="suffix"/>
        <environment name="CLASSPATH" value="/docpkg/ant/lib/ant.jar"             type="suffix"/>
        <environment name="CLASSPATH" value="/docpkg/ant/lib/ant-optional.jar"    type="suffix"/>
        <environment name="CLASSPATH" value="/docpkg/log4j/dist/lib/log4j.jar"          type="suffix"/>
        <environment name="CLASSPATH" value="/export/home/bugzilla/facet/RTEvent/facet/classes" type="suffix"/>
        <environment name="CLASSPATH" value="." type="suffix"/>

        <environment name="PATH" value="/export/home/bugzilla/facet/ACE_wrappers/ace" type="replace" />
        <environment name="PATH" value="/docpkg/jikes/bin"                   type="suffix"/>
        <environment name="PATH" value="/docpkg/jacorb/bin"                 type="suffix"/>
        <environment name="PATH" value="/docpkg/java_/bin"                   type="suffix"/>
        <environment name="PATH" value="/docpkg/aspectj_1.1/bin"                type="suffix"/>
        <environment name="PATH" value="/docpkg/ant/bin"              type="suffix"/>
        <environment name="PATH" value="/bin"                                     type="suffix"/>
        <environment name="PATH" value="/usr/bin"                                 type="suffix"/>
        <environment name="PATH" value="."                                        type="suffix"/>

        <environment name="JAVA_HOME"       value="/docpkg/java" />
        <environment name="LIBRARYCLASSPATH" value="/docpkg/java/jre/lib/rt.jar:." />

        <variable name="root"               value="/export/home/bugzilla/facet" />
        <variable name="project_root"       value="/export/home/bugzilla/facet/RTEvent/facet" />
        <variable name="log_file"           value="build.txt" />
        <variable name="log_root" value="/home/cs/group/bugzilla/.www-docs/auto_compile_logs/hokey_facet/ttl" />
        <variable name="configs"            value="Linux" />
        <variable name="make_program"       value="ant" />

    </configuration>

    <command name="status"            options="on" />
    <command name="file_manipulation" options="type=mustnotexist file=.disable" />
    <command name="file_manipulation" options="type=create file=.disable output='RUNNING'" />
    <command name="log"               options="on" />
    <command name="cvs"               options="-z 3 -q update -Pd RTEvent/facet" />

    <command name="make"	      options="clean" />
    <command name="make"              options="-Duse_feature_disable_corba=t -Duse_feature_ttl=t" />

    <command name="auto_run_tests"    />

    <command name="log"               options="off" />
    <command name="process_logs"      options="clean move prettify index" />
    <command name="file_manipulation" options="type=delete file=.disable" />
    <command name="status"            options="off" />
</autobuild>
