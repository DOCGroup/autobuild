<autobuild>
    <configuration>

        <environment name="ACE_ROOT"        value="/opt/data/acebuild/ACE_wrappers/build/linux-x86-sctp" />
        <environment name="TAO_ROOT"        value="/opt/data/acebuild/ACE_wrappers/build/linux-x86-sctp/TAO" />
        <environment name="CIAO_ROOT"        value="/opt/data/acebuild/ACE_wrappers/build/linux-x86-sctp/TAO/CIAO" />
        <environment name="DANCE_ROOT"        value="/opt/data/acebuild/ACE_wrappers/build/linux-x86-sctp/TAO/CIAO/DAnCE" />
        <environment name="LD_LIBRARY_PATH" value="/opt/data/acebuild/ACE_wrappers/build/linux-x86-sctp/lib:/opt/data/acebuild/ACE_wrappers/build/linux-x86-sctp/ace:/opt/data/acebuild/lksctp-tools/src/lib/.libs:" />

        <variable name="project_root" value="/opt/data/acebuild/ACE_wrappers/build/linux-x86-sctp" />
        <variable name="build_name" value="linux-x86-sctp" />
        <variable name="root"     value="/opt/data/acebuild" />
        <variable name="log_file" value="build-x86-sctp.txt" />
        <variable name="log_root" value="/opt/data/acebuild/buildlog.x86-sctp" />
        <variable name="configs"  value="Linux x86 NOXERCES" />

    </configuration>

    <command name="status"            options="on" />
    <command name="file_manipulation" options="type=mustnotexist file=.disable-x86-sctp" />
    <command name="file_manipulation" options="type=create file=.disable-x86-sctp output='RUNNING'" />
    <command name="log"               options="ON" />

    <command name="create_ace_build"  />

    <command name="svn"               options="co svn://svn.dre.vanderbilt.edu/DOC/Middleware/sets-anon/ACE+TAO+CIAO ." />

    <command name="shell"             options="find ACE_wrappers/build/linux-x86-sctp -name 'GNUmakefile*' -exec rm '{}' ';'" />
    <command name="shell"             options="rm -f ACE_wrappers/build/linux-x86-sctp/bin/MakeProjectCreator/config/default.features" />

    <command name="file_manipulation" options="type=create file=ACE_wrappers/build/linux-x86-sctp/ace/config.h output='#include \x22ace/config-linux.h\x22\n'" />
    <command name="file_manipulation" options="type=create file=ACE_wrappers/build/linux-x86-sctp/include/makeinclude/platform_macros.GNU output='debug=0\nsctp=lksctp\noptimize=1\ninline=1\nssl=1\nqos=0\n\ninclude $(ACE_ROOT)/include/makeinclude/platform_linux.GNU\nPLATFORM_SCTP_CPPFLAGS=-I/opt/data/acebuild/lksctp-tools/src/include/\nPLATFORM_SCTP_LDFLAGS=-L/opt/data/acebuild/lksctp-tools/src/lib/.libs\n'" />

    <command name="file_manipulation" options="type=create file=ACE_wrappers/build/linux-x86-sctp/bin/MakeProjectCreator/config/default.features output='ssl=1\nqos=0\nrwho=0\nsctp=1\n'" />

    <command name="print_os_version" />
    <command name="print_env_vars" />
    <command name="check_compiler" options="gcc" />
    <command name="print_ace_config" />

    <command name="generate_workspace" options="-type gnuace" />

    <command name="make"              options="-k realclean" />
    <command name="make"              options="-k -j 2" />

    <command name="auto_run_tests"    options="-a -o -t -C" />
    <command name="process_logs"      options="clean" />
    <command name="log"               options="OFF" />
    <command name="process_logs"      options="clean move prettify index" />
    <command name="file_manipulation" options="type=delete file=.disable-x86-sctp" />
    <command name="status"            options="off" />

</autobuild>
