<!-- $Id$ -->
<autobuild>
    <configuration>

        <environment name="ACE_ROOT"        value="/acebuilds/ACE_wrappers/build/autoconf_va6_debug" />
        <environment name="CVSROOT"         value=":ext:shuston@cvs.doc.wustl.edu:/project/cvs-repository" />
        <environment name="CVS_RSH"         value="ssh" />

        <variable name="root"         value="/acebuilds/ACE_wrappers" />
        <variable name="build_name"   value="autoconf_va6_debug" />
        <variable name="project_root" value="build/autoconf_va6_debug" />
        <variable name="log_file"     value="autoconf_va6_debug.txt" />
        <variable name="log_root"     value="/jane/acebuilds/logs/cuddy/dev/autoconf_va6_debug" />

    </configuration>

    <command name="status"            options="on" />
    <command name="file_manipulation" options="type=mustnotexist file=.disable" />
    <command name="file_manipulation" options="type=create file=.disable output='RUNNING'" />
    <command name="log"               options="on" />

    <command name="cvs"               options="-z 3 -q update -P -d ." />
    <command name="cvs"               options="-z 3 -q checkout -P MPC"/>

    <command name="file_manipulation" options="type=delete file=.disable" />

    <command name="file_manipulation" options="type=mustnotexist file=build/autoconf_va6_debug/.disable" />

    <command name="print_os_version" />
    <command name="check_compiler"    options="vacpp" />
    <command name="print_ace_config" />
    <command name="configure"         options="CXX=xlC_r ACE_TLS_CPPFLAGS=-I/opt/freeware/include ACE_TLS_LDFLAGS=-L/opt/freeware/lib --preclean" />
    <command name="file_manipulation" options="type=create file=build/autoconf_va6_debug/.disable output='RUNNING'" />

    <command name="make"              options="-j 2 -k" />

    <command name="auto_run_tests"    options="-a" />

    <command name="log"               options="off" />
    <command name="process_logs"      options="clean=5 move prettify index" />
    <command name="file_manipulation" options="type=delete file=build/autoconf_va6_debug/.disable" />
    <command name="status"            options="off" />
</autobuild>
