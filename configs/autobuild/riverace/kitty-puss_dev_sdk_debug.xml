<!-- $Id$ -->
<autobuild>
    <configuration>
        <environment name="ACE_ROOT"     value="C:\ace\ACE_wrappers-repository" />
        <environment name="PATH"         value="C:\ace\ACE_wrappers-repository\lib" type="suffix" />
        <environment name="CVSROOT"  value=":ext:shuston@cvs.doc:/project/cvs-repository" />
        <environment name="MPC_ROOT" value="C:\ace\MPC" />

        <variable name="root"            value="C:\ace" />
        <variable name="project_root"    value="C:\ace\ACE_wrappers-repository" />
	<variable name="log_file"        value="vc7_debug.log" />
        <variable name="log_root"        value="g:\build_logs\dev\vc7_debug" />
        <variable name="name"            value="WinXP VC.NET Debug" />
        <variable name="make_program"    value="nmake /F" />
        <variable name="configs"         value="Win32 Exceptions" />
    </configuration>

    <command name="status"            options="on" />
    <command name="file_manipulation" options="type=mustnotexist file=ACE_wrappers-repository/.disable" />
    <command name="file_manipulation" options="type=create file=ACE_wrappers-repository/.disable output='RUNNING'" />

    <command name="make"           options="dir=ace ace.nmake CFG='Win64 Debug' REALCLEAN" />
    <command name="make"           options="dir=ACEXML ACEXML.nmake CFG='Win64 Debug' REALCLEAN" />
    <command name="make"           options="dir=protocols protocols.nmake CFG='Win64 Debug' REALCLEAN" />
    <command name="make"           options="dir=ASNMP ASNMP.nmake CFG='Win64 Debug' REALCLEAN" />
    <command name="make"           options="dir=apps apps.nmake CFG='Win64 Debug' REALCLEAN" />
    <command name="make"           options="dir=examples examples.nmake CFG='Win64 Debug' REALCLEAN" />
    <command name="make"           options="dir=performance-tests perf.nmake CFG='Win64 Debug' REALCLEAN" />
    <command name="make"           options="dir=tests tests.nmake CFG='Win64 Debug' REALCLEAN" />

    <command name="log"               options="ON" />

    <command name="cvs"               options="checkout -P MPC" />
    <command name="cvs"               options="checkout -P ACE" />

    <command name="file_manipulation" options="type=create file=ACE_wrappers-repository/ace/config.h output='#define ACE_DISABLE_WIN32_ERROR_WINDOWS\n#define ACE_HAS_STANDARD_CPP_LIBRARY 1\n#include \x22ace/config-win32.h\x22\n'" />

    <command name="print_os_version" />
    <command name="check_compiler" options="msvc" />
    <command name="print_ace_config" />

    <command name="file_manipulation" options="type=create file=ACE_wrappers-repository/local.features output='rwho=0\nqos=1\n'" />
    <command name="generate_workspace" options="-feature_file c:\ace\ACE_wrappers-repository\local.features -relative ACE_ROOT=C:\ace\ACE_wrappers-repository -recurse -make_coexistence -type nmake -value_template platforms=Win64" />

    <command name="make"           options="dir=ace ace.nmake CFG='Win64 Debug'" />
    <command name="make"           options="dir=ACEXML ACEXML.nmake CFG='Win64 Debug'" />
    <command name="make"           options="dir=protocols protocols.nmake CFG='Win64 Debug'" />
    <command name="make"           options="dir=ASNMP ASNMP.nmake CFG='Win64 Debug'" />
    <command name="make"           options="dir=apps apps.nmake CFG='Win64 Debug'" />
    <command name="make"           options="dir=examples examples.nmake CFG='Win64 Debug'" />
    <command name="make"           options="dir=performance-tests perf.nmake CFG='Win64 Debug'" />
    <command name="make"           options="dir=tests tests.nmake CFG='Win64 Debug'" />
    <command name="auto_run_tests"    options="-a" />

    <command name="log"               options="OFF" />
    <command name="process_logs"      options="move prettify clean=5 index" />
    <command name="file_manipulation" options="type=delete file=ACE_wrappers-repository/.disable" />
    <command name="status"            options="off" />
</autobuild>
