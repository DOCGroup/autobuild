<!-- $Id$ -->
<autobuild>
    <configuration>

        <environment name="ACE_ROOT"        value="/export/home/shuston/ace/builds/ACE_wrappers" />
        <environment name="LD_LIBRARY_PATH" value="/export/home/shuston/ace/builds/ACE_wrappers/build/autoconf_mpc_forte6_debug/ace:/usr/local/ssl/lib:/usr/lib" />
        <environment name="CVSROOT"         value=":ext:shuston@cvs.doc.wustl.edu:/project/cvs-repository" />
        <environment name="CVS_RSH"         value="ssh" />
        <!-- autom4te uses M4 or /usr/ccs/bin/m4, which isn't GNU. -->
        <environment name="M4"              value="/usr/local/bin/m4" />
        <environment name="MPC_ROOT"        value="/export/home/shuston/ace/MPC" />
        <environment name="SSL_EGD_FILE"    value="/etc/entropy" />

        <variable name="root"         value="/export/home/shuston/ace/builds/ACE_wrappers" />
        <variable name="build_name"   value="autoconf_mpc_forte6_debug" />
        <variable name="project_root" value="build/autoconf_mpc_forte6_debug" />
        <variable name="log_file"     value="autoconf_mpc_forte6_debug.txt" />
        <variable name="log_root"     value="/jane/acebuilds/logs/heidi/dev/autoconf_mpc_forte6_debug" />
        <variable name="automake_build" value="yes" />

    </configuration>

    <command name="status"            options="on" />
    <command name="file_manipulation" options="type=mustnotexist file=.disable" />
    <command name="file_manipulation" options="type=create file=.disable output='RUNNING'" />
    <command name="log"               options="on" />
    <!-- Delete all the Makefile.am files so cvs checkout won't complain. -->
    <command name="shell"             options="find /export/home/shuston/ace/builds/ACE_wrappers -name 'Makefile.am' -exec rm '{}' ';'" />
    <command name="cvs"               options="-z 3 -q update -P -d ." />
    <!-- Now, delete them again to insure a clean slate. -->
    <command name="shell"             options="find /export/home/shuston/ace/builds/ACE_wrappers -name 'Makefile.am' -exec rm '{}' ';'" />

    <command name="file_manipulation" options="type=delete file=.disable" />

    <command name="file_manipulation" options="type=mustnotexist file=build/autoconf_mpc_forte6_debug/.disable" />


    <command name="print_os_version" />
    <command name="check_compiler"    options="sun_cc" />
    <command name="print_ace_config" />
    <!-- Uses ACE.mwc -->
    <command name="generate_workspace" options="-type automake -exclude TAO,build.mwc" />
    <command name="configure"         options="CXX=CC --preclean" />
    <command name="file_manipulation" options="type=create file=build/autoconf_mpc_forte6_debug/.disable output='RUNNING'" />
    <command name="make"              options="-j 2 -k" />

    <command name="auto_run_tests"    options="dir=build/autoconf_mpc_forte6_debug" />

    <command name="log"               options="off" />
    <command name="process_logs"      options="clean=5 move prettify index" />
    <command name="file_manipulation" options="type=delete file=build/autoconf_mpc_forte6_debug/.disable" />
    <command name="status"            options="off" />
</autobuild>
