<!-- $Id$ -->
<autobuild>
    <configuration>
        <environment name="ACE_ROOT"     value="C:\ace\ACE_wrappers-repository" />
        <environment name="PATH"     value="C:\ace\ACE_wrappers-repository\bin" type="prefix" />
        <environment name="SSL_ROOT"         value="C:\OpenSSL" />
        <environment name="CVSROOT"  value=":ext:shuston@cvs.doc:/project/cvs-repository" />
        <environment name="MPC_ROOT" value="C:\ace\MPC" />

        <variable name="root"     value="C:/ace" />
        <variable name="project_root" value="C:\ace\ACE_wrappers-repository" />
	<variable name="log_file" value="msvc6_debug.txt" />
        <variable name="log_root" value="G:\build_logs\dev\msvc6_debug" />
        <variable name="name"     value="Windows 2000 MSVC6 Debug" />
        <variable name="configs"  value="Win32 SSL Exceptions" />
    </configuration>

    <command name="status"            options="on" />
    <command name="file_manipulation" options="type=mustnotexist file=ACE_wrappers-repository/.disable" />
    <command name="file_manipulation" options="type=create file=ACE_wrappers-repository/.disable output='RUNNING'" />
    <command name="win32make"         options="msvc_mpc_auto_compile -dir ace -dir ACEXML -dir apps -dir ASNMP -dir examples -dir netsvcs -dir performance-tests -dir websvcs -dir tests -Debug -u -k -clean" />

    <command name="log"               options="ON" />

    <command name="cvs"               options="checkout -P MPC" />
    <command name="cvs"               options="checkout -P ACE" />

    <command name="print_os_version" />
    <command name="check_compiler"    options="msvc" />
    <command name="print_ace_config" />

    <command name="file_manipulation" options="type=create file=ACE_wrappers-repository/local.features output='rwho=0\nssl=1\nqos=1\n'" />
    <command name="generate_workspace" options="-feature_file c:\ace\ACE_wrappers-repository\local.features -relative ACE_ROOT=C:\ace\ACE_wrappers-repository -recurse -make_coexistence -type vc6" /> 

    <command name="win32make"         options="msvc_mpc_auto_compile -dir ace -dir ACEXML -dir apps -dir ASNMP -dir examples -dir netsvcs -dir performance-tests -dir websvcs -dir tests -Debug -u -k" />
    <command name="auto_run_tests"    options="-a" />

    <command name="log"               options="OFF" />
    <command name="process_logs"      options="move prettify clean=5 index" />
    <command name="file_manipulation" options="type=delete file=ACE_wrappers-repository/.disable" />
    <command name="status"            options="off" />
</autobuild>
