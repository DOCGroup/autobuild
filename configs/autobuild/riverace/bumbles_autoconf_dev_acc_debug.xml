<!-- $Id$ -->
<autobuild>
    <configuration>

        <environment name="ACE_ROOT"        value="/acebuilds/autoconf/ACE_wrappers/build/acc_debug" />
        <environment name="TAO_ROOT"        value="/acebuilds/autoconf/ACE_wrappers/build/acc_debug/TAO"/>
        <environment name="CVSROOT"         value=":ext:shuston@cvs.doc.wustl.edu:/project/cvs-repository" />
        <environment name="CVS_RSH"         value="ssh" />

        <variable name="root"         value="/acebuilds/autoconf/ACE_wrappers" />
        <variable name="build_name"   value="acc_debug" />
        <variable name="project_root" value="build/acc_debug" />
        <variable name="log_file"     value="autoconf_acc_debug.txt" />
        <variable name="log_root"     value="/jane/acebuilds/logs/bumbles/dev/autoconf_acc_debug" />

    </configuration>

    <command name="status"            options="on" />
    <command name="file_manipulation" options="type=mustnotexist file=.disable" />
    <command name="file_manipulation" options="type=create file=.disable output='RUNNING'" />
    <command name="log"               options="on" />

    <command name="cvs"               options="-z 3 -q update -P -d ." />

    <command name="file_manipulation" options="type=delete file=.disable" />

    <command name="file_manipulation" options="type=mustnotexist file=build/acc_debug/.disable" />

    <command name="print_os_version" />
    <command name="check_compiler"    options="aCC" />
    <command name="print_ace_config" />
    <command name="configure"         options="CXX=aCC --preclean" />
    <command name="file_manipulation" options="type=create file=build/acc_debug/.disable output='RUNNING'" />

    <command name="make"              options="-j 2 -k" />

    <command name="auto_run_tests"    options="-a" />

    <command name="log"               options="off" />
    <command name="process_logs"      options="clean move prettify index" />
    <command name="file_manipulation" options="type=delete file=build/acc_debug/.disable" />
    <command name="status"            options="off" />
</autobuild>
