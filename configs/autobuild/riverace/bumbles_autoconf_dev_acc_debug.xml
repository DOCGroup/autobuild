<!-- $Id$ -->
<autobuild>
    <configuration>

        <environment name="ACE_ROOT"        value="/acebuilds/ACE_wrappers-repository/build/autoconf_acc_debug" />
        <environment name="LD_LIBRARY_PATH" value="/acebuilds/ACE_wrappers-repository/build/autoconf_acc_debug/lib:/usr/lib" />
        <environment name="CVSROOT"         value=":ext:shuston@cvs.doc.wustl.edu:/project/cvs-repository" />
        <environment name="CVS_RSH"         value="ssh" />
        <environment name="MPC_ROOT"        value="/acebuilds/MPC" />

        <variable name="root"         value="/acebuilds" />
        <variable name="build_name"   value="autoconf_acc_debug" />
        <variable name="project_root" value="/acebuilds/ACE_wrappers-repository/build/autoconf_acc_debug" />
        <variable name="log_file"     value="ACE_wrappers-repository/autoconf_acc_debug.txt" />
        <variable name="log_root"     value="/jane/acebuilds/logs/bumbles/dev/autoconf_acc_debug" />
        <variable name="configs"      value="Exceptions" />

    </configuration>

    <command name="status"            options="on" />
    <command name="file_manipulation" options="type=mustnotexist file=ACE_wrappers-repository/.disable" />
    <command name="file_manipulation" options="type=create file=ACE_wrappers-repository/.disable output='RUNNING'" />
    <command name="log"               options="on" />

    <command name="cvs"               options="checkout -P MPC" />
    <command name="cvs"               options="checkout -P ACE" />

    <command name="file_manipulation" options="type=delete file=ACE_wrappers-repository/.disable" />

    <command name="file_manipulation" options="type=mustnotexist file=ACE_wrappers-repository/build/autoconf_acc_debug/.disable" />
    <command name="file_manipulation" options="type=create file=ACE_wrappers-repository/build/autoconf_acc_debug/.disable output='RUNNING'" />

    <command name="print_os_version" />
    <command name="check_compiler"    options="aCC" />
    <command name="print_ace_config" />
    <command name="configure"         options="CXX=aCC --preclean --with-ssl=no" />

    <command name="make"              options="-j 2 -k" />

    <command name="auto_run_tests"    options="-a" />

    <command name="log"               options="off" />
    <command name="process_logs"      options="clean=5 move prettify index" />
    <command name="file_manipulation" options="type=delete file=ACE_wrappers-repository/build/autoconf_acc_debug/.disable" />
    <command name="status"            options="off" />
</autobuild>
