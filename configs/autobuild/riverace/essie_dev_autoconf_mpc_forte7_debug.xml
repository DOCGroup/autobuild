<!-- $Id$ -->
<autobuild>
    <configuration>

        <environment name="ACE_ROOT"        value="/home/shuston/ace/builds/ACE_wrappers-repository/build/autoconf_mpc_forte7_debug" />
        <environment name="LD_LIBRARY_PATH" value="/home/shuston/ace/builds/ACE_wrappers-repository/build/autoconf_mpc_forte7_debug/lib:/usr/local/ssl/lib:/usr/lib" />
        <environment name="CVSROOT"         value=":ext:shuston@cvs.doc.wustl.edu:/project/cvs-repository" />
        <environment name="CVS_RSH"         value="ssh" />
        <!-- autom4te uses M4 or /usr/ccs/bin/m4, which isn't GNU. -->
        <environment name="M4"              value="/usr/local/bin/m4" />
        <environment name="MPC_ROOT"        value="/home/shuston/ace/builds/MPC" />

        <variable name="root"         value="/home/shuston/ace/builds" />
        <variable name="build_name"   value="autoconf_mpc_forte7_debug" />
        <variable name="project_root" value="/home/shuston/ace/builds/ACE_wrappers-repository" />
        <variable name="log_file"     value="autoconf_mpc_forte7_debug.txt" />
        <variable name="log_root"     value="/jane/acebuilds/logs/essie/dev/autoconf_mpc_forte7_debug" />
        <variable name="configs"      value="Exceptions" />
        <variable name="automake_build" value="yes" />

    </configuration>

    <command name="status"            options="on" />
    <command name="file_manipulation" options="type=mustnotexist file=ACE_wrappers-repository/.disable" />
    <command name="file_manipulation" options="type=create file=ACE_wrappers-repository/.disable output='RUNNING'" />
    <command name="log"               options="on" />

    <command name="cvs"               options="checkout -P MPC" />
    <command name="cvs"               options="checkout -P ACE" />

    <command name="file_manipulation" options="type=delete file=ACE_wrappers-repository/.disable" />

    <command name="file_manipulation" options="type=mustnotexist file=ACE_wrappers-repository/build/autoconf_mpc_forte7_debug/.disable" />
    <command name="file_manipulation" options="type=create file=ACE_wrappers-repository/build/autoconf_mpc_forte7_debug/.disable output='RUNNING'" />

    <command name="print_os_version" />
    <command name="check_compiler"    options="sun_cc" />
    <command name="print_ace_config" />

    <!-- command name="file_manipulation" options="type=create file=ACE_wrappers-repository/local.features output='ssl=1\nqos=1\n'" -->
    <!-- command name="generate_workspace" options="dirs=/home/shuston/ace/builds/ACE_wrappers-repository -feature_file /home/shuston/ace/builds/ACE_wrappers-repository/local.features -relative ACE_ROOT=/home/shuston/ace/builds/ACE_wrappers-repository -type automake -exclude build,Kokyu" -->

    <command name="shell"             options="(cd ACE_wrappers-repository; ./bin/bootstrap)" />

    <command name="configure"         options="CXX=CC --preclean" />
    <command name="make"              options="dir=build/autoconf_mpc_forte7_debug -j 2 -k" />

    <command name="auto_run_tests"    options="dir=build/autoconf_mpc_forte7_debug" />

    <command name="log"               options="off" />
    <command name="process_logs"      options="clean=5 move prettify index" />
    <command name="file_manipulation" options="type=delete file=ACE_wrappers-repository/build/autoconf_mpc_forte7_debug/.disable" />
    <command name="status"            options="off" />
</autobuild>
