<!-- $Id$ -->
<autobuild>
    <configuration>

        <environment name="ACE_ROOT"        value="/export/home/shuston/ace/builds/ACE_wrappers-repository/build/forte6_debug" />
        <environment name="LD_LIBRARY_PATH" value="/export/home/shuston/ace/builds/ACE_wrappers-repository/build/forte6_debug/lib:/usr/local/ssl/lib:/usr/lib" />
        <environment name="CVSROOT"         value=":ext:shuston@cvs.doc.wustl.edu:/project/cvs-repository" />
        <environment name="CVS_RSH"         value="ssh" />
        <environment name="MPC_ROOT"        value="/export/home/shuston/ace/builds/MPC" />

        <variable name="root"         value="/export/home/shuston/ace/builds" />
        <variable name="build_name"   value="forte6_debug" />
        <variable name="project_root" value="/export/home/shuston/ace/builds/ACE_wrappers-repository/build/forte6_debug" />
        <variable name="log_file"     value="ACE_wrappers-repository/forte6_debug.txt" />
        <variable name="log_root"     value="/jane/acebuilds/logs/heidi/dev/forte6_debug" />

    </configuration>

    <command name="status"            options="on" />
    <command name="file_manipulation" options="type=mustnotexist file=ACE_wrappers-repository/.disable" />
    <command name="file_manipulation" options="type=create file=ACE_wrappers-repository/.disable output='RUNNING'" />
    <command name="log"               options="on" />

    <command name="make"              options="-k reverseclean" />

    <command name="cvs"               options="checkout -P MPC" />
    <command name="cvs"               options="checkout -P ACE" />

    <command name="file_manipulation" options="type=delete file=ACE_wrappers-repository/.disable" />

    <command name="file_manipulation" options="type=mustnotexist file=ACE_wrappers-repository/build/forte6_debug/.disable" />

    <command name="create_ace_build"  options="-a -v" />
    <command name="file_manipulation" options="type=create file=ACE_wrappers-repository/build/forte6_debug/.disable output='RUNNING'" />

    <!-- Delete the build's Makefiles before regenerating them. -->
    <command name="shell"             options="find ACE_wrappers-repository/build/forte6_debug -name 'Makefile*' -exec rm '{}' ';'" />
    <command name="shell"             options="find ACE_wrappers-repository/build/forte6_debug -name 'GNUmakefile*' -exec rm '{}' ';'" />

    <command name="file_manipulation" options="type=create file=ACE_wrappers-repository/local.features output='ssl=1\nqos=1\n'" />
    <command name="generate_workspace" options="-feature_file /export/home/shuston/ace/builds/ACE_wrappers-repository/local.features -relative ACE_ROOT=/export/home/shuston/ace/builds/ACE_wrappers-repository/build/forte6_debug -type gnuace -hierarchy -make_coexistence" />

    <command name="file_manipulation" options="type=create file=ACE_wrappers-repository/build/forte6_debug/ace/config.h output='#define ACE_HAS_IPV6\n#include \x22ace/config-sunos5.8.h\x22\n'" />
    <command name="file_manipulation" options="type=create file=ACE_wrappers-repository/build/forte6_debug/include/makeinclude/platform_macros.GNU output='ssl=1\ndebug=1\ntemplates=implicit\nSSL_ROOT=/usr/local/ssl\ninclude $(ACE_ROOT)/include/makeinclude/platform_sunos5_sunc++.GNU\n'" />

    <command name="print_os_version" />
    <command name="check_compiler"    options="sun_cc" />
    <command name="print_ace_config" />

    <command name="make"              options="-j 2 -k" />

    <command name="auto_run_tests"    options="-a" />

    <command name="log"               options="off" />
    <command name="process_logs"      options="clean=5 move prettify index" />
    <command name="file_manipulation" options="type=delete file=ACE_wrappers-repository/build/forte6_debug/.disable" />
    <command name="status"            options="off" />
</autobuild>
