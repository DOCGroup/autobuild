<!-- $Id$ -->
<autobuild>
    <configuration>

        <environment name="ACE_ROOT"        value="/export/home/shuston/ace/builds/ACE_wrappers/build/forte6_debug" />
        <environment name="LD_LIBRARY_PATH" value="/export/home/shuston/ace/builds/ACE_wrappers/build/forte6_debug/ace:/usr/local/ssl/lib:/usr/lib" />
        <environment name="CVSROOT"         value=":ext:shuston@cvs.doc.wustl.edu:/project/cvs-repository" />
        <environment name="CVS_RSH"         value="ssh" />

        <variable name="root"         value="/export/home/shuston/ace/builds/ACE_wrappers" />
        <variable name="build_name"   value="forte6_debug" />
        <variable name="project_root" value="build/forte6_debug" />
        <variable name="log_file"     value="forte6_debug.txt" />
        <variable name="log_root"     value="/jane/acebuilds/logs/heidi/dev/forte6_debug" />

    </configuration>

    <command name="status"            options="on" />
    <command name="file_manipulation" options="type=mustnotexist file=.disable" />
    <command name="file_manipulation" options="type=create file=.disable output='RUNNING'" />
    <command name="log"               options="on" />

    <command name="cvs"               options="-z 3 -q update -P -d ." />

    <command name="file_manipulation" options="type=delete file=.disable" />

    <command name="file_manipulation" options="type=mustnotexist file=build/forte6_debug/.disable" />
    <command name="create_ace_build"  options="-a -v" />
    <command name="file_manipulation" options="type=create file=build/forte6_debug/.disable output='RUNNING'" />

    <command name="file_manipulation" options="type=create file=build/forte6_debug/ace/config.h output='#include \x22ace/config-sunos5.8.h\x22\n'" />
    <command name="file_manipulation" options="type=create file=build/forte6_debug/include/makeinclude/platform_macros.GNU output='ssl=1\ndebug=1\nPLATFORM_SSL_CPPFLAGS=-I/usr/local/ssl/include\nPLATFORM_SSL_LIBS=-lssl -lcrypto\nPLATFORM_SSL_LDFLAGS=-L/usr/local/ssl/lib\ninclude $(ACE_ROOT)/include/makeinclude/platform_sunos5_sunc++.GNU\n'" />

    <command name="print_os_version" />
    <command name="check_compiler"    options="sun_cc" />
    <command name="print_ace_config" />

    <command name="make"              options="-k realclean" />
    <command name="make"              options="-j 2 -k" />

    <command name="auto_run_tests"    options="-a" />

    <command name="log"               options="off" />
    <command name="process_logs"      options="clean move prettify index" />
    <command name="file_manipulation" options="type=delete file=build/forte6_debug/.disable" />
    <command name="status"            options="off" />
</autobuild>
