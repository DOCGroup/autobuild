<!-- $Id$ -->
<autobuild>
    <configuration>

        <environment name="ACE_ROOT"        value="/acebuilds/ACE_wrappers-repository/build/studio10-64_debug" />
        <environment name="LD_LIBRARY_PATH" value="/acebuilds/ACE_wrappers-repository/build/studio10-64_debug/lib:/usr/lib" />
        <environment name="CVSROOT"         value=":ext:shuston@cvs.doc.wustl.edu:/project/cvs-repository" />
        <environment name="CVS_RSH"         value="ssh" />
        <environment name="MPC_ROOT"        value="/acebuilds/MPC" />

        <variable name="root"         value="/acebuilds" />
        <variable name="build_name"   value="studio10-64_debug" />
        <variable name="project_root" value="/acebuilds/ACE_wrappers-repository/build/studio10-64_debug" />
        <variable name="log_file"     value="ACE_wrappers-repository/studio10-64_debug.txt" />
        <variable name="log_root"     value="/jane/acebuilds/logs/billy/dev/studio10-64_debug" />
        <variable name="configs"      value="Exceptions CHECK_RESOURCES" />

    </configuration>

    <command name="status"            options="on" />
    <command name="file_manipulation" options="type=mustnotexist file=ACE_wrappers-repository/.disable" />
    <command name="file_manipulation" options="type=create file=ACE_wrappers-repository/.disable output='RUNNING'" />
    <command name="log"               options="on" />

    <command name="cvs"               options="checkout -P MPC" />
    <command name="cvs"               options="checkout -P ACE" />

    <command name="file_manipulation" options="type=delete file=ACE_wrappers-repository/.disable" />

    <command name="file_manipulation" options="type=mustnotexist file=ACE_wrappers-repository/build/studio10-64_debug/.disable" />

    <command name="create_ace_build"  options="-a -v" />
    <command name="file_manipulation" options="type=create file=ACE_wrappers-repository/build/studio10-64_debug/.disable output='RUNNING'" />

    <command name="file_manipulation" options="type=create file=ACE_wrappers-repository/local.features output='qos=1\n'" />
    <command name="generate_workspace" options="-feature_file /acebuilds/ACE_wrappers-repository/local.features -relative ACE_ROOT=/acebuilds/ACE_wrappers-repository/build/studio10-64_debug -type gnuace -exclude Kokyu" />

    <command name="file_manipulation" options="type=create file=ACE_wrappers-repository/build/studio10-64_debug/ace/config.h output='#include \x22ace/config-sunos5.10.h\x22\n'" />
    <command name="file_manipulation" options="type=create file=ACE_wrappers-repository/build/studio10-64_debug/include/makeinclude/platform_macros.GNU output='exceptions=1\ndebug=1\nbuildbits=64\ninclude $(ACE_ROOT)/include/makeinclude/platform_sunos5_sunc++.GNU\n'" />

    <command name="print_os_version" />
    <command name="check_compiler"    options="sun_cc" />
    <command name="print_ace_config" />

    <command name="make"              options="-k realclean" />
    <command name="make"              options="-j 2 -k" />

    <command name="auto_run_tests"    options="-a" />

    <command name="log"               options="off" />
    <command name="process_logs"      options="clean=5 move prettify index" />
    <command name="file_manipulation" options="type=delete file=ACE_wrappers-repository/build/studio10-64_debug/.disable" />
    <command name="status"            options="off" />
</autobuild>
