    <command name="status"            options="on" />
    <command name="process_listener"  options="localhost:32001" />

    <command name="log"               options="ON" />

    <command name="svn"               options="co svn://svn.dre.vanderbilt.edu/DOC/Middleware/sets-anon/ACE+TAO+CIAO ." />

    <command name="shell"             options="find ACE_wrappers -name 'GNUmakefile*' -exec rm '{}' ';'" />

    <command name="file_manipulation" options="type=create file=ACE_wrappers/ace/config.h output='#define TEST_DIR ACE_TEXT (\x22/network/temp/ACE_wrappers/tests/\x22)\n#define ACE_DEFAULT_TEST_DIR ACE_TEXT (\x22/network/temp/ACE_wrappers/tests/\x22)\n#include \x22ace/config-win32.h\x22\n'"/>
    <command name="file_manipulation" options="type=create file=ACE_wrappers/include/makeinclude/platform_macros.GNU output='wince=1\nuses_wchar=1\ninline=0\nTAO_IDL := $(HOST_ROOT)/bin/tao_idl.exe\nTAO_IDLFLAGS += -g $(HOST_ROOT)/bin/gperf.exe\nTAO_IDL_DEP := $(HOST_ROOT)/bin/tao_idl.exe\ninclude $(ACE_ROOT)/include/makeinclude/platform_cegcc.GNU\n'" />
    <command name="file_manipulation" options="type=create file=ACE_wrappers/bin/MakeProjectCreator/config/default.features output='wince=1\nuses_wchar=1\n'" />

    <command name="print_os_version" />
    <command name="print_env_vars" />
    <command name="check_compiler" options="gcc" />
    <command name="print_ace_config" />
    <command name="print_make_version" />
    <command name="print_perl_version" />

    <command name="generate_workspace" options="-exclude TAO/CIAO -type gnuace" />
    <command name="generate_workspace" options="-exclude TAO/CIAO -type vxtest -value_template libname_prefix=\x22lib\x22" />

    <command name="make"              options="-k realclean find=*akefile" />
    <command name="make"              options="-k" />

    <!--command name="auto_run_tests"    options="-a -o" /-->

    <command name="log"               options="OFF" />
    <command name="process_logs"      options="move prettify clean index" />
    <command name="status"            options="off" />
    <command name="shell"             options="/usr/bin/rsync --progress --recursive --delete -z --exclude /ACE_wrappers*/ --include *.txt --include *.html --exclude ** ./ johnnyw@naboo.dre.vanderbilt.edu:/web/users/remedynl/cygwincegcc" />
