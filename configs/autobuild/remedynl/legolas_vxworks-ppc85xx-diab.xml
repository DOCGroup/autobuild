    <command name="file_manipulation" options="type=create file=.disable output='RUNNING'" />
    <command name="log"               options="on" />
    <!-- Delete all the Makefiles so the cvs checkout won't complain. -->
    <command name="shell"             options="find ACE_wrappers -name GNUmakefile* -exec \x22rm {}\x22 \x22;\x22" />
    <command name="cvs"               options="-z 9 -q checkout -P ACE_wrappers" />
    <command name="cvs"               options="-z 9 -q checkout -P ACE_MPC"/>

    <command name="file_manipulation" options="type=create file=ACE_wrappers/ace/config.h output='#include \x22ace/config-vxworks5.x.h\x22\n'"/>
    <command name="file_manipulation" options="type=create file=ACE_wrappers/include/makeinclude/platform_macros.GNU output='debug=0\ninline=1\nprobe=1\nfl_reactor=0\nTOOL = diab\nCPU = PPC85XX\nTAO_IDL := $(HOST_ROOT)/bin/tao_idl.exe\nTAO_IDL_DEP := $(HOST_ROOT)/bin/tao_idl.exe\nCIDLC := $(HOST_ROOT)/TAO/CIAO/bin/cidlc.exe\nCIDLC_DEP := $(HOST_ROOT)/TAO/CIAO/bin/cidlc.exe\nTAO_IDLFLAGS += -g $(HOST_ROOT)/bin/gperf\ninclude $(ACE_ROOT)/include/makeinclude/platform_vxworks5.5.x.GNU\n'" />
    <command name="file_manipulation" options="type=create file=ACE_wrappers/bin/MakeProjectCreator/config/default.features output='fl_reactor=0\ntk_reactor=1\n'" />

    <command name="print_os_version" />
    <command name="check_compiler" options="dplus -tPPCE500FS:vxworks55" />
    <command name="print_ace_config" />
    <command name="print_make_version" />
    <command name="print_perl_version" />

    <command name="generate_workspace" options="-exclude ASNMP,TAO,apps/mkcsregdb,apps/soreduce,netsvcs,examples/Shared_Malloc" />
    <command name="generate_workspace" options="dirs=TAO -exclude TAOACE.mwc,CIAO" />

    <command name="make"              options="-k realclean" />
    <command name="make"              options="-k -C TAO realclean" />

    <command name="make"              options="-k" />
    <command name="make"              options="-k -C TAO" />

    <!-- command name="auto_run_tests" options="" / -->

    <command name="log"               options="off" />
    <command name="process_logs"      options="clean=8 move prettify index" />
    <command name="file_manipulation" options="type=delete file=.disable" />
    <command name="status"            options="off" />
    <command name="shell"             options="c:\cygwin\bin\rsync --progress --recursive --delete -z --exclude /ACE_wrappers/ --include *.txt --include *.html --exclude ** ./ johnnyw@naboo.dre.vanderbilt.edu:/web/users/remedynl/VxWorks_PPC85XX_DIAB" />
