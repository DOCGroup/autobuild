<autobuild>
    <configuration>

        <environment name="ACE_ROOT"                value="Z:/VxWorks/vx551_win_x86/ACE_wrappers" />
        <environment name="TAO_ROOT"                value="Z:/VxWorks/vx551_win_x86/ACE_wrappers/TAO"/>
        <environment name="CIAO_ROOT"               value="Z:/VxWorks/vx551_win_x86/ACE_wrappers/TAO/CIAO"/>
        <environment name="HOST_ROOT"               value="C:/ACE/VxWorks/Host/ACE_wrappers"/>
        <environment name="PATH"                    value="C:\\Tornado2.2\\host\\x86-win32\\bin;C:\\tools\\nixtools;C:\\ACE\\VxWorks\\Host\\ACE_wrappers\\lib;C:\\ACE\\VxWorks\\Host\\ACE_wrappers\\bin;c:\\perl\\bin;C:\\Program Files\\Microsoft Visual Studio 8\\Vc\\bin;C:\\Program Files\\Microsoft Visual Studio 8\\Common7\\IDE;" type="prefix" />
        <environment name="WIND_BASE"               value="C:/Tornado2.2"/>
        <environment name="WIND_HOST_TYPE"          value="x86-win32"/>
        <environment name="RSYNC_RSH"               value="C:\\cygwin\\bin\\ssh" />
        <environment name="ACE_RUN_VX_TGTSVR"       value="P4" />
        <environment name="ACE_RUN_VX_TGTSVR_DEFGW" value="10.2.128.1" />
        <environment name="ACE_RUN_VX_REBOOT_TIME"  value="40" />
        <environment name="ACE_RUN_VX_REBOOT_TOOL"  value="perl C:/ACE/VxWorks/P4_reboot.pl" />
        <environment name="ACE_RUN_VX_TGT_REBOOT"   value="1" />
        <environment name="ACE_RUN_VX_TGT_HOST"     value="10.2.128.65" />

        <variable name="root"       value="Z:/VxWorks/vx551_win_x86/" />
        <variable name="log_file" 	value="build.txt" />
        <variable name="log_root"   value="Z:/VxWorks/vx551_win_x86/" />
        <variable name="configs"  	value="VxWorks STATIC" />
        <variable name="svn_program"       value="c:\\cygwin\\bin\\svn" />

    </configuration>

    <command name="status"            options="on" />
    <command name="process_listener"  options="localhost:32001" />
    <command name="log"               options="on" />

    <command name="shell"             options="find ACE_wrappers -name GNUmakefile* -exec \x22rm {}\x22 \x22;\x22" />
    <command name="svn"               options="co svn://svn.dre.vanderbilt.edu/DOC/Middleware/sets-anon/ACE+TAO+CIAO ." />

    <command name="file_manipulation" options="type=create file=ACE_wrappers/ace/config.h output='#define ACE_HAS_IP_MULTICAST\n#include \x22ace/config-vxworks5.x.h\x22\n'"/>
    <command name="file_manipulation" options="type=create file=ACE_wrappers/include/makeinclude/platform_macros.GNU output='ACE_MKDIR=c:/tools/nixtools/mkdir -p\nstatic_libs=1\ndebug=1\ninline=0\nfl_reactor=0\noptimize=0\noverride RM:=C:/tools/nixtools/rm -f\nCPU = PENTIUM4\ninclude $(ACE_ROOT)/include/makeinclude/platform_vxworks5.5.x.GNU\n'" />
    <command name="file_manipulation" options="type=create file=ACE_wrappers/bin/MakeProjectCreator/config/default.features output='cidl=1\nxerces=0\n'" />

    <command name="anonymous_shell"   options="c:\\cygwin\\bin\\scp -C johnnyw@naboo.dre.vanderbilt.edu:/web/users/isisbuilds/CIDLC/Win32_vc8/cidlc.exe ACE_wrappers/TAO/CIAO/bin" />
    <command name="anonymous_shell"   options="c:\\cygwin\\bin\\chmod 777 ACE_wrappers/TAO/CIAO/bin/cidlc.exe" />

    <command name="print_os_version" />
    <command name="print_env_vars" />
    <command name="check_compiler" options="ccpentium" />
    <command name="print_ace_config" />
    <command name="print_cidlc_version" />
    <command name="print_make_version" />
    <command name="print_perl_version" />

    <command name="generate_workspace" options="-exclude TAO/CIAO -type gnuace" />

    <command name="make"              options="-k realclean" />

    <command name="make"              options="-k " />

    <command name="log"               options="off" />
    <command name="process_logs"      options="clean move prettify index" />
    <command name="status"            options="off" />

    <command name="shell"             options="c:\\cygwin\\bin\\rsync --progress --recursive --delete -z --exclude /ACE_wrappers*/ --include *.txt --include *.html --exclude ** ./ johnnyw@naboo.dre.vanderbilt.edu:/web/users/remedynl/vx551_win_x86" />
</autobuild>
