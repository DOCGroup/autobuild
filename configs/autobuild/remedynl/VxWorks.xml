<!-- $Id$ -->
<autobuild>
    <configuration>
        <environment name="ACE_ROOT"     value="C:/ACE/VxWorks/ACE_wrappers" />
        <environment name="TAO_ROOT"     value="C:/ACE/VxWorks/ACE_wrappers/TAO"/>
        <environment name="CVSROOT"      value=":ext:wustl:/project/cvs-repository" />
        <environment name="CVS_RSH"      value="plink" />
        <environment name="Path"         value="C:\ACE\VxWorks\ACE_wrappers\bin;C:\ACE\VxWorks\ACE_wrappers\ace;c:\program files\putty;C:\T22-Proto\host\x86-win32\bin;c:\cygwin\bin" type="prefix" />
        <environment name="include"      value="" />
        <environment name="lib"          value="" />

	<environment name="BUILD"    value="all" />

        <variable name="root"     value="C:\ACE\VxWorks\" />
	<variable name="log_file" value="log.txt" />
        <variable name="log_root" value="C:\ACE\VxWorks\" />
        <variable name="name"     value="VxWorks" />
    </configuration>

    <command name="status"            options="on" />
    <command name="file_manipulation" options="type=mustnotexist file=.disable" />
    <command name="file_manipulation" options="type=create file=.disable output='RUNNING'" />

    <command name="log"               options="ON" />

    <command name="cvs"               options="-z9 -q checkout -P ACE_wrappers" />

    <command name="file_manipulation" options="type=create file=ACE_wrappers/ace/config.h output='#include \x22ace/config-vxworks5.x.h\x22\n'" />
    <command name="file_manipulation" options="type=create file=ACE_wrappers/include/makeinclude/platform_macros.GNU output='CPU=SIMNT\nWIND_BASE=c:/T22-Proto\nWIND_HOST_TYPE=x86-win32\ninclude $(ACE_ROOT)/include/makeinclude/platform_vxworks5.x_g++.GNU\n'" />

    <command name="print_ace_config" />
    <command name="print_make_version" />
    <command name="print_perl_version" />

    <command name="make"              options="-k" />

    <command name="log"               options="OFF" />
    <command name="process_logs"      options="move prettify clean=1 index" />
    <command name="file_manipulation" options="type=delete file=.disable" />
    <command name="status"            options="off" />
</autobuild>




