<!-- $Id$ -->
<autobuild>
    <configuration>
        <environment name="ACE_ROOT"        value="/home/build/ACE/gcc/ACE_wrappers"/>
        <environment name="TAO_ROOT"        value="/home/build/ACE/gcc/ACE_wrappers/TAO"/>
        <environment name="CIAO_ROOT"        value="/home/build/ACE/gcc/ACE_wrappers/TAO/CIAO"/>
        <environment name="DANCE_ROOT"        value="/home/build/ACE/gcc/ACE_wrappers/TAO/DAnCE"/>
        <environment name="LD_LIBRARY_PATH" value="/home/build/ACE/gcc/ACE_wrappers/lib:/usr/local/lib:" />

        <variable name="root"     value="/home/build/ACE/gcc/" />
        <variable name="log_file" value="build.txt" />
        <variable name="log_root" value="/home/build/ACE/gcc/" />
        <variable name="configs"  value="Exceptions" />

    </configuration>

    <command name="status"            options="on" />
    <command name="process_listener"  options="localhost:32003" />
    <command name="log"               options="on" />

    <command name="svn"               options="co svn://svn.dre.vanderbilt.edu/DOC/Middleware/sets-anon/ACE+TAO ." />

    <command name="file_manipulation" options="type=create file=ACE_wrappers/ace/config.h output='#include \x22ace/config-qnx.h\x22\n'"/>
    <command name="file_manipulation" options="type=create file=ACE_wrappers/include/makeinclude/platform_macros.GNU output='inline=0\ninclude $(ACE_ROOT)/include/makeinclude/platform_qnx.GNU\n'" />
    <command name="file_manipulation" options="type=create file=ACE_wrappers/bin/MakeProjectCreator/config/default.features output=''" />

    <command name="print_os_version" />
    <command name="print_env_vars" />
    <command name="check_compiler" options="gcc" />
    <command name="print_ace_config" />
    <command name="print_openssl_version"/>
    <command name="print_make_version" />
    <command name="print_perl_version" />

    <command name="make"              options="realclean find=*akefile" />

    <command name="generate_workspace" options="-type gnuace" />

    <command name="make"              options="-k" />

    <!-- command name="auto_run_tests"    options=" -a -o" /-->

    <command name="log"               options="off" />
    <command name="process_logs"      options="clean=5 move prettify index" />
    <command name="status"            options="off" />
    <command name="shell"             options="rsync --progress --recursive --delete -z --exclude /ACE_wrappers*/ --include *.txt --include *.html --exclude ** ./ qnap.remedy.nl::Buildlogs/qnx65" />
</autobuild>
