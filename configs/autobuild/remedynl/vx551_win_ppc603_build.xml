<autobuild>
    <configuration>

        <environment name="ACE_ROOT"                value="F:/VxWorks/vx551_win_ppc/ACE_wrappers" />
        <environment name="TAO_ROOT"                value="F:/VxWorks/vx551_win_ppc/ACE_wrappers/TAO"/>
        <environment name="HOST_ROOT"               value="C:/ACE/VxWorks/Host/ACE_wrappers"/>
        <environment name="PATH"                    value="C:\\Tornado2.2\\host\\x86-win32\\bin;C:\\tools\\nixtools;C:\\ACE\\VxWorks\\Host\\ACE_wrappers\\lib;C:\\ACE\\VxWorks\\Host\\ACE_wrappers\\bin;c:\\perl\\bin;C:\\Program Files\\Microsoft Visual Studio 8\\Vc\\bin;C:\\Program Files\\Microsoft Visual Studio 8\\Common7\\IDE;" type="prefix" />
        <environment name="WIND_BASE"               value="C:/Tornado2.2"/>
        <environment name="WIND_HOST_TYPE"          value="x86-win32"/>
        <environment name="RSYNC_RSH"               value="C:\\cygwin\\bin\\ssh" />
        <environment name="ACE_RUN_VX_TGTHOST" value="10.2.0.74"/>
        <environment name="ACE_RUN_VX_TGTSVR_ROOT" value="/nfsace/vxworks/ppc603_shared/ACE_wrappers"/>
        <environment name="ACE_TEST_VERBOSE" value="1"/>
        <environment name="ACE_RUN_VX_IBOOT" value="10.2.0.129:80"/>
        <environment name="ACE_RUN_VX_REBOOT_TIME" value="45"/>
        <environment name="ACE_RUN_VX_TGT_REBOOT"      value="1"/>

        <variable name="root"       value="F:/VxWorks/vx551_win_ppc/" />
        <variable name="log_file" 	value="build.txt" />
        <variable name="log_root"   value="F:/VxWorks/vx551_win_ppc/" />
        <variable name="configs"    value="VxWorks STATIC MINIMUM" />
        <variable name="svn_program"       value="c:\\cygwin\\bin\\svn" />

    </configuration>

    <command name="status"            options="on" />
    <command name="process_listener"  options="localhost:32001" />
    <command name="log"               options="on" />

    <command name="make"              options="-k realclean" />

    <command name="shell"             options="find ACE_wrappers -name GNUmakefile* -exec \x22rm {}\x22 \x22;\x22" />
    <command name="svn"               options="co svn://svn.dre.vanderbilt.edu/DOC/Middleware/sets-anon/ACE+TAO ." />

    <command name="file_manipulation" options="type=create file=ACE_wrappers/TAO_Host.mwc output='workspace {\nace\ntests\nTAO/tao\nTAO/tests\n}\n'" />
    <command name="file_manipulation" options="type=create file=ACE_wrappers/ace/config.h output='#define ACE_HAS_IP_MULTICAST\n#include \x22ace/config-vxworks5.x.h\x22\n'"/>
    <command name="file_manipulation" options="type=create file=ACE_wrappers/include/makeinclude/platform_macros.GNU output='minimum_corba=1\nACE_MKDIR=c:/tools/nixtools/mkdir -p\nstatic_libs=1\ndebug=1\ninline=0\nfl_reactor=0\noptimize=0\noverride RM:=C:/tools/nixtools/rm -f\nCPU = PPC603\ninclude $(ACE_ROOT)/include/makeinclude/platform_vxworks5.5.x.GNU\n'" />
    <command name="file_manipulation" options="type=create file=ACE_wrappers/bin/MakeProjectCreator/config/default.features output='minimum_corba=1\n'" />

    <command name="print_os_version" />
    <command name="print_env_vars" />
    <command name="check_compiler" options="ccppc" />
    <command name="print_ace_config" />
    <command name="print_make_version" />
    <command name="print_perl_version" />

    <command name="generate_workspace" options="TAO_Host.mwc -type gnuace" />

    <command name="make"              options="-k " />

    <command name="log"               options="off" />
    <command name="process_logs"      options="clean move prettify index" />
    <command name="status"            options="off" />

    <command name="shell"             options="c:\\cygwin\\bin\\rsync --progress --recursive --delete -z --exclude /ACE_wrappers*/ --include *.txt --include *.html --exclude ** ./ johnnyw@naboo.dre.vanderbilt.edu:/web/users/remedynl/vx551_win_ppc603" />
</autobuild>
