<!-- $Id$ -->
<autobuild>
    <configuration>
        <environment name="ACE_ROOT"     value="C:\ACE\VC71\ACE_wrappers" />
        <environment name="TAO_ROOT"     value="C:\ACE\VC71\ACE_wrappers\TAO"/>
        <environment name="CVSROOT"      value=":ext:jwillemsen@cvs.doc.wustl.edu:/project/cvs-repository" />
        <environment name="CVS_RSH"      value="ssh" />
        <environment name="PATH"         value="C:\Program Files\Microsoft Visual Studio .NET 2003\Common7\IDE;C:\Program Files\Microsoft Visual Studio .NET 2003\Vc7\bin;C:\ACE\VC71\ACE_wrappers\bin;C:\opensslvc71\openssl-0.9.7d\out32dll" type="prefix" />
        <environment name="SSL_ROOT"     value="C:\opensslvc71\openssl-0.9.7d" />

        <variable name="configs"  value="Win32 VC71 Exceptions SSL" />
        <variable name="root"     value="C:\ACE\VC71\" />
	<variable name="log_file" value="log.txt" />
        <variable name="log_root" value="C:\ACE\VC71\" />
        <variable name="name"     value="WinXP VS.NET 2003 SSL" />
    </configuration>

    <command name="status"            options="on" />
    <command name="file_manipulation" options="type=mustnotexist file=.disable" />
    <command name="file_manipulation" options="type=create file=.disable output='RUNNING'" />

    <command name="log"               options="ON" />

    <command name="cvs"               options="-z9 -q checkout -P ACE_wrappers" />
    <command name="cvs"               options="-z9 -q checkout -P ACE_MPC"/>
    <command name="file_manipulation" options="type=create file=ACE_wrappers/ace/config.h output='#define ACE_DISABLE_WIN32_ERROR_WINDOWS\n#define ACE_HAS_STANDARD_CPP_LIBRARY 1\n#include \x22ace/config-win32.h\x22\n'" />
    <command name="file_manipulation" options="type=create file=ACE_wrappers/bin/MakeProjectCreator/config/default.features output='rwho=0\nqos=1\nssl=1\n'" />

    <command name="print_os_version" />
    <command name="check_compiler" options="msvc" />
    <command name="print_ace_config" />
    <command name="print_perl_version" />

    <command name="shell"             options="find ACE_wrappers -name '*.vcproj*' -exec rm '{}' ';'" />
    <command name="shell"             options="find ACE_wrappers -name '*.sln*' -exec rm '{}' ';'" />

    <command name="generate_workspace" options="-type vc71 -recurse"/>
    <command name="vc7make"            options="/rebuild debug TAO/TAOACE.sln" />
    <command name="vc7make"            options="/rebuild debug tests/tests.sln" />
    <command name="vc7make"            options="/rebuild debug examples/examples.sln" />
    <command name="vc7make"            options="/rebuild debug performance-tests/perf.sln" />
    <command name="vc7make"            options="/rebuild debug TAO/tests/tests.sln" />
    <command name="vc7make"            options="/rebuild debug TAO/examples/examples.sln" />
    <command name="vc7make"            options="/rebuild debug TAO/performance-tests/perf.sln" />
    <command name="vc7make"            options="/rebuild debug TAO/orbsvcs/tests/tests.sln" />
    <command name="vc7make"            options="/rebuild debug TAO/orbsvcs/examples/examples.sln" />
    <command name="vc7make"            options="/rebuild debug TAO/orbsvcs/performance-tests/perf.sln" />

    <command name="auto_run_tests"    options="-s \x22c:\ace\autobuild\win32sandbox\sandbox.exe -t 1200000\x22" /> 

    <command name="log"               options="OFF" />
    <command name="process_logs"      options="move prettify clean index" />
    <command name="file_manipulation" options="type=delete file=.disable" />
    <command name="status"            options="off" />
    <command name="shell"             options="ssh willemsenj@tao.dre.vanderbilt.edu rm /web/users/willemsenj/VC71/* -f" />
    <command name="shell"             options="scp -C *.txt *.html willemsenj@tao.dre.vanderbilt.edu:/web/users/willemsenj/VC71" />
    <command name="shell"             options="ssh willemsenj@tao.dre.vanderbilt.edu chmod 755 /web/users/willemsenj/VC71/*" />
</autobuild>
