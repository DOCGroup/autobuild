<!-- $Id$ -->
<autobuild>
    <configuration>
        <environment name="ACE_ROOT"    value="C:\\ACE\\trunk\\DOC_ROOT\\ACE" />
        <environment name="TAO_ROOT"    value="C:\\ACE\\trunk\\DOC_ROOT\\TAO"/>
        <environment name="CIAO_ROOT"   value="C:\\ACE\\trunk\\DOC_ROOT\\CIAO"/>
        <environment name="DANCE_ROOT"  value="C:\\ACE\\trunk\\DOC_ROOT\\DAnCE"/>
        <environment name="MPC_ROOT"    value="C:\\ACE\\trunk\\DOC_ROOT\\MPC"/>
        <environment name="RSYNC_RSH"   value="C:\\cygwin\\bin\\ssh" />
        <environment name="PATH"        value="C:\\ACE\\trunk\\DOC_ROOT\\ACE\\bin;C:\\ACE\\trunk\\DOC_ROOT\\ACE\\lib;" type="prefix" />
        <environment name="INCLUDE"     value="C:\\Program Files (x86)\\Microsoft Visual Studio 11.0\\VC\\ATLMFC\\INCLUDE;C:\\Program Files (x86)\\Microsoft Visual Studio 11.0\\VC\\INCLUDE;C:\\Program Files (x86)\\\\Microsoft SDKs\\Windows\\v6.0A\\include;" type="prefix" />
        <environment name="LIB"         value="C:\\Program Files (x86)\\Microsoft Visual Studio 11.0\\VC\\ATLMFC\\LIB;C:\\Program Files (x86)\\Microsoft Visual Studio 11.0\\VC\\LIB;C:\\Program Files (x86)\\\\Microsoft SDKs\\Windows\\v6.0A\\lib;" type="prefix" />
        <environment name="LIBPATH"     value="C:\\WINDOWS\\Microsoft.NET\\Framework\\v3.5;C:\\WINDOWS\\Microsoft.NET\\Framework\\v2.0.50727;C:\\Program Files (x86)\\Microsoft Visual Studio 11.0\\VC\\ATLMFC\\LIB;C:\\Program Files (x86)\\Microsoft Visual Studio 11.0\\VC\\LIB;" type="prefix" />
        <environment name="PATH"        value="C:\\Program Files (x86)\\Microsoft Visual Studio 11.0\\Common7\\IDE;C:\\Program Files (x86)\\Microsoft Visual Studio 11.0\\VC\\bin;C:\\Program Files (x86)\\Microsoft SDKs\\Windows\\v6.0A\\bin;" type="prefix" />

        <!-- http://devportal.remedy.nl/projects/ngcmaintenance/wiki/DDS4CCM_DEFAULT_DOMAIN_ID -->

        <variable name="configs"  value="Win32 VC90 Exceptions NDDS XERCES" />
        <variable name="root"     value="C:\\ACE\\trunk\\" />
        <variable name="log_file" value="build.txt" />
        <variable name="log_root" value="C:\\ACE\\trunk\\" />
        <variable name="name"     value="WinXP VS.NET 2008" />
        <variable name="svn_program"  value="c:\\cygwin\\bin\\svn" />
        <variable name="project_root" value="C:\\ACE\\trunk\\DOC_ROOT" />
    </configuration>

    <command name="status"            options="on" />
    <command name="process_listener"  options="localhost:32001" />

    <command name="win32make"         options="nmake /I CFG=\x22Win32 Debug\x22 REALCLEAN" />

    <command name="log"               options="ON" />

    <command name="svn"               options="co svn://svn.dre.vanderbilt.edu/DOC/Middleware/trunk DOC_ROOT" />
    <command name="svn"               options="co svn://svn.dre.vanderbilt.edu/DOC/MPC/trunk DOC_ROOT/MPC" />

    <command name="shell"             options="c:\\cygwin\\bin\\find DOC_ROOT -name '*.mak' -exec c:\\cygwin\\bin\\rm '{}' ';'" />
    <command name="shell"             options="c:\\cygwin\\bin\\find DOC_ROOT -name '*.manifest' -exec c:\\cygwin\\bin\\rm '{}' ';'" />

    <command name="file_manipulation" options="type=create file=DOC_ROOT/ACE/ace/config.h output='#define ACE_DISABLE_WIN32_INCREASE_PRIORITY\n#define ACE_DISABLE_WIN32_ERROR_WINDOWS\n#define ACE_HAS_STANDARD_CPP_LIBRARY 1\n#define TAO_HAS_ZIOP 1\n#include \x22ace/config-win32.h\x22\n'" />
    <command name="file_manipulation" options="type=create file=DOC_ROOT/ACE/bin/MakeProjectCreator/config/default.features output=''" />

    <command name="print_os_version" />
    <command name="print_env_vars" />
    <command name="check_compiler" options="msvc" />
    <command name="print_ace_config" />
    <command name="print_perl_version" />

    <command name="generate_workspace" options="-type nmake -base vc10nmake -exclude ADBC -exclude TAO,CIAO,DAnCE"/>

    <command name="win32make"         options="nmake /I CFG=\x22Win32 Debug\x22 NO_EXTERNAL_DEPS=1" />

    <command name="log"               options="OFF" />
    <command name="process_logs"      options="move prettify clean=50 index" />
    <command name="status"            options="off" />
    <command name="shell"             options="c:\\cygwin\\bin\\rsync --progress --recursive --delete -z --exclude /DOC_ROOT*/ --include *.txt --include *.html --exclude ** ./ /cygdrive/z/win8_64b_msvc11" />
</autobuild>
