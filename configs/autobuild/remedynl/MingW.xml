<!-- $Id$ -->
<autobuild>
    <configuration>
        <environment name="ACE_ROOT"     value="D:/ACE/MingW/ACE_wrappers" />
        <environment name="TAO_ROOT"     value="D:/ACE/MingW/ACE_wrappers/TAO"/>
        <environment name="CVSROOT"      value=":ext:jwillemsen@cvs.doc.wustl.edu:/project/cvs-repository" />
        <environment name="CVS_RSH"      value="plink" />
        <environment name="Path"         value="C:\MSYS\1.0\BIN;C:\MingW\bin;D:\ACE\MingW\ACE_wrappers\bin;D:\ACE\MingW\ACE_wrappers\ace;D:\;d:\putty;d:\perl\bin" type="prefix" />
        <environment name="include"      value="" />
        <environment name="lib"          value="" />

	<environment name="BUILD"    value="all" />

        <variable name="root"     value="D:\ACE\MingW\" />
	<variable name="log_file" value="log.txt" />
        <variable name="log_root" value="D:\ACE\MingW\" />
        <variable name="name"     value="MingW" />
    </configuration>

    <command name="status"            options="on" />
    <command name="file_manipulation" options="type=mustnotexist file=.disable" />
    <command name="file_manipulation" options="type=create file=.disable output='RUNNING'" />

    <command name="log"               options="ON" />

    <command name="cvs"               options="-z9 -q checkout -P ACE_wrappers" />

    <command name="file_manipulation" options="type=create file=ACE_wrappers/ace/config.h output='#define ACE_DISABLE_WIN32_ERROR_WINDOWS\n#define TAO_HAS_SMART_PROXIES 1\n#define TAO_HAS_AMI 1\n#define ACE_HAS_STANDARD_CPP_LIBRARY 1\n#define TAO_HAS_INTERFACE_REPOSITORY 1\n#define TAO_HAS_RT_CORBA 1\n#include \x22ace/config-win32.h\x22\n'" />
    <command name="file_manipulation" options="type=create file=ACE_wrappers/include/makeinclude/platform_macros.GNU output='debug=0\ninclude $(ACE_ROOT)/include/makeinclude/platform_mingw32.GNU\n'" />

    <command name="print_os_version" options="useuname" />
    <command name="check_compiler" options="gcc" />
    <command name="print_ace_config" />
    <command name="print_make_version" />
    <command name="print_perl_version" />

    <command name="make"              options="-k reverseclean" />
    <command name="make"              options="-k -C TAO reverseclean" />

    <command name="make"              options="-k" />
    <command name="make"              options="-k -C TAO" />

    <command name="auto_run_tests" options="-a -s \x22D:\ACE\autobuild\win32sandbox\sandbox.exe -t 1200000\x22" /> 

    <command name="log"               options="OFF" />
    <command name="process_logs"      options="move prettify clean=1 index" />
    <command name="file_manipulation" options="type=delete file=.disable" />
    <command name="status"            options="off" />
    <command name="shell"             options="plink bugzilla@charanga.cs.wustl.edu rm .www-docs/auto_compile_logs/remedy.nl_Win2K_MingW/* -f" />
    <command name="shell"             options="pscp -C *.txt *.html bugzilla@charanga.cs.wustl.edu:.www-docs/auto_compile_logs/remedy.nl_Win2K_MingW" />
</autobuild>




