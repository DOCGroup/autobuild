<!-- $Id$ -->
<autobuild>
    <configuration>
        <environment name="ACE_ROOT"    value="C:\\ACE\\branch\\DOC_ROOT\\ACE" />
        <environment name="TAO_ROOT"    value="C:\\ACE\\branch\\DOC_ROOT\\TAO"/>
        <environment name="CIAO_ROOT"   value="C:\\ACE\\branch\\DOC_ROOT\\CIAO"/>
        <environment name="DANCE_ROOT"  value="C:\\ACE\\branch\\DOC_ROOT\\DAnCE"/>
        <environment name="MPC_ROOT"    value="C:\\ACE\\branch\\DOC_ROOT\\MPC"/>
        <environment name="RSYNC_RSH"   value="C:\\cygwin\\bin\\ssh" />
        <environment name="ZLIB_ROOT"   value="C:\\Program Files\\GnuWin32"/>
        <environment name="PATH"        value="C:\\Program Files\\GnuWin32\\bin;" type="prefix" />
        <environment name="BZIP2_ROOT"  value="C:\\Program Files\\GnuWin32"/>
        <environment name="XERCESCROOT" value="C:\\xerces-c-3.0.0-x86-windows-vc-9.0" />
        <environment name="PATH"        value="C:\\xerces-c-3.0.0-x86-windows-vc-9.0\\lib;C:\\xerces-c-3.0.0-x86-windows-vc-9.0\\bin;" type="prefix" />
        <environment name="PATH"        value="C:\\ACE\\branch\\DOC_ROOT\\ACE\\bin;C:\\ACE\\branch\\DOC_ROOT\\ACE\\lib;" type="prefix" />
        <environment name="PATH"        value="C:\\RTI\\ndds.4.5d.rev11\\lib\\i86Win32VS2008;" type="prefix" />
        <environment name="NDDSHOME"    value="C:\\RTI\\ndds.4.5d.rev11" />
        <environment name="NDDSARCHITECTURE"    value="i86Win32VS2008" />

        <!-- http://devportal.remedy.nl/projects/ngcmaintenance/wiki/DDS4CCM_DEFAULT_DOMAIN_ID -->
        <environment name="DDS4CCM_DEFAULT_DOMAIN_ID" value="41"/>

        <variable name="configs"  value="Win32 VC90 Exceptions NDDS XERCES" />
        <variable name="root"     value="C:\\ACE\\branch\\" />
        <variable name="log_file" value="build.txt" />
        <variable name="log_root" value="C:\\ACE\\branch\\" />
        <variable name="name"     value="WinXP VS.NET 2008" />
        <variable name="svn_program"  value="c:\\cygwin\\bin\\svn" />
        <variable name="project_root" value="C:\\ACE\\branch\\DOC_ROOT" />
    </configuration>

    <command name="status"            options="on" />
    <command name="process_listener"  options="localhost:32001" />

    <command name="eval"             options="\x22C:\\Program Files\\Microsoft Visual Studio 9\\VC\\vcvarsall.bat\x22 x86 & set" />

    <command name="win32make"         options="nmake /I CFG=\x22Win32 Debug\x22 REALCLEAN" />

    <command name="log"               options="ON" />

    <command name="svn"               options="co https://svn.dre.vanderbilt.edu/DOC/Middleware/branches/RemedyWork DOC_ROOT" />
    <command name="svn"               options="co https://svn.dre.vanderbilt.edu/DOC/MPC/trunk DOC_ROOT/MPC" />

    <command name="shell"             options="c:\\cygwin\\bin\\find DOC_ROOT -name '*.mak' -exec c:\\cygwin\\bin\\rm '{}' ';'" />
    <command name="shell"             options="c:\\cygwin\\bin\\find DOC_ROOT -name '*.manifest' -exec c:\\cygwin\\bin\\rm '{}' ';'" />

    <command name="file_manipulation" options="type=create file=DOC_ROOT/ACE/ace/config.h output='#define ACE_DISABLE_WIN32_INCREASE_PRIORITY\n#define ACE_DISABLE_WIN32_ERROR_WINDOWS\n#define ACE_HAS_STANDARD_CPP_LIBRARY 1\n#define TAO_HAS_ZIOP 1\n#include \x22ace/config-win32.h\x22\n'" />
    <command name="file_manipulation" options="type=create file=DOC_ROOT/ACE/bin/MakeProjectCreator/config/default.features output='xerces3=1\ndds4ccm_ndds=1\nndds=1\n'" />

    <command name="print_os_version" />
    <command name="print_env_vars" />
    <command name="check_compiler" options="msvc" />
    <command name="print_ace_config" />
    <command name="print_perl_version" />

    <command name="generate_workspace" options="-type nmake -base vc9nmake -exclude ADBC -exclude TAO/TAO_*.mwc,CIAO/CIAO_*.mwc"/>

    <command name="win32make"         options="nmake /I CFG=\x22Win32 Debug\x22 NO_EXTERNAL_DEPS=1" />

    <command name="auto_run_tests"    options="-a -o -C -s \x22C:\\ACE\\autobuild\\win32sandbox\\sandbox.exe -t 5400000\x22" />

    <command name="log"               options="OFF" />
    <command name="process_logs"      options="move prettify clean=50 index" />
    <command name="status"            options="off" />
    <command name="shell"             options="c:\\cygwin\\bin\\rsync --progress --recursive --delete -z --exclude /DOC_ROOT*/ --include *.txt --include *.html --exclude ** ./ qnap.remedy.nl::Buildlogs/winbranchbuild" />
</autobuild>
