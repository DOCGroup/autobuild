    <command name="file_manipulation" options="type=create file=ppu/ACE_wrappers/include/makeinclude/platform_macros.GNU output='cpumodelflag=1\nbuildbits=64\noptimize=1\ndebug=0\ninline=1\ninclude $(ACE_ROOT)/include/makeinclude/platform_linux.GNU\n'" />
