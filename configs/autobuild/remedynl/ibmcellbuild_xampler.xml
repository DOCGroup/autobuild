        <environment name="AVENGER_DIR"     value="/build/ACE/xampler/Avenger" />
        <environment name="LD_LIBRARY_PATH" value="/usr/local/lib" />
        <environment name="CROSS_COMPILE"   value="ppu-"/>
        <environment name="TCPU"            value="cell"/>

        <variable name="root"     value="/build/ACE/xampler/" />
        <variable name="log_file" value="build.txt" />
        <variable name="log_root" value="/root/ACE/xamplerlog/" />
        <variable name="configs"  value="Linux NDDS SSL Exceptions ZLIB" />
        <variable name="remote_root"  value="/build/ACE/ACE_wrappers" />
        <variable name="remote_shell" value="ssh 10.207.173.56" />

    </configuration>

    <command name="status"            options="on" />
    <command name="process_listener"  options="localhost:32003" />

    <command name="anonymous_shell"   options="rm -rf /build/ACE/xampler" />
