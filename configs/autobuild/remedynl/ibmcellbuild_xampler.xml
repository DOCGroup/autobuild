    <command name="setenv"            options="LD_LIBRARY_PATH=/build/ACE/xampler/ppu/ACE_wrappers/lib" />
    <command name="setenv"            options="PATH=/opt/cell/toolchain/bin:/usr/bin:/bin"/>
