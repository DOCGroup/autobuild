    <command name="file_manipulation" options="type=create file=ppu/ACE_wrappers/build.mwc output='workspace {\nace\nTAO/tao\n}\n'" />
    <command name="file_manipulation" options="type=create file=gcc442/ACE_wrappers/ace/config.h output='#define TAO_USE_DOTTED_DECIMAL_ADDRESSES 1\n#include \x22ace/config-linux.h\x22\n'" />
    <command name="file_manipulation" options="type=create file=gcc442/ACE_wrappers/include/makeinclude/platform_macros.GNU output='CROSS_COMPILE=powerpc64-unknown-linux-gnu-\ncpumodelflag=1\nbuildbits=64\noptimize=1\ndebug=0\ninline=1\ninclude $(ACE_ROOT)/include/makeinclude/platform_linux.GNU\n'" />
    <command name="file_manipulation" options="type=create file=gcc442/ACE_wrappers/bin/MakeProjectCreator/config/default.features output='\n'" />
    <command name="file_manipulation" options="type=create file=gcc442/ACE_wrappers/build.mwc output='workspace {\nace\nTAO/tao\n}\n'" />

    <command name="file_manipulation" options="type=create file=/build/ACE/xampler/ppu/Xampler/C++/makefile.local output='BITS=64\nLOCATOR=FILE\nMODE=STATIC\nCROSS=ppu-\nID=$(HOST_ROOT)/bin/tao_idl\nID_OPTS:=-g $(HOST_ROOT)/bin/ace_gperf\n'" />
