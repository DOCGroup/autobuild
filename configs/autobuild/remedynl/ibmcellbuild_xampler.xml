<!-- $Id$ -->
<autobuild>
    <configuration>
        <environment name="PATH"              value="/usr/bin:/usr/local/ngclinux/powerpc64/bin" type="prefix"/>
        <environment name="HOST_ROOT"       value="/root/ACE/hoststatic/ACE_wrappers"/>
        <environment name="ACE_ROOT"        value="/build/ACE/xampler/gcc/ACE_wrappers"/>
        <environment name="BAGATEL_EXE"     value="/build/ACE/xampler/Bagatel/src/bagatel" />
        <environment name="REPORT_DIR"      value="/build/ACE/xampler/Avenger/_Templt/Template-Display" />
        <environment name="AVENGER_DIR"     value="/build/ACE/xampler/Avenger" />
        <environment name="LD_LIBRARY_PATH" value="/usr/local/lib" />
        <environment name="SSL_ROOT"          value="/usr/local/ngclinux/powerpc64/powerpc64-ngc-linux/usr"/>
        <environment name="ZLIB_ROOT"         value="/usr/local/ngclinux/powerpc64/powerpc64-ngc-linux/usr"/>
        <environment name="XERCESCROOT"       value="/usr/local/ngclinux/powerpc64/powerpc64-ngc-linux/usr"/>

        <variable name="root"     value="/build/ACE/xampler/" />
        <variable name="log_file" value="build.txt" />
        <variable name="log_root" value="/root/ACE/ibmcellbuild_xamplerlog/" />
        <variable name="configs"  value="Linux SSL Exceptions ZLIB" />
        <variable name="remote_root"  value="/build/ACE/ACE_wrappers" />
        <variable name="remote_shell" value="ssh snalinux6" />

    </configuration>

    <command name="status"            options="on" />
    <command name="process_listener"  options="localhost:32004" />

    <command name="anonymous_shell"   options="mkdir -p /build/ACE/xampler/gcc" />

    <command name="log"               options="ON" />

    <command name="anonymous_shell"   options="rm -rf /build/ACE/xampler/gcc/Xampler" />
    <command name="anonymous_shell"   options="rm -rf /build/ACE/xampler/gcc/Avenger" />
    <command name="anonymous_shell"   options="rm -rf /build/ACE/xampler/Bagatel" />
    <command name="anonymous_shell"   options="rm -rf /build/ACE/xampler/Avenger" />

    <command name="svn"               options="co svn://svn.dre.vanderbilt.edu/DOC/Middleware/sets-anon/ACE+TAO gcc" />

    <command name="anonymous_shell"   options="rsync --progress --recursive jwillemsen@naboo.dre.vanderbilt.edu:/home/jwillemsen/Xampler/* /build/ACE/xampler/gcc/Xampler/" />
    <command name="anonymous_shell"   options="rsync --progress --recursive jwillemsen@naboo.dre.vanderbilt.edu:/home/jwillemsen/Avenger/* /build/ACE/xampler/gcc/Avenger/" />
    <command name="anonymous_shell"   options="rsync --progress --recursive jwillemsen@naboo.dre.vanderbilt.edu:/home/jwillemsen/Bagatel/* /build/ACE/xampler/Bagatel/" />
    <command name="anonymous_shell"   options="rsync --progress --recursive jwillemsen@naboo.dre.vanderbilt.edu:/home/jwillemsen/Avenger/* /build/ACE/xampler/Avenger/" />

    <command name="file_manipulation" options="type=create file=gcc/ACE_wrappers/ace/config.h output='#define TAO_USE_DOTTED_DECIMAL_ADDRESSES 1\n#include \x22ace/config-linux.h\x22\n'" />
    <command name="file_manipulation" options="type=create file=gcc/ACE_wrappers/include/makeinclude/platform_macros.GNU output='TCPU=cell\nCROSS_COMPILE=powerpc64-ngc-linux-\ncpumodelflag=1\nbuildbits=64\noptimize=1\ndebug=0\ninline=1\ninclude $(ACE_ROOT)/include/makeinclude/platform_linux.GNU\n'" />
    <command name="file_manipulation" options="type=create file=gcc/ACE_wrappers/bin/MakeProjectCreator/config/default.features output='\n'" />
    <command name="file_manipulation" options="type=create file=gcc/ACE_wrappers/build.mwc output='workspace {\nace\nTAO/tao\n}\n'" />

    <command name="file_manipulation" options="type=create file=/build/ACE/xampler/gcc/Xampler/C++/makefile.local output='BITS=64\nLOCATOR=FILE\nMODE=STATIC\nCROSS=powerpc64-ngc-linux-\nID=$(HOST_ROOT)/bin/tao_idl\nID_OPTS:=-g $(HOST_ROOT)/bin/ace_gperf\n'" />

    <command name="print_os_version" />
    <command name="print_remote_os_version" />
    <command name="print_env_vars" />
    <command name="print_perl_version" />
    <command name="print_perl_version" options="remote"/>
    <command name="print_openssl_version" options="remote"/>
    <command name="print_make_version" />
    <command name="check_compiler" options="powerpc64-ngc-linux-gcc" />
    <command name="print_ace_config" />

    <command name="setenv"             options="ACE_ROOT=/build/ACE/xampler/gcc/ACE_wrappers" />
    <command name="setenv"             options="TAO_ROOT=/build/ACE/xampler/gcc/ACE_wrappers/TAO"/>
    <command name="make"               options="-C /build/ACE/xampler/gcc/ACE_wrappers realclean" />
    <command name="generate_workspace" options="/build/ACE/xampler/gcc/ACE_wrappers/build.mwc -type gnuautobuild"/>
    <command name="make"               options="-C /build/ACE/xampler/gcc/ACE_wrappers -k -j 20" />

    <command name="make"              options="-C /build/ACE/xampler/Bagatel/src" />

    <command name="setenv"            options="TAO_DIR=/build/ACE/xampler/gcc/ACE_wrappers" />
    <command name="setenv"            options="ACE_ROOT=/build/ACE/xampler/gcc/ACE_wrappers" />
    <command name="setenv"            options="TAO_ROOT=/build/ACE/xampler/gcc/ACE_wrappers/TAO" />
    <command name="setenv"            options="LD_LIBRARY_PATH=/build/ACE/xampler/gcc/ACE_wrappers/lib" />
    <command name="anonymous_shell"   options="cd /build/ACE/xampler/gcc/Xampler/C++/_Suites ; find . -type d -name TAO_1.7.X_Linux_GCC -exec make clean -C {} \;" />
    <command name="anonymous_shell"   options="cd /build/ACE/xampler/gcc/Xampler/C++/_Suites ; find . -type d -name TAO_1.7.X_Linux_GCC -exec make -C {} \;" />

    <command name="anonymous_shell"   options="rm -rf /build/ACE/xampler/gcc/Avenger/_Templt/Template-Execute/_Suites" />
    <command name="anonymous_shell"   options="ln -s /build/ACE/xampler/gcc/Xampler/C++/_Suites /build/ACE/xampler/gcc/Avenger/_Templt/Template-Execute/_Suites" />
    <command name="anonymous_shell"   options="mkdir -p /build/ACE/xampler/gcc/Avenger/_Templt/Template-Execute/_Report" />

    <command name="file_manipulation" options="type=create file=/build/ACE/xampler/gcc.sh output='export LD_LIBRARY_PATH=/build/ACE/xampler/gcc/ACE_wrappers/lib\n'" />
    <command name="setenv"            options="LD_LIBRARY_PATH=/build/ACE/xampler/gcc/ACE_wrappers/lib" />

    <command name="anonymous_shell"   options="ssh snalinux6 \x22$SHELL --login -c \\\x22cd /build/ACE/xampler/gcc/Avenger/_Templt/Template-Execute/ ; sh /build/ACE/xampler/gcc/Avenger/_Ampler/Execute.sh 1 localhost /build/ACE/xampler/gcc.sh\\\x22\x22" />

    <command name="anonymous_shell"   options="mkdir -p /build/ACE/xampler/Avenger/_Templt/Template-Display/Results-One/_Result" />

    <command name="anonymous_shell"   options="ln -s /build/ACE/xampler/gcc/Avenger/_Templt/Template-Execute/_Result/TAO_1.7.X_Linux_GCC /build/ACE/xampler/Avenger/_Templt/Template-Display/Results-One/_Result/gcc_Linux_GCC" />
    <command name="anonymous_shell"   options="ln -s /build/ACE/xampler/Avenger/_Docses /build/ACE/xampler/Avenger/_Templt/Template-Display/_Docses" />
    <command name="anonymous_shell"   options="ln -s /build/ACE/xampler/Avenger/_Images /build/ACE/xampler/Avenger/_Templt/Template-Display/_Images" />
    <command name="anonymous_shell"   options="ln -s /build/ACE/xampler/Avenger/_Styles /build/ACE/xampler/Avenger/_Templt/Template-Display/_Styles" />

    <command name="anonymous_shell"   options="mkdir -p /build/ACE/xampler/Avenger/_Templt/Template-Display/_Broker/gcc" />

    <command name="anonymous_shell"   options="mkdir -p /build/ACE/xampler/Avenger/_Templt/Template-Display/_Ported/Linux_GCC" />

    <command name="file_manipulation" options="type=create file=/build/ACE/xampler/Avenger/_Templt/Template-Display/_Ported/Linux_GCC/name.txt output='Linux\n'" />

    <command name="file_manipulation" options="type=create file=/build/ACE/xampler/Avenger/_Templt/Template-Display/_Broker/gcc/name.txt output='TAO gcc\n'" />
    <command name="file_manipulation" options="type=create file=/build/ACE/xampler/Avenger/_Templt/Template-Display/title.txt output='TAO\n'" />
    <command name="file_manipulation" options="type=create file=/build/ACE/xampler/Avenger/_Templt/Template-Display/flags.txt output='Override_Size_Secondary = 600x300\n'" />

    <command name="anonymous_shell"   options="cd /build/ACE/xampler/Avenger/_Templt/Template-Display ; php -q ${AVENGER_DIR}/_Ampler/Display.php" />

    <command name="anonymous_shell"   options="sh /root/ACE/autobuild/configs/autobuild/remedynl/ibmcellbuild_xampler_technology.sh" />

    <command name="anonymous_shell"   options="cp -R /build/ACE/xampler/Avenger/_Templt/Template-Display /build/ACE/xampler/cellxampler" />
    <command name="anonymous_shell"   options="chmod 755 /build/ACE/xampler/cellxampler -R" />

    <command name="process_logs"      options="clean" />
    <command name="log"               options="OFF" />
    <command name="process_logs"      options="move prettify clean index" />
    <command name="status"            options="off" />

    <command name="shell"             options="rsync -e "/usr/bin/ssh -i /root/.ssh/id_dsa" --progress --recursive --delete -z cellxampler jwillemsen@naboo.dre.vanderbilt.edu:/web/users/jwillemsen/ibmcellbuild_xampler" />
    <command name="shell"             options="rsync -e "/usr/bin/ssh -i /root/.ssh/id_dsa" --progress --recursive --delete -z /root/ACE/ibmcellbuild_xamplerlog jwillemsen@naboo.dre.vanderbilt.edu:/web/users/jwillemsen" />

</autobuild>
