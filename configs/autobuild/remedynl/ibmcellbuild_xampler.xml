<autobuild>
    <configuration>
        <environment name="PATH"              value="/opt/cell/toolchain/bin" type="prefix"/>
        <environment name="HOST_ROOT"       value="/root/ACE/hoststatic/ACE_wrappers"/>
        <environment name="ACE_ROOT"        value="/build/ACE/xampler/ppu/ACE_wrappers"/>
        <environment name="BAGATEL_EXE"     value="/build/ACE/xampler/Bagatel/src/bagatel" />
