<!-- $Id$ -->
<autobuild>
    <configuration>
        <environment name="ACE_ROOT"        value="/home/build/ACE/GCCwarn/ACE_wrappers"/>
        <environment name="TAO_ROOT"        value="/home/build/ACE/GCCwarn/ACE_wrappers/TAO"/>
        <environment name="LD_LIBRARY_PATH" value="/usr/local/lib64:/home/build/ACE/GCCwarn/ACE_wrappers/lib" />
        <environment name="CVSROOT"         value=":ext:jwillemsen@cvs.doc.wustl.edu:/project/cvs-repository" />
        <environment name="CVS_RSH"         value="ssh" />

        <variable name="root"     value="/home/build/ACE/GCCwarn/" />
        <variable name="log_file" value="build.txt" />
        <variable name="log_root" value="/home/build/ACE/GCCwarn/" />
        <variable name="configs"  value="Linux Exceptions SSL" />

    </configuration>

    <command name="status"            options="on" />
    <command name="file_manipulation" options="type=mustnotexist file=.disable" />
    <command name="file_manipulation" options="type=create file=.disable output='RUNNING'" />
    <command name="log"               options="on" />
    <!-- Delete all the Makefiles so the cvs checkout won't complain. -->
    <command name="shell"             options="find ACE_wrappers -name 'GNUmakefile*' -exec rm '{}' ';'" />
    <command name="cvs"               options="-z 9 -q checkout -P ACE_wrappers" />
    <command name="cvs"               options="-z 9 -q checkout -P ACE_MPC"/>

    <command name="file_manipulation" options="type=create file=ACE_wrappers/ace/config.h output='#include \x22ace/config-linux.h\x22\n'"/>
    <command name="file_manipulation" options="type=create file=ACE_wrappers/include/makeinclude/platform_macros.GNU output='cidl=1\nssl=1\nsctp=1\ninline=0\nCCFLAGS+= -Woverloaded-virtual -Wcast-qual -Weffc++\ninclude $(ACE_ROOT)/include/makeinclude/platform_linux.GNU\n'" />
    <command name="file_manipulation" options="type=create file=ACE_wrappers/bin/MakeProjectCreator/config/default.features output='cidl=1\nssl=1\n'" />

    <command name="print_os_version" />
    <command name="check_compiler" options="gcc" />
    <command name="print_ace_config" />
    <command name="print_cidlc_version" />
    <command name="print_make_version" />
    <command name="print_perl_version" />

    <command name="generate_workspace" options="-exclude examples,performance-tests,apps,Kokyu,ASNMP,protocol,netsvcs,websvcs,ACEXML,TAO" />

    <command name="make"              options="-k realclean" />

    <command name="make"              options="-k" />

    <command name="log"               options="off" />
    <command name="process_logs"      options="clean=4 move prettify index" />
    <command name="file_manipulation" options="type=delete file=.disable" />
    <command name="status"            options="off" />
    <command name="shell"             options="rsync --progress --recursive --delete -z --exclude /ACE_wrappers/ --include *.txt --include *.html --exclude ** ./ johnnyw@naboo.dre.vanderbilt.edu:/web/users/remedynl/SuSE_x86_64_gccwarn" />
</autobuild>
