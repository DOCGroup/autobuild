<!-- $Id$ -->
<autobuild>
    <configuration>
      <environment name="ACE_ROOT"        value="/nfs/galadriel/VxWorks67lin_x86k/ACE_wrappers"/>
      <environment name="TAO_ROOT"        value="/nfs/galadriel/VxWorks67lin_x86k/ACE_wrappers/TAO"/>
      <environment name="CIAO_ROOT"        value="/nfs/galadriel/VxWorks67lin_x86k/ACE_wrappers/TAO/CIAO"/>
      <environment name="HOST_ROOT"        value="/home/build/ACE/VxWorks/host/ACE_wrappers"/>
      <environment name="LD_LIBRARY_PATH" value="/home/build/ACE/VxWorks/host/ACE_wrappers/lib:/nfs/galadriel/VxWorks67lin_x86k/ACE_wrappers/lib:/usr/local/lib" />
        <environment name="ACE_RUN_VX_TGTHOST" value="10.2.0.73"/>
        <environment name="ACE_RUN_VX_TGT_STARTUP_SCRIPT" value="/nfs/galadriel/VxWorks67lin_x86k/start.txt"/>
        <environment name="ACE_RUN_VX_TGTSVR_ROOT" value="/nfsace/galadriel/VxWorks67lin_x86k/ACE_wrappers"/>
        <environment name="ACE_TEST_VERBOSE" value="1"/>
        <environment name="ACE_RUN_VX_IBOOT" value="10.2.0.132:80"/>
        <environment name="ACE_RUN_VX_REBOOT_TIME" value="50"/>
        <environment name="ACE_RUN_VX_TGT_REBOOT"      value="1"/>
        <environment name="DOC_TEST_1" value="DOC_TEST_1"/>
        <environment name="DOC_TEST_1_OS" value="VXWORKS"/>
        <environment name="DOC_TEST_1_IPNAME" value="10.2.0.73" />
        <environment name="DOC_TEST_1_ACE_ROOT" value="/nfsace/galadriel/VxWorks67lin_x86k/ACE_wrappers" />
        <environment name="DOC_TEST_1_TAO_ROOT" value="/nfsace/galadriel/VxWorks67lin_x86k/ACE_wrappers/TAO" />
        <environment name="DOC_TEST_1_FS_ROOT" value="/nfsace/galadriel/VxWorks67lin_x86k/ACE_wrappers" />
        <environment name="DOC_TEST_1_HOSTNAME" value="10.2.0.73" />
        <environment name="DOC_TEST_1_HOST_ROOT" value="/nfs/galadriel/VxWorks67lin_x86k/ACE_wrappers" />
        <environment name="DOC_TEST_1_IBOOT" value="10.2.0.132:80" />
        <environment name="DOC_TEST_1_PROCESS_START_WAIT_INTERVAL" value="40" />
        <environment name="DOC_TEST_1_REBOOT" value="1" />
        <environment name="DOC_TEST_1_REBOOT_TIME" value="50" />


        <variable name="root"     value="/nfs/galadriel/VxWorks67lin_x86k/" />
        <variable name="log_file" value="build.txt" />
        <variable name="log_root" value="/nfs/galadriel/VxWorks67lin_x86k/" />
        <variable name="configs"  value="VxWorks Exceptions STATIC VxWorks67 NOXERCES" />

    </configuration>

    <command name="eval"              options="/home/build/WindRiver/wrenv.sh -p vxworks-6.7 -o print_env"/>
    <command name="status"            options="on" />
    <command name="process_listener"  options="localhost:32002" />
    <command name="log"               options="on" />

    <command name="svn"               options="co svn://svn.dre.vanderbilt.edu/DOC/Middleware/sets-anon/ACE+TAO+CIAO ." />

    <command name="file_manipulation" options="type=create file=ACE_wrappers/ace/config.h output='#define ACE_HAS_IP_MULTICAST\n#include \x22ace/config-vxworks.h\x22\n'"/>
    <command name="file_manipulation" options="type=create file=ACE_wrappers/include/makeinclude/platform_macros.GNU output='rtp=0\nstatic_libs=1\ndebug=1\ninline=0\noptimize=0\nTOOL = gnu\nCPU = PENTIUM4\ninclude $(ACE_ROOT)/include/makeinclude/platform_vxworks.GNU\n'" />
    <command name="file_manipulation" options="type=create file=ACE_wrappers/bin/MakeProjectCreator/config/default.features output='cidl=1\n'" />
    <command name="file_manipulation" options="type=create file=start.txt output='hostAdd(\x22enas\x22, \x2210.2.0.19\x22)\nnfsIdSet(1000)\nnfsMount(\x22enas\x22, \x22/share/develop/\x22, \x22/nfsace/\x22)\n'" />

    <command name="anonymous_shell"   options="scp -C johnnyw@naboo.dre.vanderbilt.edu:/web/users/isisbuilds/CIDLC/gnu-linux/cidlc ACE_wrappers/TAO/CIAO/bin" />

    <command name="print_os_version" />
    <command name="print_env_vars" />
    <command name="check_compiler" options="ccpentium" />
    <command name="print_ace_config" />
    <command name="print_cidlc_version" />
    <command name="print_make_version" />
    <command name="print_perl_version" />

    <command name="make"              options="-k realclean find=*akefile" />

    <command name="shell"             options="find ACE_wrappers -name 'GNUmakefile*' -exec rm '{}' ';'" />

    <command name="generate_workspace" options="-exclude TAO -type gnuace" />

    <command name="make"              options="-k" />

    <command name="auto_run_tests"    options=" -a" />

    <command name="log"               options="off" />
    <command name="process_logs"      options="clean move prettify index" />
    <command name="status"            options="off" />
    <command name="shell"             options="rsync --progress --recursive --delete -z --exclude /ACE_wrappers*/ --include *.txt --include *.html --exclude ** ./ johnnyw@naboo.dre.vanderbilt.edu:/web/users/remedynl/VxWorks67lin_x86k" />
</autobuild>
