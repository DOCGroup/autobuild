<autobuild>
    <configuration>
        <environment name="ACE_ROOT"     value="C:\\ACE\\VC90\\ACE_wrappers" />
        <environment name="TAO_ROOT"     value="C:\\ACE\\VC90\\ACE_wrappers\\TAO"/>
        <environment name="CIAO_ROOT"    value="C:\\ACE\\VC90\\ACE_wrappers\\TAO\\CIAO"/>
        <environment name="DANCE_ROOT"    value="C:\\ACE\\VC90\\ACE_wrappers\\TAO\\CIAO\\DAnCE"/>
        <environment name="RSYNC_RSH"    value="C:\\cygwin\\bin\\ssh" />
        <environment name="PATH"         value="C:\\Program Files\\Microsoft Visual Studio 9.0\\Common7\\IDE;C:\\Program Files\\Microsoft Visual Studio 9.0\\VC\\bin;C:\\ACE\\VC90\\ACE_wrappers\\bin;C:\\ACE\\VC90\\ACE_wrappers\\lib;c:\\xerces-c_2_5_0-windows_nt-msvc_90\\lib" type="prefix" />
        <environment name="XERCESCROOT"  value="C:\\xerces-c_2_5_0-windows_nt-msvc_90" />

        <variable name="configs"  value="Win32 VC90 Exceptions NOXERCES" />
        <variable name="root"     value="C:\\ACE\\VC90\\" />
        <variable name="log_file" value="build.txt" />
        <variable name="log_root" value="C:\\ACE\\VC90\\" />
        <variable name="name"     value="WinXP VS.NET 2008" />
    </configuration>

    <command name="status"            options="on" />
    <command name="process_listener"  options="localhost:32001" />

    <command name="win32make"         options="msvc_mpc_auto_compile -vc9 -ALL -Debug -k -clean" />

    <command name="log"               options="ON" />

    <command name="svn"               options="co svn://svn.dre.vanderbilt.edu/DOC/Middleware/sets-anon/ACE+TAO+CIAO ." />

    <command name="shell"             options="c:\\cygwin\\bin\\find ACE_wrappers -name '*.vcproj' -exec c:\\cygwin\\bin\\rm '{}' ';'" />
    <command name="shell"             options="c:\\cygwin\\bin\\find ACE_wrappers -name '*.sln' -exec c:\\cygwin\\bin\\rm '{}' ';'" />

    <command name="file_manipulation" options="type=create file=ACE_wrappers/ace/config.h output='#define ACE_DISABLE_WIN32_INCREASE_PRIORITY\n#define ACE_DISABLE_WIN32_ERROR_WINDOWS\n#define ACE_HAS_STANDARD_CPP_LIBRARY 1\n#include \x22ace/config-win32.h\x22\n'" />
    <command name="file_manipulation" options="type=create file=ACE_wrappers/bin/MakeProjectCreator/config/default.features output='cidl=1\nxerces=0\n'" />

    <command name="anonymous_shell"   options="c:\\cygwin\\bin\\scp -C johnnyw@naboo.dre.vanderbilt.edu:/web/users/isisbuilds/CIDLC/Win32/cidlc.exe ACE_wrappers/TAO/CIAO/bin" />
    <command name="anonymous_shell"   options="c:\\cygwin\\bin\\chmod 777 ACE_wrappers/TAO/CIAO/bin/cidlc.exe" />

    <command name="print_os_version" />
    <command name="print_env_vars" />
    <command name="check_compiler" options="msvc" />
    <command name="print_ace_config" />
    <command name="print_cidlc_version" />
    <command name="print_perl_version" />

    <command name="generate_workspace" options="-type vc9 -recurse -value_template Detect64BitPortabilityProblems=TRUE -exclude TAO/TAO_*.mwc,TAO/CIAO/CIAO_*.mwc"/>

    <command name="win32make"         options="msvc_mpc_auto_compile -vc9 -ALL -Debug -k" />

    <command name="auto_run_tests"    options="-s \x22C:\\ACE\\autobuild\\win32sandbox\\sandbox.exe -t 5400000\x22" />

    <command name="log"               options="OFF" />
    <command name="process_logs"      options="move prettify clean index" />
    <command name="status"            options="off" />
    <command name="shell"             options="c:\\cygwin\\bin\\rsync --progress --recursive --delete -z --exclude /ACE_wrappers*/ --include *.txt --include *.html --exclude ** ./ johnnyw@naboo.dre.vanderbilt.edu:/web/users/remedynl/VC90" />
</autobuild>
