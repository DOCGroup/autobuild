<!-- $Id$ -->
<autobuild>
    <configuration>
        <environment name="ACE_ROOT"     value="D:\ACE\Intel90_64bit\ACE_wrappers" />
        <environment name="TAO_ROOT"     value="D:\ACE\Intel90_64bit\ACE_wrappers\TAO"/>
        <environment name="CVSROOT"      value=":ext:jwillemsen@cvs.doc.wustl.edu:/project/cvs-repository" />
        <environment name="CVS_RSH"      value="ssh" />
        <environment name="RSYNC_RSH"    value="D:\cygwin\bin\ssh" />
        <environment name="PATH"         value="D:\Program Files\Microsoft Platform SDK\Include\;D:\Program Files (x86)\Microsoft Visual C++ Toolkit 2003\bin;D:\Program Files (x86)\Microsoft Visual C++ Toolkit 2003\include;D:\ACE\Intel90_64bit\ACE_wrappers\bin;D:\ACE\\ACE\Intel90_64bit\ACE_wrappers\lib;D:\Cygwin\bin" type="prefix" />
        <environment name="INCLUDE"      value="D:\Program Files\Microsoft Platform SDK\Include\;D:\Program Files (x86)\Microsoft Visual C++ Toolkit 2003\include" type="prefix" />

        <variable name="configs"  value="Win32 Exceptions" />
        <variable name="root"     value="D:\ACE\Intel90_64bit\" />
	<variable name="log_file" value="log.txt" />
        <variable name="log_root" value="D:\ACE\Intel90_64bit\" />
        <variable name="name"     value="Intel 90 64bit" />
        <variable name="cvs_program"     value="D:\cygwin\bin\cvs" />
    </configuration>

    <command name="status"            options="on" />
    <command name="file_manipulation" options="type=mustnotexist file=.disable" />
    <command name="file_manipulation" options="type=create file=.disable output='RUNNING'" />

    <command name="win32make"         options="nmake CPP=icl.exe LINK32=xilink.exe CFG=\x22Win32 Debug\x22 REALCLEAN" />

    <command name="log"               options="ON" />

    <command name="cvs"               options="-z9 -q checkout -P ACE_wrappers" />
    <command name="cvs"               options="-z9 -q checkout -P ACE_MPC"/>

    <command name="shell"             options="find ACE_wrappers -name '*.make' -exec rm '{}' ';'" />
    <command name="shell"             options="find ACE_wrappers -name 'Makefile' -exec rm '{}' ';'" />

    <command name="file_manipulation" options="type=create file=ACE_wrappers/ace/config.h output='#define ACE_HAS_IPV6\n#define ACE_DISABLE_WIN32_ERROR_WINDOWS\n#define ACE_HAS_STANDARD_CPP_LIBRARY 1\n#include \x22ace/config-win32.h\x22\n'" />
    <command name="file_manipulation" options="type=create file=ACE_wrappers/bin/MakeProjectCreator/config/default.features output='rwho=0\nqos=1\n'" />

    <command name="print_os_version" />
    <command name="check_compiler" options="icc" />
    <command name="print_ace_config" />
    <command name="print_perl_version" />

    <command name="generate_workspace" options="-type nmake -exclude TAO -recurse -value_template add_compile+=/Wp64 -value_template cc=icl.exe -value_template link=xilink.exe -value_template platforms=Win86_64"/>

    <command name="win32make"         options="nmake /I CFG=\x22Win32 Debug\x22 NO_EXTERNAL_DEPS=1" />

    <command name="log"               options="OFF" />
    <command name="process_logs"      options="move prettify clean index" />
    <command name="file_manipulation" options="type=delete file=.disable" />
    <command name="status"            options="off" />
    <command name="shell"             options="D:\cygwin\bin\rsync --progress --recursive --delete -z --exclude /ACE_wrappers/ --include *.txt --include *.html --exclude ** ./ johnnyw@naboo.dre.vanderbilt.edu:/web/users/remedynl/ICC90_64bit" />
</autobuild>
