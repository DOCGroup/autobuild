    <command name="print_os_version" />
    <command name="print_env_vars" />
    <command name="check_compiler" options="gcc" />
    <command name="print_ace_config" />
    <command name="print_make_version" />
    <command name="print_perl_version" />

    <command name="generate_workspace" options="-exclude examples,TAO,TAO/CIAO -type gnuace -value_template platforms=\x22'eBox_4300 (x86)'\x22 -value_template tao_idl=$(HOST_TAO_IDL)" />
    <command name="generate_workspace" options="-exclude examples,TAO,TAO/CIAO -type vxtest" />

    <command name="make"              options="-k realclean find=*akefile" />
    <command name="make"              options="-k" />

    <command name="auto_run_tests"    options="-a" />

    <command name="log"               options="OFF" />
    <command name="process_logs"      options="move prettify clean index" />
    <command name="status"            options="off" />
    <command name="shell"             options="/usr/bin/rsync --progress --recursive --delete -z --exclude /ACE_wrappers*/ --include *.txt --include *.html --exclude ** ./ johnnyw@naboo.dre.vanderbilt.edu:/web/users/remedynl/suse111gegcc052" />
