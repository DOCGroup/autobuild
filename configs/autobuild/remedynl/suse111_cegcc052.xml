    <command name="shell"             options="find ACE_wrappers -name 'GNUmakefile*' -exec rm '{}' ';'" />

    <command name="file_manipulation" options="type=create file=ACE_wrappers/ace/config.h output='#define TEST_DIR ACE_TEXT (\x22/network/temp/ACE_wrappers/tests/\x22)\n#define ACE_DEFAULT_TEST_DIR ACE_TEXT (\x22/network/temp/ACE_wrappers/tests/\x22)\n#define UNICODE 1\n#define _UNICODE 1\n#define _WINDOWS 1\n#define CEVER\n#define ACE_USES_WCHAR\n#define WIN32 1\n#define ACE_HAS_WINCE\n#define _WINSOCKAPI_\n#undef _WIN32_WCE\n#define _WIN32_WCE 0x600\n#include \x22ace/config-win32.h\x22\n'"/>
    <command name="file_manipulation" options="type=create file=ACE_wrappers/include/makeinclude/platform_macros.GNU output='wince=1\nuses_wchar=1\ninline=0\nTAO_IDL := $(HOST_ROOT)/bin/tao_idl\nTAO_IDLFLAGS += -g $(HOST_ROOT)/bin/gperf\nTAO_IDL_DEP := $(HOST_ROOT)/bin/tao_idl$(EXEEXT)\ninclude $(ACE_ROOT)/include/makeinclude/platform_cegcc.GNU\n'" />
