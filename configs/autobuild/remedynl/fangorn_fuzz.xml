<!-- $Id$ -->
<autobuild>
    <configuration>
        <environment name="ACE_ROOT"        value="/home/build/ACE/fuzz/ACE_wrappers"/>
        <environment name="TAO_ROOT"        value="/home/build/ACE/fuzz/ACE_wrappers/TAO"/>
        <environment name="CIAO_ROOT"        value="/home/build/ACE/fuzz/ACE_wrappers/TAO/CIAO"/>
        <environment name="DANCE_ROOT"        value="/home/build/ACE/fuzz/ACE_wrappers/TAO/CIAO/DAnCE">
        <environment name="LD_LIBRARY_PATH" value="/home/build/ACE/fuzz/ACE_wrappers/lib:/usr/local/lib:/home/build/ndds.4.4d/lib/i86Linux2.6gcc4.1.1/" />
        <environment name="NDDSHOME" value="/home/build/ndds.4.4d" />
        <environment name="NDDSARCHITECTURE" value="i86Linux2.6gcc4.1.1" />
        <environment name="CIAO_TRACE_ENABLE" value="1" />
        <environment name="DANCE_LOG_LEVEL" value="10" />
        <environment name="SSL_ROOT"        value="/usr"/>

        <variable name="root"     value="/home/build/ACE/fuzz/" />
        <variable name="log_file" value="build.txt" />
        <variable name="log_root" value="/home/build/ACE/fuzz/" />
        <variable name="configs"  value="Linux Exceptions ZLIB SSL ZIOP NDDS" />

    </configuration>

    <command name="status"            options="on" />
    <command name="process_listener"  options="localhost:32003" />
    <command name="log"               options="on" />

    <command name="svn"               options="co svn://svn.dre.vanderbilt.edu/DOC/Middleware/sets-anon/ACE+TAO+CIAO ." />

    <command name="print_os_version" />
    <command name="print_env_vars" />
    <command name="check_compiler" options="gcc" />
    <command name="print_ace_config" />
    <command name="print_openssl_version"/>
    <command name="print_make_version" />
    <command name="print_perl_version" />

    <command name="fuzz"         options="" />

    <command name="log"               options="off" />
    <command name="process_logs"      options="clean=24 move prettify index" />
    <command name="status"            options="off" />
    <command name="shell"             options="rsync --progress --recursive --delete -z --exclude /ACE_wrappers*/ --include *.txt --include *.html --exclude ** ./ johnnyw@naboo.dre.vanderbilt.edu:/web/users/remedynl/fangorn_fuzz" />
</autobuild>
