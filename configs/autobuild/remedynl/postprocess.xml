<!-- $Id$ -->
<autobuild>
    <command name="anonymous_shell"   options="killall dance_locality_manager" />
    <command name="log"               options="off" />
    <command name="process_logs"      options="clean move prettify index" />
    <command name="status"            options="off" />
    <command name="shell"             options="rsync --progress --recursive --delete -z --exclude /.svn/ --exclude $DOC_ROOT_NAME --exclude $MPC_NAME --include *.txt --include *.html --exclude ** ./ $RSYNC_HOST/$BUILD_NAME" />
</autobuild>
