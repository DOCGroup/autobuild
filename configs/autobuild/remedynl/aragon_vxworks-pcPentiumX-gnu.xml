<!-- $Id$ -->
<autobuild>
    <configuration>

        <environment name="ACE_ROOT"                value="C:/ACE/VxWorks/pcPentiumX/ACE_wrappers" />
        <environment name="TAO_ROOT"                value="C:/ACE/VxWorks/pcPentiumX/ACE_wrappers/TAO"/>
        <environment name="HOST_ROOT"               value="C:/ACE/VxWorks/CB6sr/ACE_wrappers"/>
        <environment name="PATH"                    value="C:\Tornado2.2\host\x86-win32\bin;C:\tools\nixtools;C:\ACE\VxWorks\CB6sr\ACE_wrappers\lib;C:\PROGRA~1\Borland\CBUILD~1\Bin;C:\PROGRA~1\Borland\CBUILD~1\Projects\Bpl;" type="prefix" />
        <environment name="WIND_BASE"               value="C:/Tornado2.2"/>
        <environment name="WIND_HOST_TYPE"          value="x86-win32"/>
        <environment name="CVSROOT"                 value=":ext:jwillemsen@cvs.doc.wustl.edu:/project/cvs-repository" />
        <environment name="CVS_RSH"                 value="C:\cygwin\bin\ssh" />
        <environment name="RSYNC_RSH"               value="C:\cygwin\bin\ssh" />
        <environment name="ACE_MKDIR"               value="C:\tools\nixtools\mkdir -p" />
        <environment name="ACE_RUN_VX_TGTSVR"       value="P4" />
        <environment name="ACE_RUN_VX_TGTSVR_ROOT"  value="/tgtsvr/ACE/VxWorks/pcPentiumX/ACE_wrappers" />
        <environment name="ACE_RUN_VX_TGTSVR_DEFGW" value="10.2.128.1" />
        <environment name="ACE_RUN_VX_REBOOT_TIME"  value="40" />
        <environment name="ACE_RUN_VX_REBOOT_TOOL"  value="perl C:/ACE/VxWorks/P4_reboot.pl" />
        <environment name="ACE_RUN_VX_TGT_REBOOT"   value="1" />
        <environment name="ACE_RUN_VX_TGT_HOST"     value="10.2.128.65" />

        <variable name="root"     	value="C:\ACE\VxWorks\pcPentiumX\" />
        <variable name="log_file" 	value="build.txt" />
        <variable name="log_root" 	value="C:\ACE\VxWorks\pcPentiumX\" />
        <variable name="configs"  	value="VxWorks STATIC" />
        <variable name="cvs_program"  	value="c:\cygwin\bin\cvs" />

    </configuration>

    <command name="status"            options="on" />
    <command name="file_manipulation" options="type=mustnotexist file=.disable" />
    <command name="file_manipulation" options="type=create file=.disable output='RUNNING'" />
    <command name="log"               options="on" />
    <!-- Delete all the Makefiles so the cvs checkout won't complain. -->
    <command name="shell"             options="find ACE_wrappers -name GNUmakefile* -exec \x22rm {}\x22 \x22;\x22" />
    <command name="cvs"               options="-z 9 -q checkout -P ACE_wrappers" />
    <command name="cvs"               options="-z 9 -q checkout -P ACE_MPC"/>

    <command name="file_manipulation" options="type=create file=ACE_wrappers/ace/config.h output='#define ACE_HAS_IP_MULTICAST\n#include \x22ace/config-vxworks5.x.h\x22\n'"/>
    <command name="file_manipulation" options="type=create file=ACE_wrappers/include/makeinclude/platform_macros.GNU output='static_libs=1\ndebug=1\ninline=0\nfl_reactor=0\noptimize=0\noverride RM:=C:/tools/nixtools/rm -f\nTOOL = gnu\nTAO_IDL := $(HOST_ROOT)/bin/tao_idl.exe\nTAO_IDL_DEP := $(TAO_IDL)\nTAO_IDLFLAGS += -g $(HOST_ROOT)/bin/gperf\nCPU = PENTIUM4\nCIDLC := $(ACE_ROOT)/TAO/CIAO/bin/cidlc.exe\nCIDLC_DEP := $(CIDLC_IDL)\ninclude $(ACE_ROOT)/include/makeinclude/platform_vxworks5.5.x.GNU\n'" />
    <command name="file_manipulation" options="type=create file=ACE_wrappers/bin/MakeProjectCreator/config/default.features output='cidl=1\nxerces=0\n'" />

    <command name="anonymous_shell"   options="c:\cygwin\bin\scp -C johnnyw@naboo.dre.vanderbilt.edu:/web/users/isisbuilds/CIDLC/Win32/cidlc.exe ACE_wrappers/TAO/CIAO/bin" />
    <command name="anonymous_shell"   options="c:\cygwin\bin\chmod 777 ACE_wrappers/TAO/CIAO/bin/cidlc.exe" />

    <command name="print_os_version" />
    <command name="check_compiler" options="ccpentium" />
    <command name="print_ace_config" />
    <command name="print_cidlc_version" />
    <command name="print_make_version" />
    <command name="print_perl_version" />

    <!-- Uses ACE.mwc -->
    <command name="generate_workspace" options="-exclude TAO,tests/HTBP" />
    <!-- Uses TAO/TAO.mwc -->
    <command name="generate_workspace" options="dirs=TAO -exclude TAO_*.mwc,CIAO" />
    <!-- Uses CIAO -->
    <command name="generate_workspace" options="dirs=TAO/CIAO -exclude CIAO_*.mwc" />

    <command name="make"              options="-k realclean" />
    <command name="make"              options="-k -C TAO realclean" />
    <command name="make"              options="-k -C TAO/CIAO realclean" />

    <command name="make"              options="-k -C ace" />
    <command name="make"              options="-k -C Kokyu" />
    <command name="make"              options="-k -C ACEXML" />
    <command name="make"              options="-k -C protocols" />
    <!-- command name="make"              options="-k -C examples" / -->
    <!-- command name="make"              options="-k -C netsvcs" / -->
    <!-- command name="make"              options="-k -C performance-tests" / -->
    <command name="make"              options="-k -C tests" />
    <!-- command name="make"              options="-k -C websvcs" / -->
    <!-- command name="make"              options="-k -C TAO/TAO_IDL" / -->
    <!-- command name="make"              options="-k -C TAO idl_stubs" / -->
    <command name="make"              options="-k -C TAO/tao" />
    <command name="make"              options="-k -C TAO/tests" />
    <command name="make"              options="-k -C TAO/orbsvcs" />
    <command name="make"              options="-k -C TAO/CIAO" />

    <command name="auto_run_tests" options="-a -o -s \x22C:\ACE\autobuild\win32sandbox\sandbox.exe -t 21600000\x22" />

    <command name="log"               options="off" />
    <command name="process_logs"      options="clean move prettify index" />
    <command name="file_manipulation" options="type=delete file=.disable" />
    <command name="status"            options="off" />

    <command name="shell"             options="c:\cygwin\bin\rsync --progress --recursive --delete -z --exclude /ACE_wrappers/ --include *.txt --include *.html --exclude ** ./ johnnyw@naboo.dre.vanderbilt.edu:/web/users/remedynl/VxWorks_PentiumX_GNU" />
</autobuild>
