<!-- $Id$ -->
<autobuild>
    <configuration>
        <environment name="ACE_ROOT"              value="/nfs/solaris/VxWorks55ppc603/ACE_wrappers"/>
        <environment name="TAO_ROOT"              value="/nfs/solaris/VxWorks55ppc603/ACE_wrappers/TAO"/>
        <environment name="CIAO_ROOT"             value="/nfs/solaris/VxWorks55ppc603/ACE_wrappers/TAO/CIAO"/>
        <environment name="HOST_ROOT"             value="/nfs/solaris/host55ppc603/ACE_wrappers"/>
        <environment name="LD_LIBRARY_PATH"       value="/nfs/solaris/host55ppc603/ACE_wrappers/lib:/usr/local/lib" type="prefix"/>
        <environment name="LD_LIBRARY_PATH"       value="/usr/Tornado2.2/host/sun4-solaris2/lib:" type="prefix"/>
        <environment name="PATH"                  value="/usr/local/bin:/usr/Tornado2.2/host/sun4-solaris2/bin:" type="prefix" />
        <environment name="WIND_BASE"             value="/usr/Tornado2.2"/>
        <environment name="WIND_HOST_TYPE"        value="sun4-solaris2"/>
        <environment name="LM_LICENSE_FILE"       value="27000@vxlicense"/>
        <environment name="ACE_RUN_VX_TGTHOST" value="10.2.0.74"/>
        <environment name="ACE_RUN_VX_TGTSVR_ROOT" value="/nfsace/solaris/VxWorks55ppc603/ACE_wrappers"/>
        <environment name="ACE_TEST_VERBOSE" value="1"/>
        <environment name="ACE_RUN_VX_IBOOT" value="10.2.0.129:80"/>
        <environment name="ACE_RUN_VX_REBOOT_TIME" value="45"/>
        <environment name="ACE_RUN_VX_TGT_REBOOT"      value="1"/>

        <variable name="root"     value="/nfs/solaris/VxWorks55ppc603" />
        <variable name="log_file" value="build.txt" />
        <variable name="log_root" value="/nfs/solaris/VxWorks55ppc603" />
        <variable name="configs"  value="VxWorks Exceptions STATIC MINIMUM" />
        <variable name="svn_program"  value="/usr/local/bin/svn" />

    </configuration>

    <command name="status"            options="on" />
    <command name="process_listener"  options="localhost:32002" />

    <command name="log"               options="on" />

    <command name="make"              options="-k realclean" />

    <command name="svn"               options="co svn://svn.dre.vanderbilt.edu/DOC/Middleware/sets-anon/ACE+TAO ." />

    <command name="file_manipulation" options="type=create file=ACE_wrappers/TAO_Host.mwc output='workspace {\nace\ntests\nTAO/tao\nTAO/tests\n}\n'" />
    <command name="file_manipulation" options="type=create file=ACE_wrappers/ace/config.h output='#define ACE_HAS_IP_MULTICAST\n#include \x22ace/config-vxworks5.x.h\x22\n'"/>
    <command name="file_manipulation" options="type=create file=ACE_wrappers/include/makeinclude/platform_macros.GNU output='minimum_corba=1\nstatic_libs=1\ndebug=1\ninline=0\noptimize=0\nCPU = PPC603\ninclude $(ACE_ROOT)/include/makeinclude/platform_vxworks5.5.x.GNU\n'" />
    <command name="file_manipulation" options="type=create file=ACE_wrappers/bin/MakeProjectCreator/config/default.features output='minimum_corba=1\n'" />

    <command name="print_os_version" />
    <command name="check_compiler" options="ccppc" />
    <command name="print_ace_config" />
    <command name="print_make_version" />
    <command name="print_perl_version" />

    <command name="generate_workspace" options="TAO_Host.mwc -type gnuace" />

    <command name="make"              options="-k" />

    <command name="log"               options="off" />
    <command name="process_logs"      options="clean move prettify index" />
    <command name="status"            options="off" />

    <command name="shell"             options="/usr/local/bin/rsync --progress --recursive --delete -z --exclude /ACE_wrappers*/ --include *.txt --include *.html --exclude ** ./ johnnyw@naboo.dre.vanderbilt.edu:/web/users/remedynl/vx551_sol_ppc603" />
</autobuild>
