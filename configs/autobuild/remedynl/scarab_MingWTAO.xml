<!-- $Id$ -->
<autobuild>
    <configuration>
        <environment name="ACE_ROOT"     value="C:/ACE/MingWTAO/ACE_wrappers" />
        <environment name="TAO_ROOT"     value="C:/ACE/MingWTAO/ACE_wrappers/TAO"/>
        <environment name="CVSROOT"      value=":ext:jwillemsen@cvs.doc.wustl.edu:/project/cvs-repository" />
        <environment name="CVS_RSH"      value="C:\cygwin\bin\ssh" />
        <environment name="RSYNC_RSH"    value="C:\cygwin\bin\ssh" />
        <environment name="Path"         value="C:\perl\bin;C:\MingW\bin;C:\Cygwin\bin;C:\ACE\MingWTAO\ACE_wrappers\bin;C:\ACE\MingWTAO\ACE_wrappers\lib;" type="prefix" />
        <environment name="include"      value="" />
        <environment name="lib"          value="" />

        <variable name="configs"  value="Win32" />
        <variable name="root"     value="C:\ACE\MingWTAO\" />
        <variable name="log_file" value="build.txt" />
        <variable name="log_root" value="C:\ACE\MingWTAO\" />
        <variable name="name"     value="MingWTAO" />
        <variable name="configs"  value="NOXERCES" />
    </configuration>
    <command name="status"            options="on" />
    <command name="file_manipulation" options="type=mustnotexist file=.disable" />
    <command name="file_manipulation" options="type=create file=.disable output='RUNNING'" />
    <command name="log"               options="ON" />
    <!-- Delete all the Makefiles so the cvs checkout won't complain. -->
    <command name="shell"             options="find ACE_wrappers -name 'GNUmakefile*' -exec rm '{}' ';'" />
    <command name="cvs"               options="-z 9 -q checkout -P ACE_wrappers" />
    <command name="cvs"               options="-z 9 -q checkout -P ACE_MPC"/>
    <command name="file_manipulation" options="type=create file=ACE_wrappers/ace/config.h output='#define ACE_DISABLE_WIN32_ERROR_WINDOWS\n#define ACE_HAS_STANDARD_CPP_LIBRARY 1\n#include \x22ace/config-win32.h\x22\n'" />
    <command name="file_manipulation" options="type=create file=ACE_wrappers/include/makeinclude/platform_macros.GNU output='debug=1\ninclude $(ACE_ROOT)/include/makeinclude/platform_mingw32.GNU\n'" />
    <command name="file_manipulation" options="type=create file=ACE_wrappers/bin/MakeProjectCreator/config/default.features output='cidl=1\nxerces=0\n'" />
    <command name="anonymous_shell"   options="scp -C johnnyw@naboo.dre.vanderbilt.edu:/web/users/isisbuilds/CIDLC/Win32/cidlc.exe ACE_wrappers/TAO/CIAO/bin" />
    <command name="anonymous_shell"   options="chmod 777 ACE_wrappers/TAO/CIAO/bin/cidlc.exe" />
    <command name="print_os_version" />
    <command name="check_compiler" options="gcc" />
    <command name="print_ace_config" />
    <command name="print_cidlc_version" />
    <command name="print_make_version" />
    <command name="print_perl_version" />
    <command name="generate_workspace" options="-exclude TAO/TAO_*.mwc,TAO/CIAO/CIAO_*.mwc" />
    <command name="make"              options="-k realclean" />
    <command name="make"              options="-k" />
    <command name="auto_run_tests"    options="-s \x22C:\ACE\autobuild\win32sandbox\sandbox.exe -t 5400000\x22" />
    <command name="log"               options="OFF" />
    <command name="process_logs"      options="move prettify clean index" />
    <command name="file_manipulation" options="type=delete file=.disable" />
    <command name="status"            options="off" />
    <command name="shell"             options="c:\cygwin\bin\rsync --progress --recursive --delete -z --exclude /ACE_wrappers/ --include *.txt --include *.html --exclude ** ./ johnnyw@naboo.dre.vanderbilt.edu:/web/users/remedynl/MinGWTAO" />
</autobuild>
