<!-- $Id$ -->
<autobuild>
    <configuration>
        <environment name="ACE_ROOT"              value="/nfs/solaris/557/ACE_wrappers"/>
        <environment name="TAO_ROOT"              value="/nfs/solaris/557/ACE_wrappers/TAO"/>
        <environment name="CIAO_ROOT"             value="/nfs/solaris/557/ACE_wrappers/TAO/CIAO"/>
        <environment name="HOST_ROOT"             value="/nfs/solaris/host/ACE_wrappers"/>
        <environment name="LD_LIBRARY_PATH"       value="/WindRiver/vxworks-6.3/host/sun4-solaris2/lib:/WindRiver/scopetools-5.5/host/bin/sun4-solaris2:/WindRiver/workbench-2.5/sun4-solaris2/lib:/WindRiver/workbench-2.5/foundation/4.0.9/sun4-solaris2/lib:/usr/local/lib" type="prefix"/>
        <environment name="PATH"                  value="/WindRiver/vxworks-6.3/host/sun4-solaris2/bin:/WindRiver/scopetools-5.5/host/bin/sun4-solaris2:/WindRiver:/WindRiver/workbench-2.5/sun4-solaris2/bin:/WindRiver/workbench-2.5/foundation/4.0.9/sun4-solaris2/bin:/WindRiver/gnu/3.4.4-vxworks-6.3/sun4-solaris2/bin" type="prefix" />
        <environment name="WIND_HOME"             value="/WindRiver" />
        <environment name="WIND_BASE"             value="/WindRiver/vxworks-6.3"/>
        <environment name="WIND_DFW_PATH"         value="/WindRiver/workbench-2.5/dfw/145b"/>
        <environment name="WIND_USR"              value="/WindRiver/vxworks-6.3/target/usr"/>
        <environment name="WIND_FOUNDATION_PATH"  value="/WindRiver/workbench-2.5/foundation/4.0.9"/>
        <environment name="WIND_GNU_PATH"         value="/WindRiver/gnu/3.4.4-vxworks-6.3"/>
        <environment name="WIND_HOST_TYPE"        value="sun4-solaris2"/>
        <environment name="WIND_HOME"             value="/WindRiver"/>
        <environment name="WIND_TOOLS"            value="/WindRiver/workbench-2.5"/>
        <environment name="WIND_DOCS"             value="/WindRiver/docs"/>
        <environment name="WIND_JRE_HOME"         value="/WindRiver/jre/1.4.2/sun4-solaris2"/>
        <environment name="WIND_WRWB_PATH"        value="/WindRiver/workbench-2.5/wrwb/platform/eclipse"/>
        <environment name="WIND_WRSV_PATH"        value="/WindRiver/workbench-2.5/wrsv/4.8"/>
        <environment name="WIND_SAMPLES"          value="/WindRiver/vxworks-6.3/target/usr/apps/samples:/WindRiver/vxworks-6.3/target/src/demo:/WindRiver/workbench-2.5/samples"/>
        <environment name="WRSD_LICENSE_FILE"     value="/WindRiver/license"/>
        <environment name="TCLLIBPATH"            value="/WindRiver/vxworks-6.3/host/resource/tcl"/>
        <environment name="WIND_PLATFORM"         value="vxworks-6.3"/>
        <environment name="ACE_RUN_VX_TGTHOST"     value="10.2.0.71"/>
        <environment name="ACE_RUN_VX_TGT_HOST"     value="10.2.0.71"/>
        <environment name="ACE_RUN_VX_TGTSVR_ROOT" value="/nfsace/solaris/557/ACE_wrappers"/>
        <environment name="ACE_TEST_VERBOSE"       value="0"/>
        <environment name="ACE_RUN_VX_REBOOT_CMD"  value="reboot"/>
        <environment name="ACE_RUN_VX_REBOOT_TIME" value="30"/>
        <environment name="ACE_RUN_VX_TGT_REBOOT" value="1"/>
        <!-- environment name="ACE_RUN_VX_NO_INITIAL_REBOOT" value="1"/ -->

        <variable name="root"     value="/nfs/solaris/557" />
        <variable name="log_file" value="build.txt" />
        <variable name="log_root" value="/nfs/solaris/557" />
        <variable name="configs"  value="VxWorks Exceptions STATIC MINIMUM" />

    </configuration>

    <command name="status"            options="on" />
    <command name="process_listener"  options="localhost:32002" />

    <!-- command name="make"              options="-k realclean" / -->

    <command name="log"               options="on" />

    <command name="print_os_version" />
    <command name="check_compiler" options="ccppc" />
    <command name="print_ace_config" />
    <command name="print_make_version" />
    <command name="print_perl_version" />

    <!-- command name="make"              options="-k" / -->

    <command name="auto_run_tests" options="-a -o" />

    <command name="log"               options="off" />
    <command name="process_logs"      options="clean move prettify index" />
    <command name="status"            options="off" />
</autobuild>
