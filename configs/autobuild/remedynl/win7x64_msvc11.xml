<!-- $Id$ -->
<autobuild>
    <configuration>
        <environment name="ACE_ROOT"    value="C:\\ACE\\trunk\\DOC_ROOT\\ACE" />
        <environment name="TAO_ROOT"    value="C:\\ACE\\trunk\\DOC_ROOT\\TAO"/>
        <environment name="CIAO_ROOT"   value="C:\\ACE\\trunk\\DOC_ROOT\\CIAO"/>
        <environment name="DANCE_ROOT"  value="C:\\ACE\\trunk\\DOC_ROOT\\DAnCE"/>
        <environment name="MPC_ROOT"    value="C:\\ACE\\trunk\\DOC_ROOT\\MPC"/>
        <environment name="RSYNC_RSH"   value="C:\\cygwin\\bin\\ssh" />
        <environment name="PATH"        value="C:\\ACE\\trunk\\DOC_ROOT\\ACE\\bin;C:\\ACE\\trunk\\DOC_ROOT\\ACE\\lib;" type="prefix" />

        <variable name="configs"  value="Win32 VC110" />
        <variable name="root"     value="C:\\ACE\\trunk\\" />
        <variable name="log_file" value="build.txt" />
        <variable name="log_root" value="C:\\ACE\\trunk\\" />
        <variable name="name"     value="Windows 7 Professional Visual Studio 11" />
        <variable name="svn_program"  value="c:\\cygwin\\bin\\svn" />
        <variable name="project_root" value="C:\\ACE\\trunk\\DOC_ROOT" />
    </configuration>

    <command name="status"            options="on" />
    <command name="process_listener"  options="localhost:32001" />

    <command name="print_env_vars" />

    <command name="eval"             options="\x22C:\\Program Files (x86)\\Microsoft Visual Studio 11.0\\VC\\vcvarsall.bat\x22 x86 | set" />

    <command name="print_env_vars" />

    <command name="win32make"         options="nmake /I CFG=\x22Win32 Debug\x22 REALCLEAN" />

    <command name="log"               options="ON" />

    <command name="svn"               options="co svn://svn.dre.vanderbilt.edu/DOC/Middleware/trunk DOC_ROOT" />
    <command name="svn"               options="co svn://svn.dre.vanderbilt.edu/DOC/MPC/trunk DOC_ROOT/MPC" />

    <command name="shell"             options="c:\\cygwin\\bin\\find DOC_ROOT -name '*.mak' -exec c:\\cygwin\\bin\\rm '{}' ';'" />
    <command name="shell"             options="c:\\cygwin\\bin\\find DOC_ROOT -name '*.manifest' -exec c:\\cygwin\\bin\\rm '{}' ';'" />

    <command name="file_manipulation" options="type=create file=DOC_ROOT/ACE/ace/config.h output='#define ACE_DISABLE_WIN32_INCREASE_PRIORITY\n#define ACE_DISABLE_WIN32_ERROR_WINDOWS\n#include \x22ace/config-win32.h\x22\n'" />
    <command name="file_manipulation" options="type=create file=DOC_ROOT/ACE/bin/MakeProjectCreator/config/default.features output='versioned_namespace=1\n'" />

    <command name="print_os_version" />
    <command name="print_env_vars" />
    <command name="check_compiler" options="msvc" />
    <command name="print_ace_config" />
    <command name="print_perl_version" />

    <command name="generate_workspace" options="-type nmake -base vc11nmake -exclude ADBC -exclude TAO,CIAO,DAnCE"/>

    <command name="win32make"         options="nmake /I CFG=\x22Win32 Debug\x22 NO_EXTERNAL_DEPS=1" />

    <command name="log"               options="OFF" />
    <command name="process_logs"      options="move prettify index" />
    <command name="status"            options="off" />
    <command name="shell"             options="c:\\cygwin\\bin\\chmod 744 *" />
    <command name="shell"             options="c:\\cygwin\\bin\\rsync --progress --recursive --delete -z --exclude /DOC_ROOT*/ --include *.txt --include *.html --exclude ** ./ qnap.remedy.nl::Buildlogs/win7x64_msvc11" />
</autobuild>
