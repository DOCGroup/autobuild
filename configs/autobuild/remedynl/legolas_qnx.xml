<!-- $Id$ -->
<autobuild>
    <configuration>

        <environment name="ACE_ROOT"        value="/home/build/ACE/QNX/ACE_wrappers" />
        <environment name="TAO_ROOT"        value="/home/build/ACE/QNX/ACE_wrappers/TAO"/>
        <environment name="CIAO_ROOT"       value="/home/build/ACE/QNX/ACE_wrappers/TAO/CIAO"/>
        <environment name="LD_LIBRARY_PATH" value="/home/build/ACE/QNX/ACE_wrappers/lib" />
        <environment name="RSYNC_RSH"       value="ssh" />
        <environment name="PATH"            value="/opt/qnx630/host/linux/x86/usr/bin:/opt/qnx630/host/linux/x86/bin:/opt/qnx630/host/linux/x86/sbin:/opt/qnx630/host/linux/x86/usr/sbin:/opt/qnx630/host/linux/x86/usr/photon/appbuilder:/home/build/bin:/usr/local/bin:/usr/bin:/usr/X11R6/bin:/bin:/usr/games:/opt/gnome/bin:/opt/kde3/bin:/usr/lib/jvm/jre/bin:/usr/lib/mit/bin:/usr/lib/mit/sbin:/usr/lib/qt3/bin:/etc/qnx/bin" />

        <environment name="QNX_CONFIGURATION"            value="/etc/qnx" />
        <environment name="QNX_HOST"            value="/opt/qnx630/host/linux/x86" />
        <environment name="QNX_JAVAHOME"            value="/opt/qnx630/_jvm" />
        <environment name="QNX_TARGET"            value="/opt/qnx630/target/qnx6" />

        <variable name="root"     value="/home/build/ACE/QNX" />
        <variable name="log_file" value="build.txt" />
        <variable name="log_root" value="/home/build/ACE/QNX" />
        <variable name="configs"  value="Linux SSL rtems ACE_FOR_TAO" />

    </configuration>

    <command name="status"            options="on" />

    <command name="process_listener"  options="localhost:32011" />

    <command name="log"               options="on" />

    <command name="make"              options="-k realclean" />

    <command name="shell"             options="find ACE_wrappers -name 'GNUmakefile*' -exec rm '{}' ';'" />

    <command name="svn"               options="co svn://svn.dre.vanderbilt.edu/DOC/Middleware/sets-anon/ACE+TAO+CIAO ." />

    <command name="file_manipulation" options="type=create file=ACE_wrappers/ace/config.h output='#include \x22ace/config-qnx-rtp-62x.h\x22\n'"/>
    <command name="file_manipulation" options="type=create file=ACE_wrappers/include/makeinclude/platform_macros.GNU output='CROSS-COMPILE = 1\nnto_version=630\ninline=0\nCC = ntox86-gcc-3.3.5\nCXX = ntox86-gcc-3.3.5\nLD = ntox86-gcc-3.3.5\ninclude $(ACE_ROOT)/include/makeinclude/platform_qnx_rtp_gcc.GNU\n'" />
    <command name="file_manipulation" options="type=create file=ACE_wrappers/bin/MakeProjectCreator/config/default.features output=''" />

    <command name="print_os_version" />
    <command name="check_compiler" options="qcc" />
    <command name="print_ace_config" />
    <command name="print_make_version" />
    <command name="print_perl_version" />

    <command name="generate_workspace" options="-exclude TAO -type gnuace" />

    <command name="make"              options="-k" />

    <command name="log"               options="off" />
    <command name="process_logs"      options="clean move prettify index" />
    <command name="status"            options="off" />
    <command name="shell"             options="rsync --progress --recursive --delete -z --exclude /ACE_wrappers*/ --include *.txt --include *.html --exclude ** ./ johnnyw@naboo.dre.vanderbilt.edu:/web/users/remedynl/qnx" />
</autobuild>
