<!-- $Id$ -->
<autobuild>
    <configuration>
        <environment name="ACE_ROOT"         value="C:\ACE\MSVC6sr\ACE_wrappers" />
        <environment name="TAO_ROOT"         value="C:\ACE\MSVC6sr\ACE_wrappers\TAO"/>
        <environment name="Path"             value="C:\ACE\MSVC6sr\ACE_wrappers\bin;C:\ACE\MSVC6sr\ACE_wrappers\lib;C:\Perl\bin;C:\cygwin\bin;" type="prefix" />
        <environment name="CVSROOT"          value=":ext:jwillemsen@cvs.doc.wustl.edu:/project/cvs-repository" />
        <environment name="CVS_RSH"          value="c:\cygwin\bin\ssh" />
        <environment name="RSYNC_RSH"        value="C:\cygwin\bin\ssh" />

        <variable name="root"     value="C:/ACE/MSVC6sr/" />
        <variable name="log_file" value="log.txt" />
        <variable name="log_root" value="C:/ACE/MSVC6sr/" />
        <variable name="project_root" value="C:/ACE/MSVC6sr/ACE_wrappers" />
        <variable name="name"     value="WinXP LIB Release" />
        <variable name="configs"   value="Win32 SANDBOX Exceptions" />
    </configuration>
    <command name="status"            options="on" />
    <command name="file_manipulation" options="type=mustnotexist file=.disable" />
    <command name="file_manipulation" options="type=create file=.disable output='RUNNING'" />

    <command name="win32make"         options="msvc_static_compile -Release -u -k -clean -name_modifier *_Static" />

    <command name="log"               options="ON" />

    <command name="shell"             options="find ACE_wrappers -iname '*.ds[pw]' -exec rm '{}' ';'" />

    <command name="cvs"               options="-z9 -q checkout -P ACE_wrappers" />
    <command name="cvs"               options="-z9 -q checkout -P ACE_MPC"/>

    <command name="file_manipulation" options="type=create file=ACE_wrappers/ace/config.h output='#define ACE_DISABLE_WIN32_ERROR_WINDOWS\n#include \x22ace/config-win32.h\x22\n'" />
    <command name="file_manipulation" options="type=create file=ACE_wrappers/bin/MakeProjectCreator/config/default.features output='rwho=0\nqos=1\nmfc=1\n'" />

    <command name="print_os_version" />
    <command name="check_compiler" options="msvc" />
    <command name="print_ace_config" />
    <command name="print_perl_version" />

    <command name="generate_workspace" options="-exclude TAO/CIAO -type vc6 -static -recurse -name_modifier *_Static -apply_project" />

    <command name="win32make"         options="msvc_static_compile -Release -CORE -u -k -name_modifier *_Static" />
    <command name="win32make"         options="msvc_static_compile -Release -ACE -u -k -name_modifier *_Static" />
    <command name="win32make"         options="msvc_static_compile -Release -TAO -u -k -name_modifier *_Static" />

    <command name="log"               options="OFF" />
    <command name="process_logs"      options="move prettify clean index" />
    <command name="file_manipulation" options="type=delete file=.disable" />
    <command name="status"            options="off" />

    <command name="shell"             options="c:\cygwin\bin\rsync --progress --delete -z --include *.txt --include *.html --exclude * ./ willemsenj@tao.dre.vanderbilt.edu:/web/users/remedynl/MSVC6sr" />
</autobuild>
