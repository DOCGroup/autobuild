<!-- $Id$ -->
<autobuild>
    <configuration>
        <environment name="ACE_ROOT"     value="D:/MingW/ACE_wrappers" />
        <environment name="CVSROOT"      value=":ext:wustl:/project/cvs-repository" />
        <environment name="CVS_RSH"      value="plink" />
        <environment name="MINGW_BASE"   value="C:\MingW" />
        <environment name="Path"         value="c:\mingw\bin;c:\cygwin\bin;D:\MingW\ACE_wrappers\bin;D:\;c:\putty" type="prefix" />

	<environment name="BUILD"    value="all" />

        <variable name="root"     value="D:\MingW\" />
	<variable name="log_file" value="log.txt" />
        <variable name="log_root" value="D:\MingW\" />
        <variable name="name"     value="MingW GCC 3.1" />
    </configuration>

    <command name="status"            options="on" />
    <command name="file_manipulation" options="type=mustnotexist file=.disable" />
    <command name="file_manipulation" options="type=create file=.disable output='RUNNING'" />

    <command name="log"               options="ON" />

    <command name="cvs"               options="-z9 -q checkout -P ACE_wrappers" />

    <command name="file_manipulation" options="type=create file=ACE_wrappers/ace/config.h output='#define ACE_DISABLE_WIN32_ERROR_WINDOWS\n#define TAO_HAS_SMART_PROXIES 1\n#define TAO_HAS_AMI 1\n#define ACE_HAS_STANDARD_CPP_LIBRARY 1\n#define TAO_HAS_INTERFACE_REPOSITORY 1\n#define TAO_HAS_RT_CORBA 1\n#include \x22ace/config-win32.h\x22\n'" />
    <command name="file_manipulation" options="type=create file=ACE_wrappers/include/makeinclude/platform_macros.GNU output='include $(ACE_ROOT)/include/makeinclude/platform_mingw32.GNU'" />

    <command name="print_ace_config" />

    <command name="make"              options="-j 2 -k realclean" />
    <command name="make"              options="-j 2 -k -C TAO realclean" />
    <command name="make"              options="-j 2 -k" />
    <command name="make"              options="-j 2 -k -C TAO" />

    <command name="log"               options="OFF" />
    <command name="process_logs"      options="move prettify clean=1 index" />
    <command name="file_manipulation" options="type=delete file=.disable" />
    <command name="status"            options="off" />
</autobuild>




