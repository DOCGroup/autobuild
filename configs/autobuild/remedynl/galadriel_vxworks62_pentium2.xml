<!-- $Id$ -->
<autobuild>
    <configuration>
        <environment name="ACE_ROOT"                value="/nfs/vxworks/ACE/VxWorks62/ACE_wrappers"/>
        <environment name="TAO_ROOT"                value="/nfs/vxworks/ACE/VxWorks62/ACE_wrappers/TAO"/>
        <environment name="CIAO_ROOT"               value="/nfs/vxworks/ACE/VxWorks62/ACE_wrappers/TAO/CIAO"/>
        <environment name="LD_LIBRARY_PATH" value="/nfs/vxworks/ACE/VxWorks62/ACE_wrappers/lib:/home/build/WindRiver/vxworks-6.2/host/x86-linux2/lib:/home/build/WindRiver/scopetools-5.4/host/bin/x86-linux2:/home/build/WindRiver/workbench-2.4/x86-linux2/lib:/home/build/WindRiver/workbench-2.4/foundation/4.0.7/x86-linux2/lib:/usr/local/lib" />
        <environment name="PATH"            value="/home/build/WindRiver/vxworks-6.2/host/x86-linux2/bin:/home/build/WindRiver/scopetools-5.4/host/bin/x86-linux2:/home/build/WindRiver:/home/build/WindRiver/workbench-2.4/x86-linux2/bin:/home/build/WindRiver/workbench-2.4/foundation/4.0.7/x86-linux2/bin:/home/build/WindRiver/gnu/3.3.2-vxworks-6.2/x86-linux2/bin:/home/build/WindRiver/diab/5.3.1.0/LINUX386/bin" type="prefix" />
        <environment name="WIND_HOME"               value="/home/build/WindRiver" />
        <environment name="WIND_BASE"               value="/home/build/WindRiver/vxworks-6.2"/>
        <environment name="WIND_DFW_PATH"           value="/home/build/WindRiver/workbench-2.4/dfw/0128"/>
        <environment name="WIND_DIAB_PATH"          value="/home/build/WindRiver/diab/5.3.1.0"/>
        <environment name="WIND_USR"                value="/home/build/WindRiver/vxworks-6.2/target/usr"/>
        <environment name="WIND_FOUNDATION_PATH"    value="/home/build/WindRiver/workbench-2.4/foundation/4.0.6"/>
        <environment name="WIND_GNU_PATH"           value="/home/build/WindRiver/gnu/3.3.2-vxworks-6.2"/>
        <environment name="WIND_HOST_TYPE"          value="x86-linux2"/>
        <environment name="WIND_HOME"               value="/home/build/WindRiver"/>
        <environment name="WIND_TOOLS"              value="/home/build/WindRiver/workbench-2.4"/>
        <environment name="WIND_DOCS"               value="/home/build/WindRiver/docs"/>
        <environment name="WIND_DFW_PATH"           value="/home/build/WindRiver/workbench-2.4/dfw/0109h"/>
        <environment name="WIND_JRE_HOME"           value="/home/build/WindRiver/jre/1.4.2/x86-linux2"/>
        <environment name="WIND_WRWB_PATH"          value="/home/build/WindRiver/workbench-2.4/wrwb/2.4"/>
        <environment name="WIND_WRSV_PATH"          value="/home/build/WindRiver/workbench-2.4/wrsv/4.7"/>
        <environment name="WRSD_LICENSE_FILE"       value="/home/build/WindRiver/license"/>
        <environment name="WIND_SAMPLES"            value="/home/build/WindRiver/vxworks-6.2/target/usr/apps/samples:/home/build/WindRiver/vxworks-6.2/target/src/demo:/home/build/WindRiver/workbench-2.4/samples"/>
        <environment name="TCLLIBPATH"              value="/home/build/WindRiver/vxworks-6.2/host/resource/tcl"/>
        <environment name="WIND_PLATFORM"           value="vxworks-6.2"/>
        <environment name="ACE_RUN_VX_TGTHOST"      value="10.2.128.67"/>
        <environment name="ACE_RUN_VX_TGTSVR_ROOT"  value="/nfsace/vxworks/ACE/VxWorks62/ACE_wrappers"/>
        <!-- environment name="ACE_RUN_VX_NO_INITIAL_REBOOT" value="1"/ -->
        <environment name="ACE_RUN_VX_REBOOT_CMD"   value="sysToMonitor(2)"/>
        <environment name="ACE_RUN_VX_REBOOT_TIME"  value="60" />
        <environment name="ACE_RUN_VX_TGT_REBOOT"   value="1" />
        <environment name="ACE_TEST_VERBOSE"        value="1" />

        <variable name="root"     value="/nfs/vxworks/ACE/VxWorks62/" />
        <variable name="log_file" value="build.txt" />
        <variable name="log_root" value="/nfs/vxworks/ACE/VxWorks62/" />
        <variable name="configs"  value="VxWorks_RTP STATIC" />

    </configuration>

    <command name="status"            options="on" />
    <command name="file_manipulation" options="type=mustnotexist file=.disable" />
    <command name="file_manipulation" options="type=create file=.disable output='RUNNING'" />
    <command name="log"               options="on" />

    <command name="file_manipulation" options="type=create file=ACE_wrappers/ace/config.h output='#define ACE_HAS_IP_MULTICAST\n#include \x22ace/config-vxworks6.2.h\x22\n#undef ACE_HAS_SVR4_DYNAMIC_LINKING\n'"/>
    <command name="file_manipulation" options="type=create file=ACE_wrappers/include/makeinclude/platform_macros.GNU output='HOST_ROOT:=/home/build/ACE/VxWorks/host/ACE_wrappers\nstatic_libs=1\ndebug=1\ninline=0\noptimize=0\nTOOL = gnu\nTAO_IDL := $(HOST_ROOT)/bin/tao_idl\nTAO_IDL_DEP := $(TAO_IDL)\nTAO_IDLFLAGS += -g $(HOST_ROOT)/bin/gperf\nCPU = PENTIUM2\nCIDLC := $(ACE_ROOT)/TAO/CIAO/bin/cidlc\nCIDLC_DEP := $(CIDLC)\ninclude $(ACE_ROOT)/include/makeinclude/platform_vxworks6.2.GNU\n'" />
    <command name="file_manipulation" options="type=create file=ACE_wrappers/bin/MakeProjectCreator/config/default.features output='cidl=1\nxerces=0\n'" />

    <!-- command name="anonymous_shell"   options="scp -C johnnyw@naboo.dre.vanderbilt.edu:/web/users/isisbuilds/CIDLC/gnu-linux/cidlc ACE_wrappers/TAO/CIAO/bin" / -->

    <command name="print_os_version" />
    <command name="check_compiler" options="ccpentium" />
    <command name="print_ace_config" />
    <command name="print_cidlc_version" />
    <command name="print_make_version" />
    <command name="print_perl_version" />

    <command name="make"              options="-k realclean" />

    <command name="shell"             options="find ACE_wrappers -name 'GNUmakefile*' -exec rm '{}' ';'" />

    <command name="generate_workspace" options="-exclude TAO" />

    <command name="make"              options="-k -C ace" />
    <command name="make"              options="-k -C tests" />

    <command name="auto_run_tests" options="-a" />

    <command name="log"               options="off" />
    <command name="process_logs"      options="clean move prettify index" />
    <command name="file_manipulation" options="type=delete file=.disable" />
    <command name="status"            options="off" />
    <!-- command name="shell"             options="rsync --progress --recursive --delete -z --exclude /ACE_wrappers*/ --include *.txt --include *.html --exclude ** ./ johnnyw@naboo.dre.vanderbilt.edu:/web/users/remedynl/VxWorks61" / -->
</autobuild>
