    <command name="anonymous_shell"   options="pscp -p -C C:\\mySVN\\CoSMIC\\Installer\\CoSMIC_Installer\\CoSMIC-VC8-nightly-md5sum.txt bczar@naboo.dre.vanderbilt.edu:/export/web/www/cosmic/downloads/nightly/VC8"/>
-->
    <command name="log"               options="OFF" />
    <command name="process_logs"      options="move prettify clean=5 index" />
    <command name="status"            options="off" />
