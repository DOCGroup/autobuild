    <command name="anonymous_shell"   options="pscp -p -C C:\\mySVN\\VC8_Release_Log\\*.html bczar@naboo.dre.vanderbilt.edu:/web/users/isisbuilds/auto_compile_logs/prince_cosmic_release"/>
-->
</autobuild>
