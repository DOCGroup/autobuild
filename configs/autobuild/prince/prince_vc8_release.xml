<autobuild>
    <configuration>
        <environment name="ACE_ROOT"     value="C:\\mySVN\\ACE_wrappers\\ACE" />
        <environment name="TAO_ROOT"     value="C:\\mySVN\\ACE_wrappers\\TAO"/>
        <environment name="MPC_ROOT"     value="C:\\mySVN\\ACE_wrappers\\MPC"/>
        <environment name="WIX_ROOT"     value="C:\\Program Files\\Windows Installer XML v3"/>
        <environment name="CVS_RSH"      value="plink.exe" />
        <environment name="XERCESCROOT"   value="C:\\Libraries\\xerces-c-2_7_0-win32-vc8"/>
        <environment name="BOOST_ROOT"    value="C:\\Libraries\\Boost"/>
        <environment name="BOOST_VERSION" value="boost-1_32"/>
        <environment name="BOOST_CFG"     value="-vc71-mt" />
        <environment name="BOOST_STATIC_LIB_PREFIX" value="lib" />
        <environment name="UDM_PATH" 	  value="C:\\Program Files\\ISIS\\UDM" />
        <environment name="UDM_VERSION"	  value="UDMDLL_3_1_2" />
        <environment name="GME_VERSION"	  value="7_6_29" />
        <environment name="COSMIC_ROOT"  value="C:\\mySVN\\CoSMIC" />
        <environment name="ZLIB_ROOT" value="C:\\Libraries\\zlib-1.2.3" />
        <environment name="PATH"         value="C:\\WINDOWS\\system32;C:\\WINDOWS;C:\\WINDOWS\\System32\\Wbem;C:\\Libraries\\Boost\\lib;C:\\mySVN\\ACE_wrappers\\ACE\\lib;C:\\mySVN\\CoSMIC\\bin;C:\\Libraries\\xerces-c-2_7_0-win32-vc8\\bin;C:\\Program Files\\ISIS\\UDM\\bin;C:\\Program Files\\ISIS\\UDM\\etc;C:\\Program Files\\ISIS\\UDM\\3rdparty\\xalan-c_1_8_0\\bin;C:\\Perl\\site\\bin;C:\\Perl\\bin;C:\\Program Files\\ISIS\\UDM\\3rdparty\\xerces-c_2_5_0\\bin;C:\\Putty;C:\\putty\\bin;C:\\putty\\usr\\local\\wbin;C:\\Program Files\\Subversion\\bin;C:\\Program Files\\Microsoft Visual Studio 8\\Common7\\Tools\\Bin;C:\\Program Files\\Microsoft Visual Studio 8\\VC\\bin;C:\\Program Files\\Microsoft Visual Studio 8\\Common7\\IDE;C:\\Libraries\\zlib-1.2.3\\bin" />
        <variable name="configs"  value="Win32 VC8 ZLIB Exceptions" />
        <variable name="root"     value="C:\\mySVN" />
        <variable name="log_file" value="log.txt" />
        <variable name="log_root" value="C:\\mySVN\\VC8_Release_Log" />
        <variable name="project_root" value="C:\\mySVN\\ACE_wrappers\\ACE" />
        <variable name="name"     value="CoSMIC VC8 GME-7.6.29 WinXP Release" />
    </configuration>
    <command name="process_listener" options="localhost:32001" />
    <command name="status"            options="on" />

    <command name="win32make"         options="msvc_mpc_auto_compile -vc8 -dir %ACE_ROOT% -Release -k -clean" />
    <command name="win32make"         options="msvc_mpc_auto_compile -vc8 -dir %TAO_ROOT% -Release -k -clean" />
    <command name="win32make"         options="msvc_mpc_auto_compile -vc8 -dir %COSMIC_ROOT% -Release -k -clean" />
    <command name="log"               options="ON" />

    <command name="svn"               options="co svn://svn.dre.vanderbilt.edu/DOC/Middleware/trunk/ACE ACE" />
    <command name="svn"               options="co svn://svn.dre.vanderbilt.edu/DOC/Middleware/trunk/TAO TAO" />
    <command name="svn"               options="co svn://svn.dre.vanderbilt.edu/DOC/MPC/trunk MPC"/>
    <command name="svn"               options="co svn://svn.dre.vanderbilt.edu/CoSMIC/trunk/CoSMIC CoSMIC" />

    <command name="file_manipulation" options="type=create file=ACE/ace/config.h output='#define ACE_DISABLE_WIN32_ERROR_WINDOWS\n#define ACE_HAS_STANDARD_CPP_LIBRARY 1\n#include \x22ace/config-win32.h\x22\n'" />
    <command name="file_manipulation" options="type=create file=ACE/bin/MakeProjectCreator/config/default.features output='boost=1\nzlib=1\n'" />

    <command name="print_os_version" />
    <command name="check_compiler" options="msvc" />
    <command name="print_ace_config" />
    <command name="print_perl_version" />

    <command name="generate_workspace" options="-type vc8 -include %COSMIC_ROOT% %COSMIC_ROOT%\\Release.mwc"/>
    <command name="win32make"         options="msvc_mpc_auto_compile -vc8 -dir %COSMIC_ROOT%\\Release.sln -Release -k" />

    <command name="generate_workspace" options="-type vc8 -include %COSMIC_ROOT% %COSMIC_ROOT%\\Release_idl_to_picml.mwc"/>
    <command name="win32make"         options="msvc_mpc_auto_compile -vc8 -dir %COSMIC_ROOT%\\Release_idl_to_picml.sln -Release -k" />

    <command name="generate_workspace" options="-type vc8 -include %COSMIC_ROOT% %COSMIC_ROOT%\\Installer\\CoSMIC_Installer\\CoSMIC_Installer.mwc"/>
    <command name="win32make"         options="msvc_mpc_auto_compile -vc8 -dir %COSMIC_ROOT%\\Installer\\CoSMIC_Installer -Release -k" />

    <command name="anonymous_shell"   options="md5sum C:\\mySVN\\CoSMIC\\Installer\\CoSMIC_Installer\\CoSMIC_Installer.msi > C:\\mySVN\\CoSMIC\\Installer\\CoSMIC_Installer\\CoSMIC-VC8-nightly-md5sum.txt"/>
