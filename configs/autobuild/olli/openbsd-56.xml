<autobuild>
  <configuration>
    <variable name="root"         value="/home/osa/ACE-build/ATCD" />
    <variable name="project_root" value="/home/osa/ACE-build/ATCD" />
    <variable name="log_file"     value="build.txt" />
    <variable name="log_root"     value="/mnt/ACE+TAO+CIAO/buildlogs/OpenBSD-5.6" />
    <variable name="configs"      value="OpenBSD Exceptions ZLIB" />
    <variable name="make_program" value="gmake" />
  </configuration>

  <command name="status"            options="on" />
  <command name="process_listener"  options="localhost:32003" />
  <command name="log"               options="on" />
  <command name="file_manipulation" options="type=create file=ACE/ace/config.h output='#include \x22ace/config-openbsd.h\x22\n#define ACE_HAS_REACTOR_NOTIFICATION_QUEUE\n'"/>
  <command name="file_manipulation" options="type=create file=ACE/include/makeinclude/platform_macros.GNU output='debug=1\n\nathena=1\nfl=1\nfox=1\ngl=1\nmotif=1\nqt4=0\nqt=0\nrpc=0\nssl=0\ntk=1\nwxWindows=0\nx11=1\nxt=1\nzlib=1\n\nboost=1\nbzip2=1\nexceptions=1\nlzo1=1\nlzo2=1\nstl=1\n\nace_flreactor=1\nace_qtreactor=0\nace_tkreactor=1\nace_xtreactor=1\n\ntao_flresource=0\ntao_qtresource=0\ntao_tkresource=1\ntao_xtresource=1\n\nami=1\nrt_corba=1\nsmart_proxies=1\ninterceptors=1\ninterface_repo=1\ncorba_messaging=1\n\ninclude $(ACE_ROOT)/include/makeinclude/platform_openbsd.GNU\n\nLDFLAGS+=-Wl,-R$(ACE_ROOT)/lib,-R.\n'" />
  <command name="file_manipulation" options="type=create file=ACE/bin/MakeProjectCreator/config/default.features output='qt4=1\nfox=1\nmotif=1\nathena=1\nzlib=1\nwxWindows=1\nrpc=1\nbzip2=1\nlzo1=1\nlzo2=1\nboost=1\nstl=1\n'" />

  <command name="print_os_version" />
  <command name="print_env_vars" />
  <command name="check_compiler" options="gcc" />
  <command name="print_ace_config" />
  <command name="print_openssl_version"/>
  <command name="print_make_version" />
  <command name="print_perl_version" />

  <command name="generate_workspace" options="dirs=ACE -type gnuace" />

  <command name="make"               options="-k -C ACE" />
  <command name="auto_run_tests"     options="-a" />
  <command name="log"                options="off" />
  <command name="process_logs"       options="clean=7 move prettify index" />
  <command name="status"             options="off" />
</autobuild>
