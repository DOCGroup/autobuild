<autobuild>
  <configuration>
    <variable name="root"         value="/home/osa/ACE-build/LynxOS/ATCD" />
    <variable name="project_root" value="/home/osa/ACE-build/LynxOS/ATCD" />
    <variable name="log_file"     value="build.txt" />
    <variable name="log_root"     value="/mnt/ACE+TAO+CIAO/buildlogs/LynxOS-5.0-xbuild" />
    <variable name="configs"      value="LynxOS Exceptions ZLIB SSL" />
  </configuration>

  <command name="status"            options="on" />
  <command name="process_listener"  options="localhost:32006" />
  <command name="log"               options="on" />
  <command name="file_manipulation" options="type=create file=ACE/ace/config.h output='#include \x22ace/config-lynxos.h\x22\n#define ACE_HAS_REACTOR_NOTIFICATION_QUEUE\n'"/>
  <command name="file_manipulation" options="type=create file=ACE/include/makeinclude/platform_macros.GNU output='VERSION=5.0.0\nstatic_libs_only=1\noverride shared_libs=\ndebug=0\n\nathena=1\nfl=0\nfox=0\ngl=0\nmotif=0\nqt4=0\nqt=0\nrpc=0\nssl=0\ntk=0\nwxWindows=0\nx11=0\nxt=0\nzlib=1\n\nboost=0\nbzip2=0\nexceptions=1\nlzo2=0\nstl=1\n\nace_flreactor=0\nace_qtreactor=0\nace_tkreactor=0\nace_xtreactor=0\n\ntao_flresource=0\ntao_qtresource=0\ntao_tkresource=0\ntao_xtresource=0\n\nami=1\nrt_corba=1\nsmart_proxies=1\ninterceptors=1\ninterface_repo=1\ncorba_messaging=1\n\ninclude $(ACE_ROOT)/include/makeinclude/platform_lynxos.GNU\n\nLDFLAGS+=-Wl,-R$(ACE_ROOT)/lib,-R.\n\nTAO_IDL:=/home/osa/ACE-build/CentOS-5/ATCD/ACE/bin/tao_idl\nTAO_IDLFLAGS+=-g /home/osa/ACE-build/CentOS-5/ATCD/ACE/bin/ace_gperf\nTAO_IDL_DEP:=$(TAO_IDL)$(EXEEXT)\n'" />
  <command name="file_manipulation" options="type=create file=ACE/bin/MakeProjectCreator/config/default.features output='qt4=1\nfox=1\nmotif=1\nathena=1\nzlib=1\nwxWindows=1\nrpc=1\nbzip2=1\nlzo1=1\nlzo2=1\nboost=1\nstl=1\n'" />

  <command name="print_os_version" />
  <command name="print_env_vars" />
  <command name="check_compiler" options="gcc" />
  <command name="print_ace_config" />

  <command name="print_make_version" />
  <command name="print_perl_version" />

  <command name="generate_workspace" options="dirs=ACE -type gnuace" />
  <command name="generate_workspace" options="dirs=TAO -type gnuace" />

  <command name="make"               options="-k -C ACE" />
  <command name="make"               options="-k -C TAO" />

  <command name="log"                options="off" />
  <command name="process_logs"       options="clean=7 move prettify index" />
  <command name="status"             options="off" />
</autobuild>
