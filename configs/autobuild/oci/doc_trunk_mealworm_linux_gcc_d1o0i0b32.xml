<autobuild>
  <!-- The command used to create this file (on mealworm.ociweb.com) was -->
  <!-- /distcomp/nightly/BuildXML/buildxml.pl -base /tao_builds/taoadmin/doc -buildflags "debug=1 optimize=0 inline=0 buildbits=32" -checkout "svn://svn.dre.vanderbilt.edu/DOC/Middleware/branches/OCI_Asynch_IMR/ ." -compiler gcc -creator harrisb@ociweb.com -outdir . -peerlayout 1 -->
  <!-- modified for make install -->
  <configuration>
    <environment name="ACE_ROOT"        value="/tao_builds/taoadmin/doc/build/gcc_d1o0i0b32/ACE"/>
    <environment name="CIAO_ROOT"       value="/tao_builds/taoadmin/doc/build/gcc_d1o0i0b32/CIAO"/>
    <environment name="DDS_ROOT"        value="/tao_builds/taoadmin/doc/build/gcc_d1o0i0b32"/>
    <environment name="LD_LIBRARY_PATH" value="/usr/sfw/lib:/usr/local/lib:/tao_builds/taoadmin/doc/build/gcc_d1o0i0b32/ACE/lib:"/>
    <environment name="MPC_ROOT"        value="/home/taoadmin/MPC"/>
    <environment name="PATH"            value="/tao_builds/taoadmin/doc/build/gcc_d1o0i0b32/ACE/bin:/usr/local/bin:/usr/bin:/bin:/usr/sbin:/sbin:/usr/ccs/bin:"/>
    <environment name="TAO_ROOT"        value="/tao_builds/taoadmin/doc/build/gcc_d1o0i0b32/TAO"/>
    <variable    name="base"            value="doc"/>
    <variable    name="build_name"      value="gcc_d1o0i0b32"/>
    <variable    name="creator"         value="harrisb@ociweb.com"/>
    <variable    name="log_file"        value="gcc_d1o0i0b32.log"/>
    <variable    name="log_root"        value="/www-docs/autobuild_logs/doc_trunk_mealworm_linux_gcc_d1o0i0b32"/>
    <variable    name="project_root"    value="/tao_builds/taoadmin/doc/build/gcc_d1o0i0b32"/>
    <variable    name="root"            value="/tao_builds/taoadmin/doc"/>
  </configuration>

  <!-- Build preparation -->
  <command name="process_listener"   options="localhost:32001"/>
  <command name="status"             options="on"/>
  <command name="log"                options="on"/>
  <command name="svn"                options="co svn://svn.dre.vanderbilt.edu/DOC/Middleware/branches/OCI_Asynch_IMR/ ."/>
  <command name="clone_build_tree"/>
  <command name="rem_sems"/>
  <command name="file_manipulation"  options="type=update file=build/gcc_d1o0i0b32/ACE/ace/config.h output='#include \x22ace/config-linux.h\x22\n'"/>
  <command name="file_manipulation"  options="type=create file=build/gcc_d1o0i0b32/ACE/include/makeinclude/platform_macros.GNU output='debug=1\noptimize=0\ninline=0\nbuildbits=32\ninstall_rpath=0\nINSTALL_PREFIX=/tao_builds/taoadmin/doc_installed\nINSTALL_LIB=lib\n\ninclude $(ACE_ROOT)/include/makeinclude/platform_linux.GNU\nCCFLAGS += -D_FILE_OFFSET_BITS=64\nCCFLAGS += -Wno-deprecated\nCCFLAGS += -Wno-non-template-friend\nLDFLAGS += -Xlinker -zmuldefs\n'"/>
  <command name="file_manipulation"  options="type=create file=build/gcc_d1o0i0b32/ACE/setenv.sh output='ACE_ROOT=\x22/tao_builds/taoadmin/doc/build/gcc_d1o0i0b32/ACE\x22; export ACE_ROOT\nCIAO_ROOT=\x22/tao_builds/taoadmin/doc/build/gcc_d1o0i0b32/CIAO\x22; export CIAO_ROOT\nDDS_ROOT=\x22/tao_builds/taoadmin/doc/build/gcc_d1o0i0b32\x22; export DDS_ROOT\nLD_LIBRARY_PATH=\x22/usr/sfw/lib:/usr/local/lib:/tao_builds/taoadmin/doc/build/gcc_d1o0i0b32/ACE/lib:\x22; export LD_LIBRARY_PATH\nMPC_ROOT=\x22/home/taoadmin/MPC\x22; export MPC_ROOT\nPATH=\x22/tao_builds/taoadmin/doc/build/gcc_d1o0i0b32/ACE/bin:/usr/local/bin:/usr/bin:/bin:/usr/sbin:/sbin:/usr/ccs/bin:\x22; export PATH\nTAO_ROOT=\x22/tao_builds/taoadmin/doc/build/gcc_d1o0i0b32/TAO\x22; export TAO_ROOT\n'"/>
  <command name="print_os_version"/>
  <command name="print_env_vars"/>
  <command name="print_ace_config"/>
  <command name="print_make_version"/>
  <command name="check_compiler"     options="gcc"/>

  <!-- Conditionally clean binaries -->
  <command name="make" options="-s -k find=*akefile dir=TAO conditional=/tao_builds/taoadmin/doc/build/gcc_d1o0i0b32/ACE/.clean_binaries clean_binaries"/>
  <command name="make" options="   -k find=*akefile dir=ACE/bin conditional=/tao_builds/taoadmin/doc/build/gcc_d1o0i0b32/ACE/.clean_binaries clean_binaries"/>
  <command name="make" options="-s -k find=*akefile dir=ACE/tests conditional=/tao_builds/taoadmin/doc/build/gcc_d1o0i0b32/ACE/.clean_binaries clean_binaries"/>
  <command name="make" options="-s -k find=*akefile dir=ACE/protocols conditional=/tao_builds/taoadmin/doc/build/gcc_d1o0i0b32/ACE/.clean_binaries clean_binaries"/>
  <command name="make" options="-s -k find=*akefile dir=ACE/protocols/tests conditional=/tao_builds/taoadmin/doc/build/gcc_d1o0i0b32/ACE/.clean_binaries clean_binaries"/>
  <command name="make" options="-s -k find=*akefile dir=ACE/examples conditional=/tao_builds/taoadmin/doc/build/gcc_d1o0i0b32/ACE/.clean_binaries clean_binaries"/>
  <command name="make" options="-s -k find=*akefile dir=ACE/performance-tests conditional=/tao_builds/taoadmin/doc/build/gcc_d1o0i0b32/ACE/.clean_binaries clean_binaries"/>
  <command name="make" options="-s -k find=*akefile dir=TAO/tests conditional=/tao_builds/taoadmin/doc/build/gcc_d1o0i0b32/ACE/.clean_binaries clean_binaries"/>
  <command name="make" options="-s -k find=*akefile dir=TAO/examples conditional=/tao_builds/taoadmin/doc/build/gcc_d1o0i0b32/ACE/.clean_binaries clean_binaries"/>
  <command name="make" options="-s -k find=*akefile dir=TAO/performance-tests conditional=/tao_builds/taoadmin/doc/build/gcc_d1o0i0b32/ACE/.clean_binaries clean_binaries"/>
  <command name="make" options="-s -k find=*akefile dir=TAO/orbsvcs/tests conditional=/tao_builds/taoadmin/doc/build/gcc_d1o0i0b32/ACE/.clean_binaries clean_binaries"/>
  <command name="make" options="-s -k find=*akefile dir=TAO/orbsvcs/examples conditional=/tao_builds/taoadmin/doc/build/gcc_d1o0i0b32/ACE/.clean_binaries clean_binaries"/>
  <command name="make" options="-s -k find=*akefile dir=TAO/orbsvcs/performance-tests conditional=/tao_builds/taoadmin/doc/build/gcc_d1o0i0b32/ACE/.clean_binaries clean_binaries"/>
  <command name="make" options="-s -k find=*akefile dir=TAO/DevGuideExamples conditional=/tao_builds/taoadmin/doc/build/gcc_d1o0i0b32/ACE/.clean_binaries clean_binaries"/>
  <command name="make" options="-s -k find=*akefile dir=TAO/orbsvcs/DevGuideExamples conditional_rm=/tao_builds/taoadmin/doc/build/gcc_d1o0i0b32/ACE/.clean_binaries clean_binaries"/>

  <!-- Finish build preparation -->
  <command name="generate_workspace" options="-type gnuace -recurse -exclude CIAO,DAnCE,DDS,ACE/ACE.mwc,TAO/TAO.mwc"/>

  <!-- Build dependencies -->
  <command name="make" options="-s -k find=*akefile dir=TAO depend"/>
  <command name="make" options="   -k find=*akefile dir=ACE/bin depend"/>
  <command name="make" options="-s -k find=*akefile dir=ACE/tests depend"/>
  <command name="make" options="-s -k find=*akefile dir=ACE/protocols depend"/>
  <command name="make" options="-s -k find=*akefile dir=ACE/protocols/tests depend"/>
  <command name="make" options="-s -k find=*akefile dir=ACE/examples depend"/>
  <command name="make" options="-s -k find=*akefile dir=ACE/performance-tests depend"/>
  <command name="make" options="-s -k find=*akefile dir=TAO/tests depend"/>
  <command name="make" options="-s -k find=*akefile dir=TAO/examples depend"/>
  <command name="make" options="-s -k find=*akefile dir=TAO/performance-tests depend"/>
  <command name="make" options="-s -k find=*akefile dir=TAO/orbsvcs/tests depend"/>
  <command name="make" options="-s -k find=*akefile dir=TAO/orbsvcs/examples depend"/>
  <command name="make" options="-s -k find=*akefile dir=TAO/orbsvcs/performance-tests depend"/>
  <command name="make" options="-s -k find=*akefile dir=TAO/DevGuideExamples depend"/>
  <command name="make" options="-s -k find=*akefile dir=TAO/orbsvcs/DevGuideExamples depend"/>

  <!-- Build targets -->
  <command name="make" options="-s -k find=*akefile dir=TAO"/>
  <command name="make" options="   -k find=*akefile dir=ACE/bin"/>
  <command name="make" options="-s -k find=*akefile dir=ACE/tests"/>
  <command name="make" options="-s -k find=*akefile dir=ACE/protocols"/>
  <command name="make" options="-s -k find=*akefile dir=ACE/protocols/tests"/>
  <command name="make" options="-s -k find=*akefile dir=ACE/examples"/>
  <command name="make" options="-s -k find=*akefile dir=ACE/performance-tests"/>
  <command name="make" options="-s -k find=*akefile dir=TAO/tests"/>
  <command name="make" options="-s -k find=*akefile dir=TAO/examples"/>
  <command name="make" options="-s -k find=*akefile dir=TAO/performance-tests"/>
  <command name="make" options="-s -k find=*akefile dir=TAO/orbsvcs/tests"/>
  <command name="make" options="-s -k find=*akefile dir=TAO/orbsvcs/examples"/>
  <command name="make" options="-s -k find=*akefile dir=TAO/orbsvcs/performance-tests"/>
  <command name="make" options="-s -k find=*akefile dir=TAO/DevGuideExamples"/>
  <command name="make" options="-s -k find=*akefile dir=TAO/orbsvcs/DevGuideExamples"/>

  <!-- Installation -->
  <command name="anonymous_shell" options="chmod -R ug+w /tao_builds/taoadmin/doc_installed"/>
  <command name="anonymous_shell" options="rm -rf /tao_builds/taoadmin/doc_installed"/>
  <command name="make" options="-k find=*akefile dir=TAO install"/>
  <command name="anonymous_shell" options="chmod -R ug-w /tao_builds/taoadmin/doc_installed"/>

  <!-- Post build -->
  <command name="auto_run_tests"  options="dir=ACE -a -t -o -l ../TAO/DevGuideExamples/devguide_examples.lst -Config Linux"/>

  <!-- make clean workspace to tar -->
  <command name="anonymous_shell" options="rm -rf /tao_builds/taoadmin/doc/build/TAO_ACE"/>
  <command name="anonymous_shell" options="pushd /tao_builds/taoadmin/doc/; perl /home/taoadmin/MPC/clone_build_tree.pl TAO_ACE; popd"/>
  <command name="anonymous_shell" options="cp /tao_builds/taoadmin/doc/build/gcc_d1o0i0b32/ACE/ace/config.h /tao_builds/taoadmin/doc/build/TAO_ACE/ACE/ace/config.h"/>
  <command name="anonymous_shell" options="cp /tao_builds/taoadmin/doc/build/gcc_d1o0i0b32/ACE/include/makeinclude/platform_macros.GNU /tao_builds/taoadmin/doc/build/TAO_ACE/ACE/include/makeinclude/platform_macros.GNU"/>
  <command name="anonymous_shell" options="pushd /tao_builds/taoadmin/doc/build/TAO_ACE/TAO; perl ../ACE/bin/mwc.pl -type gnuace -recurse -exclude CIAO,DAnCE,DDS,ACE/ACE.mwc,TAO/TAO.mwc; popd"/>
  <command name="anonymous_shell" options="rm -rf /tao_builds/taoadmin/doc/build/TAO_ACE/doc_trunk_mealworm_linux_*"/>
  <command name="anonymous_shell" options="rm -f /www-docs/autobuild_logs/doc_trunk_mealworm_linux_gcc_d1o0i0b32_inst/for_upload/source.tar.bz2"/>
  <command name="anonymous_shell" options="mkdir -p /www-docs/autobuild_logs/doc_trunk_mealworm_linux_gcc_d1o0i0b32_inst/for_upload/"/>
  <command name="anonymous_shell" options="pushd /tao_builds/taoadmin/doc/build/; tar cjhf /www-docs/autobuild_logs/doc_trunk_mealworm_linux_gcc_d1o0i0b32_inst/for_upload/source.tar.bz2 TAO_ACE; popd"/>

  <command name="log"             options="off"/>
  <command name="process_logs"    options="copy"/>
  <command name="status"          options="off"/>
</autobuild>
