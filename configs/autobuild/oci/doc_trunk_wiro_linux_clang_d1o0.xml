<autobuild>
  <!-- The command used to create this file (on wiro) was -->
  <!-- /home/mitza/nightly/BuildXML/buildxml.pl -base /tao_builds/taoadmin/doc -checkout svn://svn.dre.vanderbilt.edu/DOC/Middleware/sets-anon/ACE+TAO -compiler clang -outdir . -->
  <configuration>
    <environment name="ACE_ROOT"        value="/tao_builds/taoadmin/doc/ACE_wrappers/build/clang_d1o0"/>
    <environment name="CIAO_ROOT"       value="/tao_builds/taoadmin/doc/ACE_wrappers/build/clang_d1o0/TAO/CIAO"/>
    <environment name="DDS_ROOT"        value="/tao_builds/taoadmin/doc/ACE_wrappers/build/clang_d1o0/TAO/DDS"/>
    <environment name="LD_LIBRARY_PATH" value="/usr/sfw/lib:/usr/local/lib:/tao_builds/taoadmin/doc/ACE_wrappers/build/clang_d1o0/lib:"/>
    <environment name="MPC_ROOT"        value="/home/taoadmin/MPC"/>
    <environment name="PATH"            value="/tao_builds/taoadmin/doc/ACE_wrappers/build/clang_d1o0/bin:/usr/local/bin:/usr/bin:/bin:/usr/sbin:/sbin:/usr/ccs/bin:"/>
    <environment name="TAO_ROOT"        value="/tao_builds/taoadmin/doc/ACE_wrappers/build/clang_d1o0/TAO"/>
    <variable    name="build_name"      value="clang_d1o0"/>
    <variable    name="creator"         value="taoadmin"/>
    <variable    name="log_file"        value="clang_d1o0.log"/>
    <variable    name="log_root"        value="/www-docs/autobuild_logs/doc_trunk_wiro_linux_clang_d1o0"/>
    <variable    name="project_root"    value="/tao_builds/taoadmin/doc/ACE_wrappers/build/clang_d1o0"/>
    <variable    name="root"            value="/tao_builds/taoadmin/doc"/>
  </configuration>

  <!-- Build preparation -->
  <command name="process_listener"   options="localhost:32001"/>
  <command name="status"             options="on"/>
  <command name="log"                options="on"/>
  <command name="svn"                options="co svn://svn.dre.vanderbilt.edu/DOC/Middleware/sets-anon/ACE+TAO ."/>
  <command name="clone_build_tree"/>
  <command name="rem_sems"/>
  <command name="file_manipulation"  options="type=update file=ACE_wrappers/build/clang_d1o0/ace/config.h output='#include \x22ace/config-linux.h\x22\n'"/>
  <command name="file_manipulation"  options="type=create file=ACE_wrappers/build/clang_d1o0/include/makeinclude/platform_macros.GNU output='debug=1\noptimize=0\n\ninclude $(ACE_ROOT)/include/makeinclude/platform_linux_clang.GNU\n'"/>
  <command name="file_manipulation"  options="type=create file=ACE_wrappers/build/clang_d1o0/setenv.sh output='ACE_ROOT=\x22/tao_builds/taoadmin/doc/ACE_wrappers/build/clang_d1o0\x22; export ACE_ROOT\nCIAO_ROOT=\x22/tao_builds/taoadmin/doc/ACE_wrappers/build/clang_d1o0/TAO/CIAO\x22; export CIAO_ROOT\nDDS_ROOT=\x22/tao_builds/taoadmin/doc/ACE_wrappers/build/clang_d1o0/TAO/DDS\x22; export DDS_ROOT\nLD_LIBRARY_PATH=\x22/usr/sfw/lib:/usr/local/lib:/tao_builds/taoadmin/doc/ACE_wrappers/build/clang_d1o0/lib:\x22; export LD_LIBRARY_PATH\nMPC_ROOT=\x22/home/taoadmin/MPC\x22; export MPC_ROOT\nPATH=\x22/tao_builds/taoadmin/doc/ACE_wrappers/build/clang_d1o0/bin:/usr/local/bin:/usr/bin:/bin:/usr/sbin:/sbin:/usr/ccs/bin:\x22; export PATH\nTAO_ROOT=\x22/tao_builds/taoadmin/doc/ACE_wrappers/build/clang_d1o0/TAO\x22; export TAO_ROOT\n'"/>
  <command name="print_os_version"/>
  <command name="print_env_vars"/>
  <command name="print_ace_config"/>
  <command name="print_make_version"/>
  <command name="check_compiler"     options="clang"/>

  <!-- Conditionally clean binaries -->
  <command name="make" options="-s -k  find=*akefile dir=ace conditional=/tao_builds/taoadmin/doc/ACE_wrappers/build/clang_d1o0/.clean_binaries clean_binaries"/>
  <command name="make" options="   -k  find=*akefile dir=bin conditional=/tao_builds/taoadmin/doc/ACE_wrappers/build/clang_d1o0/.clean_binaries clean_binaries"/>
  <command name="make" options="-s -k  find=*akefile dir=apps/gperf/src conditional=/tao_builds/taoadmin/doc/ACE_wrappers/build/clang_d1o0/.clean_binaries clean_binaries"/>
  <command name="make" options="-s -k  find=*akefile dir=ACEXML conditional=/tao_builds/taoadmin/doc/ACE_wrappers/build/clang_d1o0/.clean_binaries clean_binaries"/>
  <command name="make" options="-s -k  find=*akefile dir=Kokyu conditional=/tao_builds/taoadmin/doc/ACE_wrappers/build/clang_d1o0/.clean_binaries clean_binaries"/>
  <command name="make" options="-s -k  find=*akefile dir=tests conditional=/tao_builds/taoadmin/doc/ACE_wrappers/build/clang_d1o0/.clean_binaries clean_binaries"/>
  <command name="make" options="-s -k  find=*akefile dir=protocols conditional=/tao_builds/taoadmin/doc/ACE_wrappers/build/clang_d1o0/.clean_binaries clean_binaries"/>
  <command name="make" options="-s -k  find=*akefile dir=examples conditional=/tao_builds/taoadmin/doc/ACE_wrappers/build/clang_d1o0/.clean_binaries clean_binaries"/>
  <command name="make" options="-s -k  find=*akefile dir=performance-tests conditional=/tao_builds/taoadmin/doc/ACE_wrappers/build/clang_d1o0/.clean_binaries clean_binaries"/>

  <!-- Finish build preparation -->
  <command name="generate_workspace" options="-type gnuace ace/ace.mwc apps/gperf/src ACEXML/ACEXML.mwc Kokyu/Kokyu.mwc protocols/protocols.mwc bin tests examples performance-tests"/>

  <!-- Build dependencies -->
  <command name="make" options="-s -k  find=*akefile dir=ace depend"/>
  <command name="make" options="   -k  find=*akefile dir=bin depend"/>
  <command name="make" options="-s -k  find=*akefile dir=apps/gperf/src depend"/>
  <command name="make" options="-s -k  find=*akefile dir=ACEXML depend"/>
  <command name="make" options="-s -k  find=*akefile dir=Kokyu depend"/>
  <command name="make" options="-s -k  find=*akefile dir=tests depend"/>
  <command name="make" options="-s -k  find=*akefile dir=protocols depend"/>
  <command name="make" options="-s -k  find=*akefile dir=examples depend"/>
  <command name="make" options="-s -k  find=*akefile dir=performance-tests depend"/>

  <!-- Build targets -->
  <command name="make" options="-s -k  find=*akefile dir=ace"/>
  <command name="make" options="   -k  find=*akefile dir=bin"/>
  <command name="make" options="-s -k  find=*akefile dir=apps/gperf/src"/>
  <command name="make" options="-s -k  find=*akefile dir=ACEXML"/>
  <command name="make" options="-s -k  find=*akefile dir=Kokyu"/>
  <command name="make" options="-s -k  find=*akefile dir=tests"/>
  <command name="make" options="-s -k  find=*akefile dir=protocols"/>
  <command name="make" options="-s -k  find=*akefile dir=examples"/>
  <command name="make" options="-s -k  find=*akefile dir=performance-tests"/>

  <!-- Post build -->
  <command name="auto_run_tests"  options="-a -Config Linux"/>
  <command name="log"             options="off"/>
  <command name="process_logs"    options="copy"/>
  <command name="status"          options="off"/>
</autobuild>
