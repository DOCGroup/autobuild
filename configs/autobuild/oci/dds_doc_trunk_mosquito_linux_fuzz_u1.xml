<autobuild>
  <!-- The command used to create this file (on mosquito.ociweb.com) was -->
  <!-- /distcomp/nightly/BuildXML/buildxml.pl -autotest 0 -base /tao_builds/taoadmin/DDS -buildflags utility=1 -checkout "" -compiler fuzz -dds 1 -devguide 0 -docbuild 1 -docrepo 1 -outdir . -piggyback /tao_builds/taoadmin/doc/build/doxygen_u1 -piggypeer 1 -supporting 0 -->
  <configuration>
    <environment name="ACE_ROOT"              value="/tao_builds/taoadmin/doc/build/doxygen_u1/ACE"/>
    <environment name="DDS_ROOT"              value="/tao_builds/taoadmin/DDS/build/fuzz_u1"/>
    <variable    name="base"                  value="DDS"/>
    <variable    name="build_name"            value="fuzz_u1"/>
    <variable    name="creator"               value="mitza"/>
    <variable    name="log_file"              value="fuzz_u1.log"/>
    <variable    name="log_root"              value="/www-docs/autobuild_logs/dds_doc_trunk_mosquito_linux_fuzz_u1"/>
    <variable    name="project_root"          value="/tao_builds/taoadmin/DDS/build/fuzz_u1"/>
    <variable    name="root"                  value="/tao_builds/taoadmin/DDS"/>
  </configuration>

  <!-- Build preparation -->
  <command name="process_listener"   options="localhost:32001"/>
  <command name="status"             options="on"/>
  <command name="log"                options="on"/>
  <command name="clone_build_tree"/>
  <command name="rem_sems"/>
  <command name="file_manipulation"  options="type=create file=build/fuzz_u1/setenv.sh output='ACE_ROOT=\x22/tao_builds/taoadmin/doc/build/doxygen_u1/ACE\x22; export ACE_ROOT\nDDS_ROOT=\x22/tao_builds/taoadmin/DDS/build/fuzz_u1\x22; export DDS_ROOT\n'"/>
  <command name="print_os_version"/>
  <command name="print_env_vars"/>

  <!-- Build targets -->
  <command name="shell" options="rm -rf build/fuzz_u1/dds_* build/fuzz_u1/tools/modeling/plugins"/>
  <command name="shell" options="cd build/fuzz_u1 ; tools/scripts/dds_fuzz.pl"/>

  <!-- Post build -->
  <command name="log"             options="off"/>
  <command name="process_logs"    options="copy"/>
  <command name="status"          options="off"/>
</autobuild>
