    <environment name="DDS_ROOT"        value="/tao_builds/taoadmin/DDS/build/gcc_d0o1i0c1"/>
    <environment name="JAVA_HOME"       value="/tao_builds/taoadmin/jdk1.6.0_25"/>
    <environment name="JAVA_PLATFORM"   value="linux"/>
    <environment name="JBOSS_HOME"      value="/tao_builds/taoadmin/jboss-4.2.3.GA"/>
    <environment name="LD_LIBRARY_PATH" value="/usr/sfw/lib:/usr/local/lib:/tao_builds/taoadmin/DDS/build/gcc_d0o1i0c1/lib:/tao_builds/taoadmin/doc/build/gcc_d0o1i0c1/ACE/lib:"/>
    <environment name="MAKEFLAGS"       value="built_in_topics=0 java=1"/>
    <environment name="MPC_ROOT"        value="/home/taoadmin/MPC"/>
    <environment name="OPENDDS_RTPS_DEFAULT_D0" value="136"/>
    <environment name="PATH"            value="/tao_builds/taoadmin/jdk1.6.0_25/bin:/tao_builds/taoadmin/DDS/build/gcc_d0o1i0c1/bin:/tao_builds/taoadmin/doc/build/gcc_d0o1i0c1/ACE/bin:/usr/local/bin:/usr/bin:/bin:/usr/sbin:/sbin:/usr/ccs/bin:"/>
    <environment name="TAO_ROOT"        value="/tao_builds/taoadmin/doc/build/gcc_d0o1i0c1/TAO"/>
    <variable    name="base"            value="DDS"/>
    <variable    name="build_name"      value="gcc_d0o1i0c1"/>
    <variable    name="creator"         value="mitza@ociweb.com"/>
    <variable    name="log_file"        value="gcc_d0o1i0c1.log"/>
    <variable    name="log_root"        value="/www-docs/autobuild_logs/dds_doc_trunk_mosquito_linux_gcc_d0o1i0c1"/>
    <variable    name="project_root"    value="/tao_builds/taoadmin/DDS/build/gcc_d0o1i0c1"/>
    <variable    name="root"            value="/tao_builds/taoadmin/DDS"/>
  </configuration>

  <!-- Build preparation -->
  <command name="process_listener"   options="localhost:32001"/>
  <command name="status"             options="on"/>
  <command name="log"                options="on"/>
  <command name="svn"                options="co svn://svn.dre.vanderbilt.edu/DOC/DDS/trunk ."/>
  <command name="clone_build_tree"/>
  <command name="rem_sems"/>
  <command name="file_manipulation"  options="type=create file=build/gcc_d0o1i0c1/setenv.sh output='ACE_ROOT=\x22/tao_builds/taoadmin/doc/build/gcc_d0o1i0c1/ACE\x22; export ACE_ROOT\nANT_HOME=\x22/usr/local/ant\x22; export ANT_HOME\nCIAO_ROOT=\x22/tao_builds/taoadmin/doc/build/gcc_d0o1i0c1/CIAO\x22; export CIAO_ROOT\nDANCE_ROOT=\x22/tao_builds/taoadmin/doc/build/gcc_d0o1i0c1/DAnCE\x22; export DANCE_ROOT\nDDS_ROOT=\x22/tao_builds/taoadmin/DDS/build/gcc_d0o1i0c1\x22; export DDS_ROOT\nJAVA_HOME=\x22/tao_builds/taoadmin/jdk1.6.0_25\x22; export JAVA_HOME\nJAVA_PLATFORM=\x22linux\x22; export JAVA_PLATFORM\nJBOSS_HOME=\x22/tao_builds/taoadmin/jboss-4.2.3.GA\x22; export JBOSS_HOME\nLD_LIBRARY_PATH=\x22/usr/sfw/lib:/usr/local/lib:/tao_builds/taoadmin/DDS/build/gcc_d0o1i0c1/lib:/tao_builds/taoadmin/doc/build/gcc_d0o1i0c1/ACE/lib:\x22; export LD_LIBRARY_PATH\nMAKEFLAGS=\x22built_in_topics=0 java=1\x22; export MAKEFLAGS\nMPC_ROOT=\x22/home/taoadmin/MPC\x22; export MPC_ROOT\nPATH=\x22/tao_builds/taoadmin/jdk1.6.0_25/bin:/tao_builds/taoadmin/DDS/build/gcc_d0o1i0c1/bin:/tao_builds/taoadmin/doc/build/gcc_d0o1i0c1/ACE/bin:/usr/local/bin:/usr/bin:/bin:/usr/sbin:/sbin:/usr/ccs/bin:\x22; export PATH\nTAO_ROOT=\x22/tao_builds/taoadmin/doc/build/gcc_d0o1i0c1/TAO\x22; export TAO_ROOT\nexport OPENDDS_RTPS_DEFAULT_D0=136\n'"/>
  <command name="print_os_version"/>
  <command name="print_env_vars"/>
  <command name="print_ace_config"/>
  <command name="print_make_version"/>
  <command name="check_compiler"     options="gcc"/>
  <command name="shell"              options="ls /tao_builds/taoadmin/DDS/build/gcc_d0o1i0c1/tools/modeling/tests/*/model > /dev/null 2>&1 && find /tao_builds/taoadmin/DDS/build/gcc_d0o1i0c1/tools/modeling/tests/*/model -name '*.mpc' -exec rm {} \;"/>
  <command name="shell"              options="cd /tao_builds/taoadmin/DDS/build/gcc_d0o1i0c1/tools/modeling/tests ; ./setup.pl"/>
  <command name="generate_workspace" options="-type gnuace DDS.mwc tools/modeling/tests/modeling_tests.mwc -features built_in_topics=0,java=1"/>

  <!-- Build dependencies -->
  <command name="make" options="-s -k  find=*akefile dir=. depend"/>
  <command name="make" options="-s -k -j2 find=*akefile dir=tools/modeling/tests depend"/>

  <!-- Build targets -->
  <command name="make" options="-s -k  find=*akefile dir=."/>
  <command name="make" options="-s -k -j2 find=*akefile dir=tools/modeling/tests"/>

  <!-- Post build -->
  <command name="anonymous_shell" options="perl java/build_scripts/jms_scoreboard.pl build -Dnative.debug=false -Dmwc.features=built_in_topics=0,java=1 "/>
  <command name="auto_run_tests"  options="dir=. -Config CORBA_E_COMPACT -Config NO_BUILT_IN_TOPICS -Config Linux -a -l java/tests/dcps_java_tests.lst -l tools/modeling/tests/modeling_tests.lst -Config RTPS"/>
  <command name="anonymous_shell" options="perl java/build_scripts/jms_scoreboard.pl test -Dnative.debug=false"/>
  <command name="log"             options="off"/>
  <command name="process_logs"    options="copy"/>
  <command name="status"          options="off"/>
</autobuild>
