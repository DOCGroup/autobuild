<autobuild>
  <!-- The command used to create this file (on viper) was -->
  <!-- /distcomp/nightly/BuildXML/buildxml.pl -base /tao_builds/taoadmin/doc -buildflags "debug=1 optimize=0 templates=manual TAO_IDL_PREPROCESSOR=gcc" -checkout "svn://svn.dre.vanderbilt.edu/DOC/Middleware/sets-anon/ACE+TAO+CIAO ." -compiler va8 -creator mitza -docbuild 1 -docrepo 1 -outdir /home/taoadmin/autobuild/configs/autobuild/oci/ -sandbox 1 -->
  <configuration>
    <environment name="ACE_ROOT"     value="/tao_builds/taoadmin/doc/ACE_wrappers/build/va8_d1o0"/>
    <environment name="CIAO_ROOT"    value="/tao_builds/taoadmin/doc/ACE_wrappers/build/va8_d1o0/TAO/CIAO"/>
    <environment name="CVSROOT"      value=":pserver:taoadmin@cvs.ociweb.com:/cvs"/>
    <environment name="CVS_RSH"      value="ssh"/>
    <environment name="DDS_ROOT"     value="/tao_builds/taoadmin/doc/ACE_wrappers/build/va8_d1o0/TAO/DDS"/>
    <environment name="LIBPATH"      value="/opt/v8/usr/vacpp/lib:/usr/sfw/lib:/usr/local/lib:/tao_builds/taoadmin/doc/ACE_wrappers/build/va8_d1o0/lib:"/>
    <environment name="MPC_ROOT"     value="/home/taoadmin/MPC"/>
    <environment name="PATH"         value="/opt/v8/usr/vacpp/bin:/tao_builds/taoadmin/doc/ACE_wrappers/build/va8_d1o0/bin:/usr/local/bin:/usr/bin:/bin:/usr/sbin:/sbin:/usr/ccs/bin:"/>
    <environment name="TAO_ROOT"     value="/tao_builds/taoadmin/doc/ACE_wrappers/build/va8_d1o0/TAO"/>
    <variable    name="build_name"   value="va8_d1o0"/>
    <variable    name="creator"      value="mitza"/>
    <variable    name="log_file"     value="va8_d1o0.log"/>
    <variable    name="log_root"     value="/www-docs/autobuild_logs/doc_trunk_viper_aix53_va8_d1o0"/>
    <variable    name="project_root" value="/tao_builds/taoadmin/doc/ACE_wrappers/build/va8_d1o0"/>
    <variable    name="root"         value="/tao_builds/taoadmin/doc"/>
  </configuration>

  <!-- Build preparation -->
  <command name="process_listener"   options="localhost:32001"/>
  <command name="status"             options="on"/>
  <command name="log"                options="on"/>
  <command name="svn"                options="co svn://svn.dre.vanderbilt.edu/DOC/Middleware/sets-anon/ACE+TAO+CIAO ."/>
  <command name="clone_build_tree"/>
  <command name="rem_sems"/>
  <command name="file_manipulation"  options="type=update file=ACE_wrappers/build/va8_d1o0/ace/config.h output='#include \x22ace/config-aix-5.x.h\x22\n'"/>
  <command name="file_manipulation"  options="type=create file=ACE_wrappers/build/va8_d1o0/include/makeinclude/platform_macros.GNU output='debug=1\noptimize=0\ntemplates=manual\nTAO_IDL_PREPROCESSOR=gcc\n\ninclude $(ACE_ROOT)/include/makeinclude/platform_aix_ibm.GNU\n'"/>
  <command name="file_manipulation"  options="type=create file=ACE_wrappers/build/va8_d1o0/setenv.sh output='ACE_ROOT=\x22/tao_builds/taoadmin/doc/ACE_wrappers/build/va8_d1o0\x22; export ACE_ROOT\nCIAO_ROOT=\x22/tao_builds/taoadmin/doc/ACE_wrappers/build/va8_d1o0/TAO/CIAO\x22; export CIAO_ROOT\nCVSROOT=\x22:pserver:taoadmin@cvs.ociweb.com:/cvs\x22; export CVSROOT\nCVS_RSH=\x22ssh\x22; export CVS_RSH\nDDS_ROOT=\x22/tao_builds/taoadmin/doc/ACE_wrappers/build/va8_d1o0/TAO/DDS\x22; export DDS_ROOT\nLIBPATH=\x22/opt/v8/usr/vacpp/lib:/usr/sfw/lib:/usr/local/lib:/tao_builds/taoadmin/doc/ACE_wrappers/build/va8_d1o0/lib:\x22; export LIBPATH\nMPC_ROOT=\x22/home/taoadmin/MPC\x22; export MPC_ROOT\nPATH=\x22/opt/v8/usr/vacpp/bin:/tao_builds/taoadmin/doc/ACE_wrappers/build/va8_d1o0/bin:/usr/local/bin:/usr/bin:/bin:/usr/sbin:/sbin:/usr/ccs/bin:\x22; export PATH\nTAO_ROOT=\x22/tao_builds/taoadmin/doc/ACE_wrappers/build/va8_d1o0/TAO\x22; export TAO_ROOT\n'"/>
  <command name="print_os_version"/>
  <command name="print_env_vars"/>
  <command name="print_ace_config"/>
  <command name="print_make_version"/>

  <!-- Build the test sandbox binary -->
  <command name="file_manipulation"  options="type=copy from=<AUTOBUILD_ROOT>/posixsandbox/sandbox.cpp to=ACE_wrappers/build/va8_d1o0/bin/sandbox.cpp"/>
  <command name="file_manipulation"  options="type=create file=ACE_wrappers/build/va8_d1o0/bin/sandbox.mpc output='project {\n  Source_Files {\n    sandbox.cpp\n  }\n}\n'"/>
  <command name="anonymous_shell"    options="perl ACE_wrappers/build/va8_d1o0/bin/mpc.pl -type gnuace ACE_wrappers/build/va8_d1o0//bin/sandbox.mpc"/>
  <command name="make"               options="-s -C bin -f GNUmakefile.sandbox"/>
  <command name="file_manipulation"  options="type=delete file=ACE_wrappers/build/va8_d1o0/bin/sandbox.mpc"/>
  <command name="file_manipulation"  options="type=delete file=ACE_wrappers/build/va8_d1o0/bin/GNUmakefile.sandbox"/>


  <!-- Conditionally clean binaries -->
  <command name="make" options="-s -k  find=*akefile dir=TAO conditional=/tao_builds/taoadmin/doc/ACE_wrappers/build/va8_d1o0/.clean_binaries clean_binaries"/>
  <command name="make" options="   -k  find=*akefile dir=bin conditional=/tao_builds/taoadmin/doc/ACE_wrappers/build/va8_d1o0/.clean_binaries clean_binaries"/>
  <command name="make" options="-s -k  find=*akefile dir=tests conditional=/tao_builds/taoadmin/doc/ACE_wrappers/build/va8_d1o0/.clean_binaries clean_binaries"/>
  <command name="make" options="-s -k  find=*akefile dir=protocols conditional=/tao_builds/taoadmin/doc/ACE_wrappers/build/va8_d1o0/.clean_binaries clean_binaries"/>
  <command name="make" options="-s -k  find=*akefile dir=protocols/tests conditional=/tao_builds/taoadmin/doc/ACE_wrappers/build/va8_d1o0/.clean_binaries clean_binaries"/>
  <command name="make" options="-s -k  find=*akefile dir=examples conditional=/tao_builds/taoadmin/doc/ACE_wrappers/build/va8_d1o0/.clean_binaries clean_binaries"/>
  <command name="make" options="-s -k  find=*akefile dir=performance-tests conditional=/tao_builds/taoadmin/doc/ACE_wrappers/build/va8_d1o0/.clean_binaries clean_binaries"/>
  <command name="make" options="-s -k  find=*akefile dir=TAO/tests conditional=/tao_builds/taoadmin/doc/ACE_wrappers/build/va8_d1o0/.clean_binaries clean_binaries"/>
  <command name="make" options="-s -k  find=*akefile dir=TAO/examples conditional=/tao_builds/taoadmin/doc/ACE_wrappers/build/va8_d1o0/.clean_binaries clean_binaries"/>
  <command name="make" options="-s -k  find=*akefile dir=TAO/performance-tests conditional=/tao_builds/taoadmin/doc/ACE_wrappers/build/va8_d1o0/.clean_binaries clean_binaries"/>
  <command name="make" options="-s -k  find=*akefile dir=TAO/orbsvcs/tests conditional=/tao_builds/taoadmin/doc/ACE_wrappers/build/va8_d1o0/.clean_binaries clean_binaries"/>
  <command name="make" options="-s -k  find=*akefile dir=TAO/orbsvcs/examples conditional=/tao_builds/taoadmin/doc/ACE_wrappers/build/va8_d1o0/.clean_binaries clean_binaries"/>
  <command name="make" options="-s -k  find=*akefile dir=TAO/orbsvcs/performance-tests conditional=/tao_builds/taoadmin/doc/ACE_wrappers/build/va8_d1o0/.clean_binaries clean_binaries"/>
  <command name="make" options="-s -k  find=*akefile dir=TAO/DevGuideExamples conditional=/tao_builds/taoadmin/doc/ACE_wrappers/build/va8_d1o0/.clean_binaries clean_binaries"/>
  <command name="make" options="-s -k  find=*akefile dir=TAO/orbsvcs/DevGuideExamples conditional_rm=/tao_builds/taoadmin/doc/ACE_wrappers/build/va8_d1o0/.clean_binaries clean_binaries"/>

  <!-- Finish build preparation -->
  <command name="generate_workspace" options="-type gnuace -recurse -exclude TAO/CIAO,TAO/DDS,ACE.mwc,TAO/TAO.mwc"/>

  <!-- Build dependencies -->
  <command name="make" options="-s -k  find=*akefile dir=TAO depend"/>
  <command name="make" options="   -k  find=*akefile dir=bin depend"/>
  <command name="make" options="-s -k  find=*akefile dir=tests depend"/>
  <command name="make" options="-s -k  find=*akefile dir=protocols depend"/>
  <command name="make" options="-s -k  find=*akefile dir=protocols/tests depend"/>
  <command name="make" options="-s -k  find=*akefile dir=examples depend"/>
  <command name="make" options="-s -k  find=*akefile dir=performance-tests depend"/>
  <command name="make" options="-s -k  find=*akefile dir=TAO/tests depend"/>
  <command name="make" options="-s -k  find=*akefile dir=TAO/examples depend"/>
  <command name="make" options="-s -k  find=*akefile dir=TAO/performance-tests depend"/>
  <command name="make" options="-s -k  find=*akefile dir=TAO/orbsvcs/tests depend"/>
  <command name="make" options="-s -k  find=*akefile dir=TAO/orbsvcs/examples depend"/>
  <command name="make" options="-s -k  find=*akefile dir=TAO/orbsvcs/performance-tests depend"/>
  <command name="make" options="-s -k  find=*akefile dir=TAO/DevGuideExamples depend"/>
  <command name="make" options="-s -k  find=*akefile dir=TAO/orbsvcs/DevGuideExamples depend"/>

  <!-- Build targets -->
  <command name="make" options="-s -k  find=*akefile dir=TAO"/>
  <command name="make" options="   -k  find=*akefile dir=bin"/>
  <command name="make" options="-s -k  find=*akefile dir=tests"/>
  <command name="make" options="-s -k  find=*akefile dir=protocols"/>
  <command name="make" options="-s -k  find=*akefile dir=protocols/tests"/>
  <command name="make" options="-s -k  find=*akefile dir=examples"/>
  <command name="make" options="-s -k  find=*akefile dir=performance-tests"/>
  <command name="make" options="-s -k  find=*akefile dir=TAO/tests"/>
  <command name="make" options="-s -k  find=*akefile dir=TAO/examples"/>
  <command name="make" options="-s -k  find=*akefile dir=TAO/performance-tests"/>
  <command name="make" options="-s -k  find=*akefile dir=TAO/orbsvcs/tests"/>
  <command name="make" options="-s -k  find=*akefile dir=TAO/orbsvcs/examples"/>
  <command name="make" options="-s -k  find=*akefile dir=TAO/orbsvcs/performance-tests"/>
  <command name="make" options="-s -k  find=*akefile dir=TAO/DevGuideExamples"/>
  <command name="make" options="-s -k  find=*akefile dir=TAO/orbsvcs/DevGuideExamples"/>

  <!-- Post build -->
  <command name="auto_run_tests"  options="-a -t -o -Config SANDBOX -s \x22<project_root>/bin/sandbox 1200\x22 -Config NO_LOADAVG"/>
  <command name="log"             options="off"/>
  <command name="process_logs"    options="copy"/>
  <command name="status"          options="off"/>
</autobuild>
