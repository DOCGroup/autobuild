<autobuild>
  <configuration>
    <environment name="ACE_ROOT"        value="/tao_builds/taoadmin/doc/ACE_wrappers/build/android"/>
    <environment name="CIAO_ROOT"       value="/tao_builds/taoadmin/doc/ACE_wrappers/build/android/TAO/CIAO"/>
    <environment name="DANCE_ROOT"      value="/tao_builds/taoadmin/doc/ACE_wrappers/build/android/TAO/DAnCE"/>
    <environment name="LD_LIBRARY_PATH" value="/tao_builds/taoadmin/doc/ACE_wrappers/build/android/lib:/tao_builds/taoadmin/doc/ACE_wrappers/build/arch/lib/i686:/usr/sfw/lib:/usr/local/lib:"/>
    <environment name="MPC_ROOT"        value="/home/taoadmin/MPC"/>
    <environment name="TAO_ROOT"        value="/tao_builds/taoadmin/doc/ACE_wrappers/build/android/TAO"/>
    <environment name="SDK"             value="/tao_builds/android/android-sdk-linux_x86"/>
    <environment name="NDK"             value="/tao_builds/android/android-ndk-r6"/>
    <environment name="NDK_STANDALONE"  value="/tao_builds/android/arm_platform9"/>
    <environment name="PATH"            value="/tao_builds/taoadmin/doc/ACE_wrappers/build/android/bin:/tao_builds/android/arm_platform9/bin:/tao_builds/android/android-sdk-linux_x86/tools:/tao_builds/android/android-sdk-linux_x86/platform-tools:/usr/local/bin:/usr/bin:/bin:/usr/sbin:/sbin:/usr/ccs/bin:"/>
    <environment name="HOST_ROOT"        value="/tao_builds/taoadmin/doc/ACE_wrappers/build/arch"/>

    <variable    name="build_name"      value="android"/>
    <variable    name="creator"         value="fieldst"/>
    <variable    name="log_file"        value="android.log"/>
    <variable    name="log_root"        value="/www-docs/autobuild_logs/doc_trunk_tick_android_gcc_d0o1"/>
    <variable    name="project_root"    value="/tao_builds/taoadmin/doc/ACE_wrappers/build/android"/>
    <variable    name="root"            value="/tao_builds/taoadmin/doc"/>
  </configuration>

  <!-- Build preparation -->
  <command name="process_listener"   options="localhost:32001"/>
  <command name="status"             options="on"/>
  <command name="log"                options="on"/>
<!-- Use the same as the doc_trunk_tick_linux_gcc_d0o1b32 build 
  <command name="svn"                options="co svn://svn.dre.vanderbilt.edu/DOC/Middleware/sets-anon/ACE+TAO+CIAO ."/>
-->
  <command name="clone_build_tree"/>
  <command name="file_manipulation"  options="type=update file=ACE_wrappers/build/android/ace/config.h output='#include \x22ace/config-android.h\x22\n'"/>
  <command name="file_manipulation"  options="type=create file=ACE_wrappers/build/android/include/makeinclude/platform_macros.GNU output='debug=0\noptimize=1\nCROSS-COMPILE=1\nCROSS_COMPILE = arm-linux-androideabi-\ninclude $(ACE_ROOT)/include/makeinclude/platform_android_linux.GNU\nTAO_IDL := $(HOST_ROOT)/bin/i686/tao_idl\nTAO_IDLFLAGS += -g $(HOST_ROOT)/bin/i686/ace_gperf\nTAO_IDL_DEP := $(HOST_ROOT)/bin/i686/tao_idl$(EXEEXT)\n'"/>
  <command name="file_manipulation"  options="type=create file=ACE_wrappers/build/android/setenv.sh output='ACE_ROOT=\x22/tao_builds/taoadmin/doc/ACE_wrappers/build/android\x22; export ACE_ROOT\nCIAO_ROOT=\x22/tao_builds/taoadmin/doc/ACE_wrappers/build/android/TAO/CIAO\x22; export CIAO_ROOT\nDANCE_ROOT=\x22/tao_builds/taoadmin/doc/ACE_wrappers/build/android/TAO/DAnCE\x22; export DANCE_ROOT\nLD_LIBRARY_PATH=\x22/tao_builds/taoadmin/doc/ACE_wrappers/build/android/lib:/tao_builds/taoadmin/doc/ACE_wrappers/build/arch/lib/i686:/usr/sfw/lib:/usr/local/lib:\x22; export LD_LIBRARY_PATH\nMPC_ROOT=\x22/home/taoadmin/MPC\x22; export MPC_ROOT\nPATH=\x22/tao_builds/taoadmin/doc/ACE_wrappers/build/android/bin:/tao_builds/android/arm_platform9/bin:/tao_builds/android/android-sdk-linux_x86/tools:/tao_builds/android/android-sdk-linux_x86/platform-tools:/usr/local/bin:/usr/bin:/bin:/usr/sbin:/sbin:/usr/ccs/bin:\x22; export PATH\nexport QTDIR=/usr/lib/qt4\nTAO_ROOT=\x22/tao_builds/taoadmin/doc/ACE_wrappers/build/android/TAO\x22; export TAO_ROOT\n'"/>
  <command name="print_os_version"/>
  <command name="print_env_vars"/>
  <command name="print_openssl_version"/>
  <command name="print_ace_config"/>
  <command name="print_make_version"/>
  <command name="check_compiler"     options="gcc"/>

  <command name="anonymous_shell"    options="/tao_builds/android/android-ndk-r6/build/tools/make-standalone-toolchain.sh --platform=android-9 --install-dir=/tao_builds/android/arm_platform9"/>

  <!-- Conditionally clean binaries -->
  <command name="make" options="-s -k -j2 find=*akefile dir=TAO conditional=/tao_builds/taoadmin/doc/ACE_wrappers/build/android/.clean_binaries clean_binaries"/>
  <command name="make" options="   -k -j2 find=*akefile dir=bin conditional=/tao_builds/taoadmin/doc/ACE_wrappers/build/android/.clean_binaries clean_binaries"/>
  <command name="make" options="-s -k -j2 find=*akefile dir=tests conditional=/tao_builds/taoadmin/doc/ACE_wrappers/build/android/.clean_binaries clean_binaries"/>
  <command name="make" options="-s -k -j2 find=*akefile dir=protocols conditional=/tao_builds/taoadmin/doc/ACE_wrappers/build/android/.clean_binaries clean_binaries"/>
  <command name="make" options="-s -k -j2 find=*akefile dir=protocols/tests conditional=/tao_builds/taoadmin/doc/ACE_wrappers/build/android/.clean_binaries clean_binaries"/>
  <command name="make" options="-s -k -j2 find=*akefile dir=examples conditional=/tao_builds/taoadmin/doc/ACE_wrappers/build/android/.clean_binaries clean_binaries"/>
  <command name="make" options="-s -k -j2 find=*akefile dir=performance-tests conditional=/tao_builds/taoadmin/doc/ACE_wrappers/build/android/.clean_binaries clean_binaries"/>
  <command name="make" options="-s -k -j2 find=*akefile dir=TAO/tests conditional=/tao_builds/taoadmin/doc/ACE_wrappers/build/android/.clean_binaries clean_binaries"/>
  <command name="make" options="-s -k -j2 find=*akefile dir=TAO/examples conditional=/tao_builds/taoadmin/doc/ACE_wrappers/build/android/.clean_binaries clean_binaries"/>
  <command name="make" options="-s -k -j2 find=*akefile dir=TAO/performance-tests conditional=/tao_builds/taoadmin/doc/ACE_wrappers/build/android/.clean_binaries clean_binaries"/>
  <command name="make" options="-s -k -j2 find=*akefile dir=TAO/orbsvcs/tests conditional=/tao_builds/taoadmin/doc/ACE_wrappers/build/android/.clean_binaries clean_binaries"/>
  <command name="make" options="-s -k -j2 find=*akefile dir=TAO/orbsvcs/examples conditional=/tao_builds/taoadmin/doc/ACE_wrappers/build/android/.clean_binaries clean_binaries"/>
  <command name="make" options="-s -k -j2 find=*akefile dir=TAO/orbsvcs/performance-tests conditional=/tao_builds/taoadmin/doc/ACE_wrappers/build/android/.clean_binaries clean_binaries"/>
  <command name="make" options="-s -k -j2 find=*akefile dir=TAO/DAnCE conditional_rm=/tao_builds/taoadmin/doc/ACE_wrappers/build/android/.clean_binaries clean_binaries"/>
  <command name="make" options="-s -k -j2 find=*akefile dir=TAO/CIAO conditional=/tao_builds/taoadmin/doc/ACE_wrappers/build/android/.clean_binaries clean_binaries"/>
  <command name="make" options="-s -k -j2 find=*akefile dir=TAO/DevGuideExamples conditional=/tao_builds/taoadmin/doc/ACE_wrappers/build/android/.clean_binaries clean_binaries"/>
  <command name="make" options="-s -k -j2 find=*akefile dir=TAO/orbsvcs/DevGuideExamples conditional_rm=/tao_builds/taoadmin/doc/ACE_wrappers/build/android/.clean_binaries clean_binaries"/>

  <!-- Finish build preparation -->
  <command name="generate_workspace" options="-type gnuace -recurse -exclude TAO/CIAO,TAO/DAnCE,ACE.mwc,TAO/TAO.mwc"/>
  <command name="generate_workspace" options="-type gnuace TAO/DAnCE"/>
  <command name="generate_workspace" options="-type gnuace TAO/CIAO/CIAO.mwc"/>

  <!-- Build dependencies -->
  <command name="make" options="-s -k -j2 find=*akefile dir=TAO depend"/>
  <command name="make" options="   -k -j2 find=*akefile dir=bin depend"/>
  <command name="make" options="-s -k -j2 find=*akefile dir=tests depend"/>
  <command name="make" options="-s -k -j2 find=*akefile dir=protocols depend"/>
  <command name="make" options="-s -k -j2 find=*akefile dir=protocols/tests depend"/>
  <command name="make" options="-s -k -j2 find=*akefile dir=examples depend"/>
  <command name="make" options="-s -k -j2 find=*akefile dir=performance-tests depend"/>
  <command name="make" options="-s -k -j2 find=*akefile dir=TAO/tests depend"/>
  <command name="make" options="-s -k -j2 find=*akefile dir=TAO/examples depend"/>
  <command name="make" options="-s -k -j2 find=*akefile dir=TAO/performance-tests depend"/>
  <command name="make" options="-s -k -j2 find=*akefile dir=TAO/orbsvcs/tests depend"/>
  <command name="make" options="-s -k -j2 find=*akefile dir=TAO/orbsvcs/examples depend"/>
  <command name="make" options="-s -k -j2 find=*akefile dir=TAO/orbsvcs/performance-tests depend"/>
  <command name="make" options="-s -k -j2 find=*akefile dir=TAO/DAnCE depend"/>
  <command name="make" options="-s -k -j2 find=*akefile dir=TAO/CIAO depend"/>
  <command name="make" options="-s -k -j2 find=*akefile dir=TAO/DevGuideExamples depend"/>
  <command name="make" options="-s -k -j2 find=*akefile dir=TAO/orbsvcs/DevGuideExamples depend"/>

  <!-- Build targets -->
  <command name="make" options="-s -k  find=*akefile dir=TAO"/>
  <command name="make" options="   -k  find=*akefile dir=bin"/>
  <command name="make" options="-s -k  find=*akefile dir=tests"/>
  <command name="make" options="-s -k  find=*akefile dir=protocols"/>
  <command name="make" options="-s -k  find=*akefile dir=protocols/tests"/>
  <command name="make" options="-s -k  find=*akefile dir=examples"/>
  <command name="make" options="-s -k  find=*akefile dir=performance-tests"/>
  <command name="make" options="-s -k  find=*akefile dir=TAO/tests"/>
  <command name="make" options="-s -k  find=*akefile dir=TAO/examples"/>
  <command name="make" options="-s -k  find=*akefile dir=TAO/performance-tests"/>
  <command name="make" options="-s -k  find=*akefile dir=TAO/orbsvcs/tests"/>
  <command name="make" options="-s -k  find=*akefile dir=TAO/orbsvcs/examples"/>
  <command name="make" options="-s -k  find=*akefile dir=TAO/orbsvcs/performance-tests"/>
  <command name="make" options="-s -k  find=*akefile dir=TAO/DAnCE"/>
  <command name="make" options="-s -k  find=*akefile dir=TAO/CIAO"/>
  <command name="make" options="-s -k  find=*akefile dir=TAO/DevGuideExamples"/>
  <command name="make" options="-s -k  find=*akefile dir=TAO/orbsvcs/DevGuideExamples"/>


  <!-- Post build -->
  <command name="log"             options="off"/>
  <command name="process_logs"    options="copy"/>
  <command name="status"          options="off"/>
</autobuild>
