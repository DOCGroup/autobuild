<autobuild>
  <!-- The command used to create this file (on mosquito.ociweb.com) was -->
  <!-- ../../../../oci/BuildXML/buildxml.pl -base /tao_builds/taoadmin/doc -checkout "svn://svn.dre.vanderbilt.edu/DOC/Middleware/sets-anon/ACE+TAO ." -compiler suncc58 -docbuild 1 -docrepo 1 -hostname fly -outdir ./ -platform sunos59 -->
  <configuration>
    <environment name="ACE_ROOT"        value="/tao_builds/taoadmin/doc/ACE_TAO/ACE"/>
    <environment name="LD_LIBRARY_PATH" value="/opt/compilers/suncc5.8/lib:/usr/sfw/lib:/usr/local/lib:/tao_builds/taoadmin/doc/ACE_TAO/ACE/lib:"/>
    <environment name="MPC_ROOT"        value="/home/taoadmin/MPC"/>
    <environment name="PATH"            value="/opt/compilers/suncc5.8/bin:/tao_builds/taoadmin/doc/ACE_TAO/ACE/bin:/usr/local/bin:/usr/bin:/bin:/usr/sbin:/sbin:/usr/ccs/bin:"/>
    <environment name="TAO_ROOT"        value="/tao_builds/taoadmin/doc/ACE_TAO/TAO"/>
    <variable    name="build_name"      value="suncc58_d1o0"/>
    <variable    name="creator"         value="johnc"/>
    <variable    name="log_file"        value="suncc58_d1o0.log"/>
    <variable    name="log_root"        value="/www-docs/autobuild_logs/doc_master_fly_sunos59_suncc58_d1o0"/>
    <variable    name="project_root"    value="/tao_builds/taoadmin/doc/ACE_TAO"/>
    <variable    name="root"            value="/tao_builds/taoadmin/doc"/>
  </configuration>

  <!-- Build preparation -->
  <command name="process_listener"   options="localhost:32001"/>
  <command name="status"             options="on"/>
  <command name="log"                options="on"/>
  <command name="git"                options="clone git://git.ociweb.com/git/DOCGroup/ACE_TAO.git --depth 1"/>
  <command name="anonymous_shell"    options="cd ACE_TAO && git remote -v"/>
  <command name="anonymous_shell"    options="cd ACE_TAO && git log -1"/>
  <command name="rem_sems"/>
  <command name="file_manipulation"  options="type=update file=ACE_TAO/ACE/ace/config.h output='#include \x22ace/config-sunos5.9.h\x22\n'"/>
  <command name="file_manipulation"  options="type=create file=ACE_TAO/ACE/include/makeinclude/platform_macros.GNU output='debug=1\noptimize=0\n\ninclude $(ACE_ROOT)/include/makeinclude/platform_sunos5_sunc++.GNU\nCFLAGS  += -xtarget=generic\nCCFLAGS += -xtarget=generic\nLDFLAGS += -xtarget=generic\n'"/>
  <command name="file_manipulation"  options="type=create file=ACE_TAO/setenv.sh output='ACE_ROOT=\x22/tao_builds/taoadmin/doc/ACE_TAO/ACE\x22; export ACE_ROOT\nLD_LIBRARY_PATH=\x22/opt/compilers/suncc5.8/lib:/usr/sfw/lib:/usr/local/lib:/tao_builds/taoadmin/doc/ACE_TAO/ACE/lib:\x22; export LD_LIBRARY_PATH\nMPC_ROOT=\x22/home/taoadmin/MPC\x22; export MPC_ROOT\nPATH=\x22/opt/compilers/suncc5.8/bin:/tao_builds/taoadmin/doc/ACE_TAO/ACE/bin:/usr/local/bin:/usr/bin:/bin:/usr/sbin:/sbin:/usr/ccs/bin:\x22; export PATH\nTAO_ROOT=\x22/tao_builds/taoadmin/doc/ACE_TAO/TAO\x22; export TAO_ROOT\n'"/>
  <command name="print_os_version"/>
  <command name="print_env_vars"/>
  <command name="print_ace_config"/>
  <command name="print_make_version"/>
  <command name="check_compiler"     options="sun_cc"/>

  <!-- Conditionally clean binaries -->
  <command name="make" options="-s -k  find=*akefile dir=TAO conditional=/tao_builds/taoadmin/doc/ACE_TAO/ACE/.clean_binaries clean_binaries"/>
  <command name="make" options="   -k  find=*akefile dir=ACE/bin conditional=/tao_builds/taoadmin/doc/ACE_TAO/ACE/.clean_binaries clean_binaries"/>
  <command name="make" options="-s -k  find=*akefile dir=ACE/tests conditional=/tao_builds/taoadmin/doc/ACE_TAO/ACE/.clean_binaries clean_binaries"/>
  <command name="make" options="-s -k  find=*akefile dir=ACE/protocols conditional=/tao_builds/taoadmin/doc/ACE_TAO/ACE/.clean_binaries clean_binaries"/>
  <command name="make" options="-s -k  find=*akefile dir=ACE/protocols/tests conditional=/tao_builds/taoadmin/doc/ACE_TAO/ACE/.clean_binaries clean_binaries"/>
  <command name="make" options="-s -k  find=*akefile dir=ACE/examples conditional=/tao_builds/taoadmin/doc/ACE_TAO/ACE/.clean_binaries clean_binaries"/>
  <command name="make" options="-s -k  find=*akefile dir=ACE/performance-tests conditional=/tao_builds/taoadmin/doc/ACE_TAO/ACE/.clean_binaries clean_binaries"/>
  <command name="make" options="-s -k  find=*akefile dir=TAO/tests conditional=/tao_builds/taoadmin/doc/ACE_TAO/ACE/.clean_binaries clean_binaries"/>
  <command name="make" options="-s -k  find=*akefile dir=TAO/examples conditional=/tao_builds/taoadmin/doc/ACE_TAO/ACE/.clean_binaries clean_binaries"/>
  <command name="make" options="-s -k  find=*akefile dir=TAO/performance-tests conditional=/tao_builds/taoadmin/doc/ACE_TAO/ACE/.clean_binaries clean_binaries"/>
  <command name="make" options="-s -k  find=*akefile dir=TAO/orbsvcs/tests conditional=/tao_builds/taoadmin/doc/ACE_TAO/ACE/.clean_binaries clean_binaries"/>
  <command name="make" options="-s -k  find=*akefile dir=TAO/orbsvcs/examples conditional=/tao_builds/taoadmin/doc/ACE_TAO/ACE/.clean_binaries clean_binaries"/>
  <command name="make" options="-s -k  find=*akefile dir=TAO/orbsvcs/performance-tests conditional=/tao_builds/taoadmin/doc/ACE_TAO/ACE/.clean_binaries clean_binaries"/>
  <command name="make" options="-s -k  find=*akefile dir=TAO/DevGuideExamples conditional=/tao_builds/taoadmin/doc/ACE_TAO/ACE/.clean_binaries clean_binaries"/>
  <command name="make" options="-s -k  find=*akefile dir=TAO/orbsvcs/DevGuideExamples conditional_rm=/tao_builds/taoadmin/doc/ACE_TAO/ACE/.clean_binaries clean_binaries"/>

  <!-- Finish build preparation -->
  <command name="generate_workspace" options="-type gnuace -recurse -exclude CIAO,DAnCE,ACE/ACE.mwc,TAO/TAO.mwc"/>

  <!-- Build dependencies -->
  <command name="make" options="-s -k  find=*akefile dir=TAO depend"/>
  <command name="make" options="   -k  find=*akefile dir=ACE/bin depend"/>
  <command name="make" options="-s -k  find=*akefile dir=ACE/tests depend"/>
  <command name="make" options="-s -k  find=*akefile dir=ACE/protocols depend"/>
  <command name="make" options="-s -k  find=*akefile dir=ACE/protocols/tests depend"/>
  <command name="make" options="-s -k  find=*akefile dir=ACE/examples depend"/>
  <command name="make" options="-s -k  find=*akefile dir=ACE/performance-tests depend"/>
  <command name="make" options="-s -k  find=*akefile dir=TAO/tests depend"/>
  <command name="make" options="-s -k  find=*akefile dir=TAO/examples depend"/>
  <command name="make" options="-s -k  find=*akefile dir=TAO/performance-tests depend"/>
  <command name="make" options="-s -k  find=*akefile dir=TAO/orbsvcs/tests depend"/>
  <command name="make" options="-s -k  find=*akefile dir=TAO/orbsvcs/examples depend"/>
  <command name="make" options="-s -k  find=*akefile dir=TAO/orbsvcs/performance-tests depend"/>
  <command name="make" options="-s -k  find=*akefile dir=TAO/DevGuideExamples depend"/>
  <command name="make" options="-s -k  find=*akefile dir=TAO/orbsvcs/DevGuideExamples depend"/>

  <!-- Build targets -->
  <command name="make" options="-s -k  find=*akefile dir=TAO"/>
  <command name="make" options="   -k  find=*akefile dir=ACE/bin"/>
  <command name="make" options="-s -k  find=*akefile dir=ACE/tests"/>
  <command name="make" options="-s -k  find=*akefile dir=ACE/protocols"/>
  <command name="make" options="-s -k  find=*akefile dir=ACE/protocols/tests"/>
  <command name="make" options="-s -k  find=*akefile dir=ACE/examples"/>
  <command name="make" options="-s -k  find=*akefile dir=ACE/performance-tests"/>
  <command name="make" options="-s -k  find=*akefile dir=TAO/tests"/>
  <command name="make" options="-s -k  find=*akefile dir=TAO/examples"/>
  <command name="make" options="-s -k  find=*akefile dir=TAO/performance-tests"/>
  <command name="make" options="-s -k  find=*akefile dir=TAO/orbsvcs/tests"/>
  <command name="make" options="-s -k  find=*akefile dir=TAO/orbsvcs/examples"/>
  <command name="make" options="-s -k  find=*akefile dir=TAO/orbsvcs/performance-tests"/>
  <command name="make" options="-s -k  find=*akefile dir=TAO/DevGuideExamples"/>
  <command name="make" options="-s -k  find=*akefile dir=TAO/orbsvcs/DevGuideExamples"/>

  <!-- Post build -->
  <command name="auto_run_tests"  options="-a -t -o -Config NO_LOOPBACK_MCAST"/>
  <command name="log"             options="off"/>
  <command name="process_logs"    options="copy"/>
  <command name="status"          options="off"/>
</autobuild>
