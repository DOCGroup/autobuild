<autobuild>
  <!-- The command used to create this file (on pillbug) was -->
  <!-- ../../../../oci/BuildXML/buildxml.pl -base /tao_builds/taoadmin/doc -checkout "svn://svn.dre.vanderbilt.edu/DOC/Middleware/trunk/ACE . dir=ACE" -checkout "svn://svn.dre.vanderbilt.edu/DOC/Middleware/trunk/TAO . dir=TAO" -compiler aCC -creator johnc@ociweb.com -devguide 0 -docbuild 1 -docrepo 1 -outdir . -peerlayout 1 -supporting 0 -->
  <configuration>
    <environment name="ACE_ROOT"        value="/tao_builds/taoadmin/doc/build/aCC_d1o0/ACE"/>
    <environment name="CIAO_ROOT"       value="/tao_builds/taoadmin/doc/build/aCC_d1o0/CIAO"/>
    <environment name="CVSROOT"         value=":pserver:taoadmin@cvs.ociweb.com:/cvs"/>
    <environment name="CVS_RSH"         value="ssh"/>
    <environment name="DDS_ROOT"        value="/tao_builds/taoadmin/doc/build/aCC_d1o0"/>
    <environment name="LD_LIBRARY_PATH" value="/opt/aCC/lib:/opt/langtools/lib:/usr/sfw/lib:/usr/local/lib:/tao_builds/taoadmin/doc/build/aCC_d1o0/ACE/lib:"/>
    <environment name="MPC_ROOT"        value="/home/taoadmin/MPC"/>
    <environment name="PATH"            value="/opt/aCC/bin:/opt/langtools/bin:/tao_builds/taoadmin/doc/build/aCC_d1o0/ACE/bin:/usr/local/bin:/usr/bin:/bin:/usr/sbin:/sbin:/usr/ccs/bin:"/>
    <environment name="SHLIB_PATH"      value="/opt/aCC/lib:/opt/langtools/lib:/usr/sfw/lib:/usr/local/lib:/tao_builds/taoadmin/doc/build/aCC_d1o0/ACE/lib:"/>
    <environment name="TAO_ROOT"        value="/tao_builds/taoadmin/doc/build/aCC_d1o0/TAO"/>
    <variable    name="base"            value="doc"/>
    <variable    name="build_name"      value="aCC_d1o0"/>
    <variable    name="creator"         value="johnc@ociweb.com"/>
    <variable    name="log_file"        value="aCC_d1o0.log"/>
    <variable    name="log_root"        value="/www-docs/autobuild_logs/doc_trunk_pillbug_hpux11_aCC_d1o0"/>
    <variable    name="project_root"    value="/tao_builds/taoadmin/doc/build/aCC_d1o0"/>
    <variable    name="root"            value="/tao_builds/taoadmin/doc"/>
  </configuration>

  <!-- Build preparation -->
  <command name="process_listener"   options="localhost:32001"/>
  <command name="status"             options="on"/>
  <command name="log"                options="on"/>
  <command name="svn"                options="co svn://svn.dre.vanderbilt.edu/DOC/Middleware/trunk/ACE . dir=ACE"/>
  <command name="svn"                options="co svn://svn.dre.vanderbilt.edu/DOC/Middleware/trunk/TAO . dir=TAO"/>
  <command name="clone_build_tree"/>
  <command name="rem_sems"/>
  <command name="file_manipulation"  options="type=update file=build/aCC_d1o0/ACE/ace/config.h output='#include \x22ace/config-hpux-11.00.h\x22\n'"/>
  <command name="file_manipulation"  options="type=create file=build/aCC_d1o0/ACE/include/makeinclude/platform_macros.GNU output='debug=1\noptimize=0\n\ninclude $(ACE_ROOT)/include/makeinclude/platform_hpux_aCC.GNU\n'"/>
  <command name="file_manipulation"  options="type=create file=build/aCC_d1o0/ACE/setenv.sh output='ACE_ROOT=\x22/tao_builds/taoadmin/doc/build/aCC_d1o0/ACE\x22; export ACE_ROOT\nCIAO_ROOT=\x22/tao_builds/taoadmin/doc/build/aCC_d1o0/CIAO\x22; export CIAO_ROOT\nCVSROOT=\x22:pserver:taoadmin@cvs.ociweb.com:/cvs\x22; export CVSROOT\nCVS_RSH=\x22ssh\x22; export CVS_RSH\nDDS_ROOT=\x22/tao_builds/taoadmin/doc/build/aCC_d1o0\x22; export DDS_ROOT\nLD_LIBRARY_PATH=\x22/opt/aCC/lib:/opt/langtools/lib:/usr/sfw/lib:/usr/local/lib:/tao_builds/taoadmin/doc/build/aCC_d1o0/ACE/lib:\x22; export LD_LIBRARY_PATH\nMPC_ROOT=\x22/home/taoadmin/MPC\x22; export MPC_ROOT\nPATH=\x22/opt/aCC/bin:/opt/langtools/bin:/tao_builds/taoadmin/doc/build/aCC_d1o0/ACE/bin:/usr/local/bin:/usr/bin:/bin:/usr/sbin:/sbin:/usr/ccs/bin:\x22; export PATH\nSHLIB_PATH=\x22/opt/aCC/lib:/opt/langtools/lib:/usr/sfw/lib:/usr/local/lib:/tao_builds/taoadmin/doc/build/aCC_d1o0/ACE/lib:\x22; export SHLIB_PATH\nTAO_ROOT=\x22/tao_builds/taoadmin/doc/build/aCC_d1o0/TAO\x22; export TAO_ROOT\n'"/>
  <command name="print_os_version"/>
  <command name="print_ace_config"/>
  <command name="print_make_version"/>
  <command name="check_compiler"     options="acc"/>

  <!-- Conditionally clean binaries -->
  <command name="make" options="-s -k -j2 find=*akefile dir=TAO conditional=/tao_builds/taoadmin/doc/build/aCC_d1o0/ACE/.clean_binaries clean_binaries"/>
  <command name="make" options="   -k -j2 find=*akefile dir=ACE/bin conditional_rm=/tao_builds/taoadmin/doc/build/aCC_d1o0/ACE/.clean_binaries clean_binaries"/>

  <!-- Finish build preparation -->
  <command name="generate_workspace" options="-type gnuace -recurse -exclude CIAO,DDS,ACE/ACE.mwc,TAO/TAO.mwc"/>

  <!-- Build dependencies -->
  <command name="make" options="-s -k -j2 find=*akefile dir=TAO depend"/>
  <command name="make" options="   -k -j2 find=*akefile dir=ACE/bin depend"/>

  <!-- Build targets -->
  <command name="make" options="-s -k -j2 find=*akefile dir=TAO"/>
  <command name="make" options="   -k -j2 find=*akefile dir=ACE/bin"/>

  <!-- Post build -->
  <command name="log"             options="off"/>
  <command name="process_logs"    options="copy"/>
  <command name="status"          options="off"/>
</autobuild>
