<autobuild>
    <configuration>
        <environment name="ACE_ROOT"           value="/tao_builds/taoadmin/DOC_pmb/ACE_wrappers/build/doc_pmb_branch_paris_rh72_gcc2.95.3_d0o1" />
        <environment name="LD_LIBRARY_PATH"    value="/tao_builds/taoadmin/DOC_pmb/ACE_wrappers/build/doc_pmb_branch_paris_rh72_gcc2.95.3_d0o1/ace:/opt/gcc/2.95.3/lib:" />
        <environment name="LIBPATH"            value="/tao_builds/taoadmin/DOC_pmb/ACE_wrappers/build/doc_pmb_branch_paris_rh72_gcc2.95.3_d0o1/ace:/opt/gcc/2.95.3/lib:" />
        <environment name="TAO_ORBSVCS"        value="" />
        <environment name="SELECTION_CRITERIA" value=" -Config Linux  -Config STATIC " />
        <environment name="PATH"               value="/opt/gcc/2.95.3/bin:/usr/local/bin:/usr/bin:/bin" />
        <environment name="LM_LICENSE_FILE"    value="" />

        <!-- Set the cvsroot value -->
        <environment name="CVSROOT"            value=":ext:oci@cvs.doc.wustl.edu:/project/cvs-repository" />
        <environment name="CVS_RSH"            value="ssh" />

        <!-- Uncomment and set the value to your email address-->
        <!-- <variable name="MAIL_ADMIN"  value="taoadmin@ociweb.com" /> -->

        <variable name="root"     value="/tao_builds/taoadmin/DOC_pmb/ACE_wrappers" />
        <variable name="project_root"     value="build/doc_pmb_branch_paris_rh72_gcc2.95.3_d0o1" />
        <variable name="log_file" value="build/doc_pmb_branch_paris_rh72_gcc2.95.3_d0o1/rh72_gcc2.95.3_d0o1_build.txt" />

        <!--  where to move the logs to  -->
        <variable name="log_root" value="/home/taoadmin/.www-docs/autobuild_logs/doc_pmb_branch_paris_rh72_gcc2.95.3_d0o1" />

        <!--  configuration options for running tests;  equivalent to CONFIG var in auto_compile scripts  -->
        <variable name="configs"  value="STATIC Linux " />
        <!--  needed for the create_ace_build command -->
        <variable name="build_name" value="doc_pmb_branch_paris_rh72_gcc2.95.3_d0o1" />

    </configuration>
    
    <command name="status"            options="on" />
    <command name="file_manipulation" options="type=mustnotexist file=/build/doc_pmb_branch_paris_rh72_gcc2.95.3_d0o1/.disable" />
    <command name="file_manipulation" options="type=create file=/build/doc_pmb_branch_paris_rh72_gcc2.95.3_d0o1/.disable output='RUNNING'" />
    <command name="log"               options="on" />

    <command name="cvs"               options="-z 3 -q update -P -d ." />
    <command name="cvs"               options="-z 3 -q checkout -P ACE_MPC"/>
    <command name="create_ace_build"  options="-a -v" />

    <command name="file_manipulation" options="type=create file=/build/doc_pmb_branch_paris_rh72_gcc2.95.3_d0o1/ace/config.h output='\n#define TAO_HAS_DIOP 0\n#include \x22ace/config-linux.h\x22\n\n'" /> 
    <command name="file_manipulation" options="type=create file=/build/doc_pmb_branch_paris_rh72_gcc2.95.3_d0o1/include/makeinclude/platform_macros.GNU output='inline=1\nami=1\nrt_corba=1\nsmart_proxies=1\ninterceptors=1\ninterface_repo=1\ncorba_messaging=1\nprobe=0\nprofile=0\nfakesvcconf=0\noptimize=1\ndebug=0\nexceptions=0\nstatic_libs_only=1\ninclude $(ACE_ROOT)/include/makeinclude/platform_linux.GNU\n'" />

    <command name="print_ace_config" />

    <!-- Use these if the system can handle multiple make processes -->
    <!-- <command name="make"              options="-j 2 -k" /> -->
    <!-- <command name="make"              options="-j 2 -k -C TAO" /> -->

    <!-- By default the build is done with a single make process -->
    <command name="make"              options="-k" />
    <command name="make"              options="-k -C TAO" />

<!--    <command name="shell"             options="cd /tao_builds/taoadmin/DOC_pmb/ACE_wrappers/build/doc_pmb_branch_paris_rh72_gcc2.95.3_d0o1; ../../" /> -->

    <command name="auto_run_tests"    options="" />

    <!-- Forces a sync to clean state -->
    <command name="shell"             options="/bin/sync" /> 
    <command name="shell"             options="sleep 10" /> 
    <command name="shell"             options="/bin/sync" /> 
    <command name="shell"              options="/tao_builds/taoadmin/DOC_pmb/ACE_wrappers/build/doc_pmb_branch_paris_rh72_gcc2.95.3_d0o1/bin/footprint_stats.sh /tao_builds/taoadmin/DOC_pmb/ACE_wrappers/build/doc_pmb_branch_paris_rh72_gcc2.95.3_d0o1 /home/taoadmin/.www-docs/DOC_pmb_perf_footpr_stats/" />
    <command name="shell"              options="/tao_builds/taoadmin/DOC_pmb/ACE_wrappers/build/doc_pmb_branch_paris_rh72_gcc2.95.3_d0o1/bin/performance_stats.sh /tao_builds/taoadmin/DOC_pmb/ACE_wrappers/build/doc_pmb_branch_paris_rh72_gcc2.95.3_d0o1 /home/taoadmin/.www-docs/DOC_pmb_perf_footpr_stats/" />
    <command name="shell"              options="/tao_builds/taoadmin/DOC_pmb/ACE_wrappers/build/doc_pmb_branch_paris_rh72_gcc2.95.3_d0o1/bin/topinfo_simple_stats.sh /tao_builds/taoadmin/DOC_pmb/ACE_wrappers/build/doc_pmb_branch_paris_rh72_gcc2.95.3_d0o1/ /home/taoadmin/.www-docs/DOC_pmb_perf_footpr_stats/ taoadmin" />
    <command name="shell"              options="/tao_builds/taoadmin/DOC_pmb/ACE_wrappers/build/doc_pmb_branch_paris_rh72_gcc2.95.3_d0o1/bin/topinfo_iorsize_stats.sh /tao_builds/taoadmin/DOC_pmb/ACE_wrappers/build/doc_pmb_branch_paris_rh72_gcc2.95.3_d0o1/ /home/taoadmin/.www-docs/DOC_pmb_perf_footpr_stats/ taoadmin" />
    <command name="shell"              options="rsync -r --rsh=ssh /home/taoadmin/.www-docs/DOC_pmb_perf_footpr_stats/* prince.ociweb.com:/var/www/ociweb/private/Restricted/DOC_pmb_stats/" />

    <command name="log"               options="off" />
    <command name="process_logs"      options="clean move prettify index" />
    <command name="file_manipulation" options="type=delete file=/build/doc_pmb_branch_paris_rh72_gcc2.95.3_d0o1/.disable" />
    <command name="status"            options="off" />

    <!-- Push the logs to the webserver -->
    <command name="shell"             options="/home/taoadmin/nightly/uploadBuildLogs.sh doc_pmb_branch_paris_rh72_gcc2.95.3_d0o1" />
</autobuild>
