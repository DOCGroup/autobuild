<autobuild>
    <configuration>
        <environment name="ACE_ROOT"           value="/tao_builds/taoadmin/DOC-Latest/ACE_wrappers///build/doc_xi.ociweb.com_rh72_gcc2.96_d0o0" />
        <environment name="LD_LIBRARY_PATH"    value="/tao_builds/taoadmin/DOC-Latest/ACE_wrappers///build/doc_xi.ociweb.com_rh72_gcc2.96_d0o0/ace:$LD_LIBRARY_PATH" />
        <environment name="LIBPATH"            value="$LD_LIBRARY_PATH" />
        <environment name="TAO_ORBSVCS"        value="" />
        <environment name="SELECTION_CRITERIA" value=" -Config Linux " />
        <environment name="PATH"               value="/usr/bin:/usr/bin:/bin" />
        <environment name="LM_LICENSE_FILE"    value="" />

        <!-- Set the cvsroot value -->
        <!--<environment name="CVSROOT"            value=":ext:oci@cvs.doc.wustl.edu:/project/cvs-repository" />-->
        <environment name="CVSROOT"            value=":ext:oci@cvs.doc.wustl.edu:/project/cvs-repository" />
        <environment name="CVS_RSH"            value="ssh" />

        <!-- Uncomment and set the value to your email address-->
        <!-- <variable name="MAIL_ADMIN"  value="taoadmin@ociweb.com" /> -->

        <variable name="root"     value="/tao_builds/taoadmin/DOC-Latest/ACE_wrappers/" />
        <variable name="project_root"     value="/tao_builds/taoadmin/DOC-Latest/ACE_wrappers///build/doc_xi.ociweb.com_rh72_gcc2.96_d0o0" />
        <variable name="log_file" value="rh72_gcc2.96_d0o0_build.txt" />

        <!--  where to move the logs to  -->
        <variable name="log_root" value="/home/taoadmin/.www-docs/autobuild_logs/doc_xi.ociweb.com_rh72_gcc2.96_d0o0" />

        <!--  configuration options for running tests; equivalent to CONFIG var in auto_compile scripts  -->
        <variable name="configs"  value=" Linux " />
        <!--  needed for the create_ace_build command -->
        <variable name="build_name" value="doc_xi.ociweb.com_rh72_gcc2.96_d0o0" />

    </configuration>
    
    <command name="cvs"               options="-z 3 -q update -P -d ." />

    <command name="status"            options="on" />
    <command name="file_manipulation" options="type=mustnotexist file=/build/doc_xi.ociweb.com_rh72_gcc2.96_d0o0/.disable" />
    <command name="create_ace_build"  options="-a -v" />
    <command name="file_manipulation" options="type=create file=/build/doc_xi.ociweb.com_rh72_gcc2.96_d0o0/.disable output='RUNNING'" />
    <command name="log"               options="on" />

    <command name="file_manipulation" options="type=create file=/build/doc_xi.ociweb.com_rh72_gcc2.96_d0o0/ace/config.h output='\n#include \x22ace/config-linux.h\x22\n\n'" /> 
    <command name="file_manipulation" options="type=create file=/build/doc_xi.ociweb.com_rh72_gcc2.96_d0o0/include/makeinclude/platform_macros.GNU output='threads=0\ninclude $(ACE_ROOT)/include/makeinclude/platform_linux.GNU\ndebug=0\nexceptions=1\n'" />

    <command name="print_ace_config" />

    <command name="make"              options="-j 2 -k" />
    <command name="make"              options="-j 2 -k -C TAO" />

<!--    <command name="shell"             options="cd /tao_builds/taoadmin/DOC-Latest/ACE_wrappers///build/doc_xi.ociweb.com_rh72_gcc2.96_d0o0; ../../" /> -->

    <command name="auto_run_tests"    options="" />

    <command name="log"               options="off" />
    <command name="process_logs"      options="clean move prettify index" />
    <command name="file_manipulation" options="type=delete file=/build/doc_xi.ociweb.com_rh72_gcc2.96_d0o0/.disable" />
    <command name="status"            options="off" />

    <!-- Push the logs to the webserver -->
    <command name="shell"             options="cd /home/taoadmin/.www-docs/autobuild_logs/doc_xi.ociweb.com_rh72_gcc2.96_d0o0/..; scp -r doc_xi.ociweb.com_rh72_gcc2.96_d0o0 prince.ociweb.com:/var/www/html/OCIscoreboard/" />
</autobuild>
