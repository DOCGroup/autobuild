<autobuild>
  <!-- The command used to create this file (on wiro) was -->
  <!-- /distcomp/nightly/BuildXML/buildxml.pl -base /tao_builds/taoadmin/DDS -checkout svn://svn.dre.vanderbilt.edu/DOC/DDS/trunk -compiler gcc-4.6 -creator mitza@ociweb.com -dds 1 -docbuild 1 -docrepo 1 -outdir . -piggyback /tao_builds/taoadmin/doc/ACE_wrappers/build/gcc-4.6_d1o0 -environment OPENDDS_RTPS_DEFAULT_D0=146 -testopt "-Config RTPS" -->
  <configuration>
    <environment name="ACE_ROOT"        value="/tao_builds/taoadmin/doc/ACE_wrappers/build/gcc-4.6_d1o0"/>
    <environment name="CIAO_ROOT"       value="/tao_builds/taoadmin/doc/ACE_wrappers/build/gcc-4.6_d1o0/TAO/CIAO"/>
    <environment name="DANCE_ROOT"      value="/tao_builds/taoadmin/doc/ACE_wrappers/build/gcc-4.6_d1o0/TAO/DAnCE"/>
    <environment name="DDS_ROOT"        value="/tao_builds/taoadmin/DDS/build/gcc-4.6_d1o0"/>
    <environment name="LD_LIBRARY_PATH" value="/usr/sfw/lib:/usr/local/lib:/tao_builds/taoadmin/DDS/build/gcc-4.6_d1o0/lib:/tao_builds/taoadmin/doc/ACE_wrappers/build/gcc-4.6_d1o0/lib:"/>
    <environment name="MPC_ROOT"        value="/home/taoadmin/MPC"/>
    <environment name="OPENDDS_RTPS_DEFAULT_D0" value="146"/>
    <environment name="PATH"            value="/tao_builds/taoadmin/DDS/build/gcc-4.6_d1o0/bin:/tao_builds/taoadmin/doc/ACE_wrappers/build/gcc-4.6_d1o0/bin:/usr/local/bin:/usr/bin:/bin:/usr/sbin:/sbin:/usr/ccs/bin:"/>
    <environment name="TAO_ROOT"        value="/tao_builds/taoadmin/doc/ACE_wrappers/build/gcc-4.6_d1o0/TAO"/>
    <variable    name="base"            value="DDS"/>
    <variable    name="build_name"      value="gcc-4.6_d1o0"/>
    <variable    name="creator"         value="mitza@ociweb.com"/>
    <variable    name="log_file"        value="gcc-4.6_d1o0.log"/>
    <variable    name="log_root"        value="/www-docs/autobuild_logs/dds_doc_trunk_wiro_linux_gcc-4.6_d1o0"/>
    <variable    name="project_root"    value="/tao_builds/taoadmin/DDS/build/gcc-4.6_d1o0"/>
    <variable    name="root"            value="/tao_builds/taoadmin/DDS"/>
  </configuration>

  <!-- Build preparation -->
  <command name="process_listener"   options="localhost:32001"/>
  <command name="status"             options="on"/>
  <command name="log"                options="on"/>
  <command name="svn"                options="co svn://svn.dre.vanderbilt.edu/DOC/DDS/trunk ."/>
  <command name="clone_build_tree"/>
  <command name="rem_sems"/>
  <command name="file_manipulation"  options="type=create file=build/gcc-4.6_d1o0/setenv.sh output='ACE_ROOT=\x22/tao_builds/taoadmin/doc/ACE_wrappers/build/gcc-4.6_d1o0\x22; export ACE_ROOT\nCIAO_ROOT=\x22/tao_builds/taoadmin/doc/ACE_wrappers/build/gcc-4.6_d1o0/TAO/CIAO\x22; export CIAO_ROOT\nDANCE_ROOT=\x22/tao_builds/taoadmin/doc/ACE_wrappers/build/gcc-4.6_d1o0/TAO/DAnCE\x22; export DANCE_ROOT\nDDS_ROOT=\x22/tao_builds/taoadmin/DDS/build/gcc-4.6_d1o0\x22; export DDS_ROOT\nLD_LIBRARY_PATH=\x22/usr/sfw/lib:/usr/local/lib:/tao_builds/taoadmin/DDS/build/gcc-4.6_d1o0/lib:/tao_builds/taoadmin/doc/ACE_wrappers/build/gcc-4.6_d1o0/lib:\x22; export LD_LIBRARY_PATH\nMPC_ROOT=\x22/home/taoadmin/MPC\x22; export MPC_ROOT\nPATH=\x22/tao_builds/taoadmin/DDS/build/gcc-4.6_d1o0/bin:/tao_builds/taoadmin/doc/ACE_wrappers/build/gcc-4.6_d1o0/bin:/usr/local/bin:/usr/bin:/bin:/usr/sbin:/sbin:/usr/ccs/bin:\x22; export PATH\nTAO_ROOT=\x22/tao_builds/taoadmin/doc/ACE_wrappers/build/gcc-4.6_d1o0/TAO\x22; export TAO_ROOT\n'"/>
  <command name="print_os_version"/>
  <command name="print_env_vars"/>
  <command name="print_ace_config"/>
  <command name="print_make_version"/>
  <command name="check_compiler"     options="gcc-4.6"/>
  <command name="generate_workspace" options="-type gnuace DDS.mwc"/>

  <!-- Build dependencies -->
  <command name="make" options="-s -k  find=*akefile dir=. depend"/>

  <!-- Build targets -->
  <command name="make" options="-s -k  find=*akefile dir=."/>

  <!-- Post build -->
  <command name="auto_run_tests"  options="dir=. -Config Linux -Config RTPS"/>
  <command name="log"             options="off"/>
  <command name="process_logs"    options="copy"/>
  <command name="status"          options="off"/>
</autobuild>
