<autobuild>
    <configuration>
        <environment name="ACE_ROOT"     value="C:\\ACE\\noinline\\ACE_wrappers" />
        <environment name="TAO_ROOT"     value="C:\\ACE\\noinline\\ACE_wrappers\\TAO"/>
        <environment name="CIAO_ROOT"     value="C:\\ACE\\noinline\\ACE_wrappers\\TAO\\CIAO"/>
        <environment name="DANCE_ROOT"     value="C:\\ACE\\noinline\\ACE_wrappers\\TAO\\CIAO\\DAnCE"/>
        <environment name="XERCESCROOT"  value="C:\\utils"/>
        <environment name="RSYNC_RSH"    value="c:\\Cygwin\\bin\\ssh" />
        <environment name="PATH"         value="C:\\ACE\\noinline\\ACE_wrappers\\bin;C:\\ACE\\noinline\\ACE_wrappers\\lib;C:\\utils\\lib;c:\\cygwin\\bin" type="prefix" />
        <environment name="PATH"          value="C:\\Program Files (x86)\\Microsoft Visual Studio 9.0\\Common7\\IDE;C:\\Program Files (x86)\\Microsoft Visual Studio 9.0\\VC\\bin;c:\\Program Files\\Microsoft SDKs\\Windows\\v6.0A\\bin" type="prefix" />
        <environment name="INCLUDE"      value="c:\\Program Files (x86)\\Microsoft Visual Studio 9.0\\VC\\ATLMFC\\INCLUDE;c:\\Program Files (x86)\\Microsoft Visual Studio 9.0\\VC\\INCLUDE;c:\\Program Files (x86)\\Microsoft Visual Studio 9.0\\VC\\PlatformSDK\\include;c:\\Program Files (x86)\\Microsoft Visual Studio 9.0\\SDK\\v2.0\\include;c:\\Program Files\\Microsoft SDKs\\Windows\\v6.0A\\include" type="prefix" />
        <environment name="LIB"          value="c:\\Program Files (x86)\\Microsoft Visual Studio 9.0\\VC\\ATLMFC\\LIB;c:\\Program Files (x86)\\Microsoft Visual Studio 9.0\\VC\\LIB;c:\\Program Files (x86)\\Microsoft Visual Studio 9.0\\VC\\PlatformSDK\\lib;c:\\Program Files (x86)\\Microsoft Visual Studio 9.0\\SDK\\v2.0\\lib;C:\Program Files\\Microsoft SDKs\\Windows\\v6.0A\\lib" type="prefix" />
        <environment name="BOOST_ROOT"  value="C:\\utils" />
        <environment name="BOOST_VER"   value="boost-1_36" />
        <variable name="configs"  value="Win32 Exceptions" />
        <variable name="root"     value="C:\\ACE\\noinline\\" />
        <variable name="log_file" value="build.txt.txt" />
        <variable name="log_root" value="C:\\ACE\\noinline\\" />
        <variable name="name"     value="Win2003 noinline RT_CAPABLE" />
        <variable name="make_program" value="nmake /K" />
    </configuration>

    <command name="status"            options="on" />

    <command name="process_listener"  options="localhost:32002" />

    <command name="make"              options="/I CFG=\x22Win32 Debug\x22 REALCLEAN" />

    <command name="log"               options="ON" />

    <command name="svn"               options="co svn://svn.dre.vanderbilt.edu/DOC/Middleware/sets-anon/ACE+TAO+CIAO ." />

    <command name="shell"             options="find ACE_wrappers -name '*.mak' -exec rm '{}' ';'" />
    <command name="shell"             options="find ACE_wrappers -name '*.manifest' -exec rm '{}' ';'" />

    <command name="file_manipulation" options="type=create file=ACE_wrappers/ace/config.h output='#define ACE_DISABLE_WIN32_ERROR_WINDOWS\n#include \x22ace/config-win32.h\x22\n'" />
    <command name="file_manipulation" options="type=create file=ACE_wrappers/bin/MakeProjectCreator/config/default.features output='noinline=1\nxerces2=1\n'" />

    <command name="print_os_version" />
    <command name="print_env_vars" />
    <command name="check_compiler" options="msvc" />
    <command name="print_ace_config" />
    <command name="print_perl_version" />

    <command name="generate_workspace" options="dirs=TAO/CIAO -type vc9 -value_template compile_flags+=/Ob0 CIAO_TAO_DAnCE.mwc"/>

    <command name="win32make"          options="msvc_mpc_auto_compile -dirs TAO/CIAO -vc9 -ALL -Debug -k" />

    <command name="log"               options="OFF" />
    <command name="process_logs"      options="move prettify clean index" />
    <command name="status"            options="off" />

    <command name="shell"             options="c:\\cygwin\\bin\\rsync --progress --recursive --delete -z --exclude /tao-1.4.5/ --exclude /ACE_wrappers*/ --include *.txt --include *.html --exclude ** ./ bczar@naboo.dre.vanderbilt.edu:/web/users/isisbuilds/auto_compile_logs/trian_noinline" />
</autobuild>
