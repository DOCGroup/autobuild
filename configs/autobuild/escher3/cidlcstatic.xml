<!-- $Id$ -->
<autobuild>
    <configuration>
        <environment name="ACE_ROOT"         value="D:\isisbuilds\CIDLC\ACE_wrappers" />
        <environment name="TAO_ROOT"         value="D:\isisbuilds\CIDLC\ACE_wrappers\TAO"/>
        <environment name="CIAO_ROOT"         value="D:\isisbuilds\CIDLC\ACE_wrappers\TAO\CIAO"/>
        <environment name="Path"             value="D:\isisbuilds\CIDLC\ACE_wrappers\bin;D:\isisbuilds\CIDLC\ACE_wrappers\lib;C:\cvs\;C:\Perl\bin;C:\putty\;" type="prefix" />
        <variable name="root"     value="D:/isisbuilds/CIDLC/" />
        <variable name="log_file" value="log.txt" />
        <variable name="log_root" value="D:/isisbuilds/CIDLC/" />
        <variable name="project_root" value="D:/isisbuilds/CIDLC/ACE_wrappers" />
        <variable name="name"     value="WinXP LIB" />
        <variable name="configs"   value="Win32 SANDBOX Exceptions" />
    </configuration>
    <command name="status"            options="on" />
    <command name="file_manipulation" options="type=mustnotexist file=.disable" />
    <command name="file_manipulation" options="type=create file=.disable output='RUNNING'" />

    <command name="log"               options="ON" />

    <command name="cvs"               options="-z9 -q checkout -P ACE_wrappers" />

    <command name="print_os_version" />
    <command name="check_compiler" options="msvc" />
    <command name="print_ace_config" />
    <command name="print_perl_version" />


    <command name="win32make"         options="msvc_cidlc -vc7 -s -clean" />
    <command name="win32make"         options="msvc_cidlc -vc7 -s" />

    <command name="log"               options="OFF" />
    <command name="process_logs"      options="move prettify clean index" />
    <command name="file_manipulation" options="type=delete file=.disable" />
    <command name="status"            options="off" />
    <command name="shell"             options="pscp -i c:\putty\isisbuilds.ppk ACE_wrappers\TAO\CIAO\bin\cidlc.exe bczar@www.dre.vanderbilt.edu:/web/users/isisbuilds/CIDLC/Win32" />
    <command name="shell"             options="pscp -i c:\putty\isisbuilds.ppk *.txt bczar@www.dre.vanderbilt.edu:/web/users/isisbuilds/auto_compile_logs/Win32_CIDLC" />
    <command name="shell"             options="pscp -i c:\putty\isisbuilds.ppk *.html bczar@www.dre.vanderbilt.edu:/web/users/isisbuilds/auto_compile_logs/Win32_CIDLC" />
</autobuild>
