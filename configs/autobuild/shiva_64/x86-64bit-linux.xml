<autobuild>
    <configuration>

        <environment name="ACE_ROOT"        value="/build/x86-64bit/ACE_wrappers" />
        <environment name="TAO_ROOT"        value="/build/x86-64bit/ACE_wrappers/TAO" />
        <environment name="CIAO_ROOT"       value="/build/x86-64bit/ACE_wrappers/TAO/CIAO" />
        <environment name="LD_LIBRARY_PATH" value="/build/x86-64bit/ACE_wrappers/lib:/build/x86-64bit/ACE_wrappers/ace:" />

        <variable name="project_root" value="/build/x86-64bit/ACE_wrappers" />
        <variable name="build_name" value="linux-x86-64bit" />
        <variable name="root"     value="/build/x86-64bit" />
        <variable name="log_file" value="build-log.txt" />
        <variable name="log_root" value="/build/x86-64bit/buildlog" />
        <variable name="configs"  value="Linux 64bit x86 CIAO" />

    </configuration>

    <command name="status"            options="on" />
    <command name="process_listener"  options="localhost:32001" />
    <command name="log"               options="ON" />

    <command name="make"              options="-k realclean" />

    <command name="shell"             options="find ACE_wrappers -name 'GNUmakefile*' -exec rm '{}' ';'" />

    <command name="svn"               options="co svn://svn.dre.vanderbilt.edu/DOC/Middleware/sets-anon/ACE+TAO+CIAO ." />

    <!-- <command name="create_ace_build"  /> -->

    <command name="file_manipulation" options="type=create file=ACE_wrappers/ace/config.h output='#include \x22ace/config-linux.h\x22\n'" />
    <command name="file_manipulation" options="type=create file=ACE_wrappers/include/makeinclude/platform_macros.GNU output='include $(ACE_ROOT)/include/makeinclude/platform_linux.GNU\ncidl=1\ndebug=0\n'" />
    <command name="file_manipulation" options="type=create file=ACE_wrappers/bin/MakeProjectCreator/config/default.features output='ssl=0\ncidl=1\ndebug=0\n'" />

    <command name="print_os_version" />
    <command name="check_compiler" options="g++" />
    <command name="print_ace_config" />
    <command name="print_make_version" />
    <command name="print_cidlc_version" />
    <command name="print_perl_version" />

    <command name="generate_workspace" options="-exclude TAO/TAO_*.mwc,TAO/CIAO/CIAO_*.mwc -type gnuace" />

    <command name="anonymous_shell"   options="/bin/cp /web/users/isisbuilds/CIDLC/gnu-linux/cidlc ACE_wrappers/TAO/CIAO/bin" />

    <command name="make"              options="-k -j 4" />

    <command name="auto_run_tests"    options="-a -o -t" />
    <command name="log"               options="OFF" />
    <command name="process_logs"      options="move prettify clean index" />
    <command name="status"            options="off" />
    <command name="anonymous_shell"   options="rsync -av --delete -z buildlog/ bczar@naboo.dre.vanderbilt.edu:/web/users/isisbuilds/auto_compile_logs/shiva_64" />

</autobuild>

