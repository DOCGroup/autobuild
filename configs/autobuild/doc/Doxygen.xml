<!-- $Id$ -->
<autobuild>
    <configuration>

        <environment name="ACE_ROOT"        value="/project/doctmp/Doxygen/staging_area/Current/ACE_wrappers" />
        <environment name="TAO_ROOT"        value="/project/doctmp/Doxygen/staging_area/Current/ACE_wrappers/TAO"/>
        <environment name="CVSROOT"         value=":ext:ucibuilds@cvs.doc.wustl.edu:/project/cvs-repository" />
        <environment name="CVS_RSH"         value="/usr/bin/ssh" />

        <variable name="root"     value="/project/doctmp/Doxygen/staging_area/Current" />
        <variable name="log_file" value="build.txt" />
        <variable name="log_root" value="/home/bugzilla/.www-docs/auto_compile_logs/doc_TAO_doxygen" />

    </configuration>

    <command name="status"            options="on" />
    <command name="file_manipulation" options="type=mustnotexist file=.disable" />
    <command name="file_manipulation" options="type=create file=.disable output='RUNNING'" />
    <command name="log"               options="on" />
    <command name="cvs"               options="-z 3 -q checkout -P ACE_wrappers" />

    <command name="file_manipulation" options="type=create file=ACE_wrappers/ace/config.h output='#include \x22ace/config-doxygen.h\x22\n'" />

    <command name="print_os_version" />
    <command name="print_ace_config" />

    <command name="shell" options="/home/bugzilla/autobuild/configs/autobuild/doc/auto_doxygen.pl" />


    <command name="log"               options="off" />
    <command name="process_logs"      options="clean move prettify index" />
    <command name="file_manipulation" options="type=delete file=.disable" />
    <command name="status"            options="off" />
</autobuild>
