<!-- $Id$ -->
<autobuild>
    <configuration>
        <include name="fc6-env.xml" />
        <variable name="root"     value="/isisbuilds/" />
        <variable name="log_file" value="build.txt" />
        <variable name="log_root" value="/proj/autobuilds/logs/gen_ostream/" />
        <variable name="configs"  value="Linux SSL Exceptions" />
    </configuration>

    <command name="status"            options="on" />

    <command name="process_listener"  options="localhost:32005" />

    <command name="log"               options="on" />

    <command name="svn"               options="co svn://svn.dre.vanderbilt.edu/DOC/Middleware/sets-anon/ACE+TAO+CIAO ." />

    <command name="file_manipulation" options="type=create file=ACE_wrappers/ace/config.h output='#define ACE_USES_WCHAR\n#include \x22ace/config-linux.h\x22\n'" />
    <command name="file_manipulation" options="type=create file=ACE_wrappers/include/makeinclude/platform_macros.GNU output='gen_ostream=1\ndebug=0\nrt_corba=1\nsmart_proxies=1\ninterface_repo=1\ninterceptors=1\nssl=1\ncidl=1\ninclude $(ACE_ROOT)/include/makeinclude/platform_linux.GNU\n'" />
    <command name="file_manipulation" options="type=create file=ACE_wrappers/bin/MakeProjectCreator/config/default.features output='gen_ostream=1\nssl=1\ncidl=1\n'" />

    <command name="anonymous_shell"   options="scp bczar@naboo.dre.vanderbilt.edu:/web/users/isisbuilds/CIDLC/gnu-linux/cidlc ACE_wrappers/TAO/CIAO/bin" />

    <command name="print_os_version" />
    <command name="print_env_vars" />
    <command name="check_compiler" options="gcc" />
    <command name="print_ace_config" />
    <command name="print_cidlc_version" />
    <command name="print_make_version" />
    <command name="print_perl_version" />

    <command name="generate_workspace" options="-exclude TAO/TAO_*.mwc,TAO/CIAO -type gnuautobuild" />

    <command name="make"              options="-j 4 -k" />

    <command name="auto_run_tests"    options="-a -o -t" />

    <command name="log"               options="off" />
    <command name="process_logs"      options="clean move prettify index" />
    <command name="status"            options="off" />
</autobuild>
