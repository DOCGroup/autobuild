<!-- $Id$ -->
<autobuild>
    <configuration>
        <environment name="ACE_ROOT"      value="C:\ACE\VC8\ACE_wrappers" />
        <environment name="TAO_ROOT"      value="C:\ACE\VC8\ACE_wrappers\TAO"/>
        <environment name="CIAO_ROOT"     value="C:\ACE\VC8\ACE_wrappers\TAO\CIAO"/>
        <environment name="CVSROOT"       value=":ext:isisbuilds@cvs.doc.wustl.edu:/project/cvs-repository" />
        <environment name="CVS_RSH"       value="C:\bin\plink.exe" />
        <environment name="XERCESCROOT"   value="C:\xerces-vc8" />
        <environment name="ZLIB_ROOT"     value="C:\zlib" />
        <environment name="ZZIP_ROOT"     value="C:\zzip" />
        <environment name="PATH"         value="C:\Program Files\Microsoft Visual Studio 8\Common7\IDE;C:\Program Files\Microsoft Visual Studio 8\Vc\vcpackages;C:\Program Files\Microsoft Visual Studio 8\Vc\bin;C:\Program Files\Microsoft Visual Studio 8\VC\vcpackages;C:\bin;C:\zlib\lib;C:\zzip\lib;c:\xerces-vc8\lib;C:\Perl\bin;C:\Program Files\Microsoft Visual Studio 8\Common7\IDE;C:\Program Files\Microsoft Visual Studio 8\Vc\bin;C:\ACE\VC8\ACE_wrappers\bin;C:\ACE\VC8\ACE_wrappers\lib;C:\ACE\VC8\ACE_wrappers\TAO\CIAO\bin" type="prefix" />
        <environment name="INCLUDE"      value="c:\Program Files\Microsoft Visual Studio 8\VC\ATLMFC\INCLUDE;c:\Program Files\Microsoft Visual Studio 8\VC\INCLUDE;c:\Program Files\Microsoft Visual Studio 8\VC\PlatformSDK\include;c:\Program Files\Microsoft Visual Studio 8\SDK\v2.0\include;" type="prefix" />
        <environment name="LIB"          value="c:\Program Files\Microsoft Visual Studio 8\VC\ATLMFC\LIB;c:\Program Files\Microsoft Visual Studio 8\VC\LIB;c:\Program Files\Microsoft Visual Studio 8\VC\PlatformSDK\lib;c:\Program Files\Microsoft Visual Studio 8\SDK\v2.0\lib;LIBPATH=c:\WINDOWS\Microsoft.NET\Framework\v2.0.50727;c:\Program Files\Microsoft Visual Studio 8\VC\ATLMFC\LIB" type="prefix" />

        <variable name="configs"    value="Win32 VC8" />
        <variable name="root"       value="C:\ACE\VC8\" />
        <variable name="log_file"   value="log.txt" />
        <variable name="log_root"   value="C:\ACE\VC8\" />
        <variable name="name"       value="WinXP VC8 Full" />
    </configuration>

    <command name="status"            options="on" />
    <command name="file_manipulation" options="type=mustnotexist file=.disable" />
    <command name="file_manipulation" options="type=create file=.disable output='RUNNING'" />

    <command name="win32make"         options="nmake /I CFG=\x22Win32 Debug\x22 REALCLEAN" />

    <command name="log"               options="ON" />

    <command name="cvs"               options="-z9 -q checkout -P ACE_wrappers" />
    <command name="cvs"               options="-z9 -q checkout -P ACE_MPC"/>

    <command name="shell"             options="find ACE_wrappers -name '*.mak' -exec rm '{}' ';'" />
    <command name="shell"             options="find ACE_wrappers -name '*.manifest' -exec rm '{}' ';'" />

    <command name="file_manipulation" options="type=create file=ACE_wrappers/ace/config.h output='#define ACE_DISABLE_WIN32_ERROR_WINDOWS\n#define ACE_HAS_STANDARD_CPP_LIBRARY 1\n#include \x22ace/config-win32.h\x22\n'" />
    <command name="file_manipulation" options="type=create file=ACE_wrappers/bin/MakeProjectCreator/config/default.features output='rwho=0\nqos=1\nzlib=1\nzzip=1\n'" />

    <command name="anonymous_shell"   options="pscp -C bczar@naboo.dre.vanderbilt.edu:/web/users/isisbuilds/CIDLC/Win32/cidlc.exe ACE_wrappers/TAO/CIAO/bin" />

    <command name="print_os_version" />
    <command name="check_compiler"    options="msvc" />
    <command name="print_ace_config" />
    <command name="print_cidlc_version" />
    <command name="print_perl_version" />

    <command name="generate_workspace" options="-type nmake -base vc8nmake -exclude TAO/CIAO,TAO/TAO_*.mwc"/>

    <command name="win32make"         options="nmake /I CFG=\x22Win32 Debug\x22 NO_EXTERNAL_DEPS=1" />

    <command name="log"               options="OFF" />
    <command name="process_logs"      options="move prettify clean=5 index" />
    <command name="file_manipulation" options="type=delete file=.disable" />
    <command name="status"            options="off" />
    <command name="shell"             options="unison -batch -auto -force . -ignore \x22Name ACE_wrappers \x22 -ignore \x22Name log.txt\x22 -ignore \x22Name .disable \x22 . ssh://bczar@naboo.dre.vanderbilt.edu//web/users/isisbuilds/auto_compile_logs/blade50_vc8_full" />
</autobuild>
