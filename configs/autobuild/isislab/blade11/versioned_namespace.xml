<autobuild>
    <configuration>
        <environment name="ACE_ROOT"     value="C:\Docume~1\bczar\builds\versioned_namespace\ACE_wrappers" />
        <environment name="TAO_ROOT"     value="C:\Docume~1\bczar\builds\versioned_namespace\ACE_wrappers\TAO"/>
        <environment name="CIAO_ROOT"     value="C:\Docume~1\bczar\builds\versioned_namespace\ACE_wrappers\TAO\CIAO"/>
        <environment name="XERCESCROOT"  value="C:\Docume~1\bczar\xerces"/>
        <environment name="CVSROOT"      value=":ext:isisbuilds@cvs.doc.wustl.edu:/project/cvs-repository" />
        <environment name="CVS_RSH"      value="plink" />
        <environment name="PATH"         value="C:\Docume~1\bczar\builds\versioned_namespace\ACE_wrappers\bin;C:\Docume~1\bczar\builds\versioned_namespace\ACE_wrappers\lib;C:\Docume~1\bczar\xerces\lib" type="prefix" />
        <variable name="configs"  value="Win32 Exceptions" />
        <variable name="root"     value="C:\Docume~1\bczar\builds\versioned_namespace\" />
        <variable name="log_file" value="log.txt" />
        <variable name="log_root" value="C:\Docume~1\bczar\builds\versioned_namespace\" />
        <variable name="name"     value="Win2003 VERSIONED_NAMESPACE" />
    </configuration>

    <command name="status"            options="on" />
    <command name="file_manipulation" options="type=mustnotexist file=.disable" />
    <command name="file_manipulation" options="type=create file=.disable output='RUNNING'" />

    <command name="win32make"         options="msvc_mpc_auto_compile -vc71 -ALL -Debug -k -clean" />

    <command name="log"               options="ON" />

    <command name="cvs"               options="-z 3 -q checkout -P ACE_wrappers" />
    <command name="cvs"               options="-z 3 -q checkout -P ACE_MPC" />

    <command name="file_manipulation" options="type=create file=ACE_wrappers/ace/config.h output='#define ACE_DISABLE_WIN32_ERROR_WINDOWS\n#define ACE_HAS_STANDARD_CPP_LIBRARY 1\n#include \x22ace/config-win32.h\x22\n'" />
    <command name="file_manipulation" options="type=create file=ACE_wrappers/bin/MakeProjectCreator/config/default.features output='rwho=0\nqos=1\nversioned_namespace=1\n'" />
    <command name="anonymous_shell"   options="pscp -C bczar@naboo.dre.vanderbilt.edu:/web/users/isisbuilds/CIDLC/Win32/cidlc.exe ACE_wrappers/TAO/CIAO/bin" />

    <command name="print_os_version" />
    <command name="check_compiler" options="msvc" />
    <command name="print_ace_config" />
    <command name="print_cidlc_version" />
    <command name="print_perl_version" />

    <command name="generate_workspace" options="-exclude TAO/CIAO/CIDLC,TAO/CIAO/CCF -type vc71 -recurse"/>

    <command name="win32make"         options="msvc_mpc_auto_compile -vc71 -ALL -Debug -k" />

    <command name="auto_run_tests"    options="-s \x22C:\Docume~1\bczar\autobuild\win32sandbox\sandbox.exe -t 5400000\x22" />

    <command name="log"               options="OFF" />
    <command name="process_logs"      options="move prettify clean=5 index" />
    <command name="file_manipulation" options="type=delete file=.disable" />
    <command name="status"            options="off" />
    <command name="anonymous_shell"   options="pscp -p -C *.txt *.html bczar@naboo.dre.vanderbilt.edu:/web/users/isisbuilds/auto_compile_logs/isislab/blade11/versioned_namespace"/>
</autobuild>
