<!-- $Id$ -->
<autobuild>
    <configuration>
        <environment name="ACE_ROOT"     value="C:\ACE\VC71s\ACE_wrappers" />
        <environment name="TAO_ROOT"     value="C:\ACE\VC71s\ACE_wrappers\TAO"/>
        <environment name="CIAO_ROOT"     value="C:\ACE\VC71s\ACE_wrappers\TAO\CIAO"/>
        <environment name="RSYNC_RSH"       value="ssh" />
        <environment name="PATH"         value="C:\Program Files\Microsoft Visual Studio .NET 2003\Common7\IDE;C:\Program Files\Microsoft Visual Studio .NET 2003\Vc7\bin;C:\ACE\VC71s\ACE_wrappers\bin;C:\ACE\VC71s\ACE_wrappers\lib;c:\xerces\bin;C:\Perl\bin;c:\zzip\lib;c:\zlib\lib;c:\putty;c:\xerces\lib;c:\cygwin\bin"  type="prefix" />
        <environment name="XERCESCROOT"   value="C:\xerces"/>
        <environment name="BOOST_ROOT"    value="C:\Boost-1.32"/>
        <environment name="BOOST_VERSION" value="boost-1_32"/>
        <environment name="BOOST_CFG"     value="-vc71-mt" />
        <environment name="BOOST_STATIC_LIB_PREFIX" value="lib" />
        <environment name="ZLIB_ROOT"       value="C:\zlib" />
        <environment name="ZZIP_ROOT"       value="C:\zzip" />
        <variable name="configs"  value="Win32 VC71 Exceptions RT_CAPABLE" />
        <variable name="root"     value="C:\ACE\VC71s\" />
        <variable name="log_file" value="log.txt" />
        <variable name="log_root" value="C:\ACE\VC71s\" />
        <variable name="name"     value="WinXP VS.NET 2003" />
    </configuration>

    <command name="status"            options="on" />

    <command name="process_listener"  options="localhost:32001" />

    <command name="win32make"         options="msvc_static_compile -TAO -vc7 -Debug -k -clean -name_modifier *_Static" />

    <command name="shell"             options="c:\cygwin\bin\find ACE_wrappers -name '*.vcproj*' -exec c:\cygwin\bin\rm '{}' ';'" />
    <command name="shell"             options="c:\cygwin\bin\find ACE_wrappers -name '*.sln*' -exec c:\cygwin\bin\rm '{}' ';'" />

    <command name="log"               options="ON" />

    <command name="svn"               options="co svn://svn.dre.vanderbilt.edu/DOC/Middleware/sets-anon/ACE+TAO+CIAO ." />

    <command name="file_manipulation" options="type=create file=ACE_wrappers/ace/config.h output='#define ACE_DISABLE_WIN32_ERROR_WINDOWS\n#include \x22ace/config-win32.h\x22\n'" />
    <command name="file_manipulation" options="type=create file=ACE_wrappers/bin/MakeProjectCreator/config/default.features output='rwho=0\nqos=1\nboost=0\ncidl=0\nzlib=0\nzzip=0\n'" />


    <command name="anonymous_shell"   options="scp -C bczar@naboo.dre.vanderbilt.edu:/web/users/isisbuilds/CIDLC/Win32/cidlc.exe ACE_wrappers/TAO/CIAO/bin" />
    <command name="anonymous_shell"   options="chmod 777 ACE_wrappers/TAO/CIAO/bin/cidlc.exe" />

    <command name="print_os_version" />
    <command name="check_compiler" options="msvc" />
    <command name="print_ace_config" />
    <command name="print_cidlc_version" />
    <command name="print_perl_version" />

    <command name="generate_workspace" options="dirs=TAO/CIAO CIAO_TAO.mwc -type vc71 -static -name_modifier *_Static "/>

    <command name="win32make"         options="msvc_static_compile -CORE -vc7 -Debug -k -name_modifier *_Static" />

    <command name="log"               options="OFF" />
    <command name="process_logs"      options="move prettify clean index" />
    <command name="status"            options="off" />
    <command name="anonymous_shell"   options="rsync --progress --recursive --delete -z --exclude /ACE_wrappers*/ --include  *.txt --include *.html --exclude ** ./  bczar@naboo.dre.vanderbilt.edu:/web/users/isisbuilds/auto_compile_logs/isislab/blade17/vc71s"/>
</autobuild>
