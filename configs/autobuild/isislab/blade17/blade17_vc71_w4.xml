<!-- $Id$ -->
<autobuild>
    <configuration>
        <environment name="ACE_ROOT"     value="C:\ACE\VC71_W4\ACE_wrappers" />
        <environment name="TAO_ROOT"     value="C:\ACE\VC71_W4\ACE_wrappers\TAO"/>
        <environment name="CIAO_ROOT"     value="C:\ACE\VC71_W4\ACE_wrappers\TAO\CIAO"/>
        <environment name="RSYNC_RSH"       value="c:\cygwin\bin\ssh" />
        <environment name="PATH"         value="C:\Program Files\Microsoft Visual Studio .NET 2003\Common7\IDE;C:\Program Files\Microsoft Visual Studio .NET 2003\Vc7\bin;C:\ACE\VC71_W4\ACE_wrappers\bin;C:\ACE\VC71_W4\ACE_wrappers\lib;c:\xerces\bin;C:\Perl\bin;c:\zzip\lib;c:\zlib\lib;c:\putty:c:\xerces\lib;c:\cygwin\bin"  type="prefix" />
        <environment name="XERCESCROOT"   value="C:\xerces"/>
        <environment name="BOOST_ROOT"    value="C:\Boost-1.32"/>
        <environment name="BOOST_VERSION" value="boost-1_32"/>
        <environment name="BOOST_CFG"     value="-vc71-mt" />
        <environment name="BOOST_STATIC_LIB_PREFIX" value="lib" />
        <environment name="ZLIB_ROOT"       value="C:\zlib" />
        <environment name="ZZIP_ROOT"       value="C:\zzip" />
        <variable name="configs"  value="Win32 VC71_W4 Exceptions" />
        <variable name="root"     value="C:\ACE\VC71_W4\" />
        <variable name="log_file" value="log.txt" />
        <variable name="log_root" value="C:\ACE\VC71_W4\" />
        <variable name="name"     value="WinXP VS.NET 2003" />
    </configuration>

    <command name="status"            options="on" />

    <command name="process_listener"  options="localhost:32002" />

    <command name="win32make"         options="msvc_mpc_auto_compile -vc7 -ALL -Debug -k -clean" />

    <command name="shell"             options="c:\cygwin\bin\find ACE_wrappers -name '*.vcproj*' -exec c:\cygwin\bin\rm '{}' ';'" />
    <command name="shell"             options="c:\cygwin\bin\find ACE_wrappers -name '*.sln*' -exec c:\cygwin\bin\rm '{}' ';'" />

    <command name="log"               options="ON" />

    <command name="svn"               options="co svn://svn.dre.vanderbilt.edu/DOC/Middleware/sets-anon/ACE+TAO+CIAO ." />

    <command name="file_manipulation" options="type=create file=ACE_wrappers/ace/config.h output='#define ACE_DISABLE_WIN32_ERROR_WINDOWS\n#include \x22ace/config-win32.h\x22\n'" />
    <command name="file_manipulation" options="type=create file=ACE_wrappers/bin/MakeProjectCreator/config/default.features output='rwho=0\nqos=1\nboost=0\ncidl=0\nzlib=0\nzzip=0\n'" />

    <command name="anonymous_shell"   options="scp bczar@naboo.dre.vanderbilt.edu:/web/users/isisbuilds/CIDLC/gnu-linux/cidlc ACE_wrappers/TAO/CIAO/bin" />

    <command name="print_os_version" />
    <command name="check_compiler" options="msvc" />
    <command name="print_ace_config" />
    <command name="print_cidlc_version" />
    <command name="print_perl_version" />

    <command name="generate_workspace" options="-value_template warning_level=4 -type vc71 -exclude TAO/TAO_*.mwc,TAO/CIAO" />

    <command name="win32make"         options="msvc_mpc_auto_compile -vc71 -ALL -Debug -k" />

    <!-- Explicitly enable ACE, TAO and orbsvcs tests so that CIAO tests aren't added automatically -->
    <command name="auto_run_tests"    options="-s \x22C:\ACE\autobuild\win32sandbox\sandbox.exe -t 5400000\x22 -a -o -t" />

    <command name="log"               options="OFF" />
    <command name="process_logs"      options="move prettify clean=5 index" />
    <command name="status"            options="off" />
    <command name="anonymous_shell"   options="rsync --progress --recursive --delete -z --exclude /ACE_wrappers*/ --include  *.txt --include *.html --exclude ** ./  bczar@naboo.dre.vanderbilt.edu:/web/users/isisbuilds/auto_compile_logs/isislab/blade17/vc71_w4"/>
</autobuild>
