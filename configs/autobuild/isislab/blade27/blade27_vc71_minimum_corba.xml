<!-- $Id$ -->
<autobuild>
    <configuration>
        <environment name="ACE_ROOT"     value="C:\ACE\VC71\Minimum\ACE_wrappers" />
        <environment name="TAO_ROOT"     value="C:\ACE\VC71\Minimum\ACE_wrappers\TAO"/>
        <environment name="CVSROOT"      value=":ext:isisbuilds@cvs.doc.wustl.edu:/project/cvs-repository" />
        <environment name="CVS_RSH"      value="c:\Cygwin\bin\ssh" />
        <environment name="RSYNC_RSH"    value="c:\Cygwin\bin\ssh" />
        <environment name="PATH"         value="C:\Program Files\Microsoft Visual Studio .NET 2003\Common7\IDE;C:\Program Files\Microsoft Visual Studio .NET 2003\Vc7\bin;C:\ACE\VC71\Minimum\ACE_wrappers\bin;C:\ACE\VC71\Minimum\ACE_wrappers\lib;c:\xerces\bin;C:\Perl\bin;c:\zzip\lib;c:\zlib\lib;c:\putty:c:\xerces\lib:c:\cygwin\bin"  type="prefix" />

        <variable name="configs"  value="Win32 VC71 Exceptions MINIMUM" />
        <variable name="root"     value="C:\ACE\VC71\Minimum" />
        <variable name="log_file" value="log.txt" />
        <variable name="log_root" value="C:\ACE\VC71\Minimum" />
        <variable name="name"     value="WinXP VS.NET 2003" />
    </configuration>

    <command name="status"            options="on" />

    <command name="process_listener"  options="localhost:32001" />

    <command name="win32make"         options="msvc_mpc_auto_compile -vc7 -ALL -Debug -k -clean" />

    <command name="shell"             options="c:\cygwin\bin\find ACE_wrappers -name '*.vcproj*' -exec c:\cygwin\bin\rm '{}' ';'" />
    <command name="shell"             options="c:\cygwin\bin\find ACE_wrappers -name '*.sln*' -exec c:\cygwin\bin\rm '{}' ';'" />

    <command name="log"               options="ON" />

    <command name="cvs"               options="-z9 -q checkout -P ACE_wrappers" />
    <command name="cvs"               options="-z9 -q checkout -P ACE_MPC"/>

    <command name="file_manipulation" options="type=create file=ACE_wrappers/ace/config.h output='#define ACE_DISABLE_WIN32_ERROR_WINDOWS\n#define ACE_HAS_STANDARD_CPP_LIBRARY 1\n#include \x22ace/config-win32.h\x22\n'" />
    <command name="file_manipulation" options="type=create file=ACE_wrappers/bin/MakeProjectCreator/config/default.features output='minimum_corba=1\nzzip=0\n'" />

    <command name="print_os_version" />
    <command name="check_compiler" options="msvc" />
    <command name="print_ace_config" />
    <command name="print_cidlc_version" />
    <command name="print_perl_version" />

    <command name="generate_workspace" options="-type vc71 -recurse -exclude TAO/TAO_*.mwc,TAO/CIAO"/>

    <command name="win32make"         options="msvc_mpc_auto_compile -vc7 -ALL -Debug -k" />

    <command name="auto_run_tests"    options="-s \x22C:\ACE\autobuild\win32sandbox\sandbox.exe -a -o 5400000\x22"/>

    <command name="log"               options="OFF" />

    <command name="process_logs"      options="move prettify clean index" />
    <command name="status"            options="off" />
    <command name="shell"             options="c:\cygwin\bin\rsync --progress --recursive --delete -z --exclude /ACE_wrappers/ --include *.txt --include *.html --exclude ** ./ bczar@naboo.dre.vanderbilt.edu:/web/users/isisbuilds/auto_compile_logs/isislab/blade27" />
</autobuild>
