<!-- $Id$ -->
<autobuild>
    <configuration>
        <environment name="ACE_ROOT"      value="C:\ACE\VC71\ACE_wrappers" />
        <environment name="TAO_ROOT"      value="C:\ACE\VC71\ACE_wrappers\TAO"/>
        <environment name="CIAO_ROOT"     value="C:\ACE\VC71\ACE_wrappers\TAO\CIAO"/>
        <environment name="RSYNC_RSH"     value="c:\Cygwin\bin\ssh" />
        <environment name="PATH"          value="C:\bin;C:\zlib\lib;C:\zzip\lib;c:\xerces-vc8\lib;C:\Perl\bin;C:\Program Files\Microsoft Visual Studio .NET 2003\Common7\IDE;C:\Program Files\Microsoft Visual Studio .NET 2003\Vc7\bin;C:\ACE\VC71\ACE_wrappers\bin;C:\ACE\VC71\ACE_wrappers\lib;C:\ACE\VC71\ACE_wrappers\TAO\CIAO\bin;c:\cygwin\bin" type="prefix" />
        <environment name="XERCESCROOT"   value="C:\xerces-vc8" />
        <environment name="ZLIB_ROOT"     value="C:\zlib" />
        <environment name="ZZIP_ROOT"     value="C:\zzip" />


        <variable name="configs"    value="Win32 VC71 ZLIB" />
        <variable name="root"       value="C:\ACE\VC71\" />
        <variable name="log_file"   value="log.txt" />
        <variable name="log_root"   value="C:\ACE\VC71\" />
        <variable name="name"       value="WinXP VC71 Full" />
    </configuration>

    <command name="status"            options="on" />
    <command name="process_listener"  options="localhost:32001" />

    <command name="win32make"         options="msvc_mpc_auto_compile -vc7 -ALL -Debug -k -clean" />

    <command name="log"               options="ON" />

    <command name="svn"               options="co svn://svn.dre.vanderbilt.edu/DOC/Middleware/sets-anon/ACE+TAO+CIAO ." />

    <command name="file_manipulation" options="type=create file=ACE_wrappers/ace/config.h output='#define ACE_DISABLE_WIN32_ERROR_WINDOWS\n#define ACE_HAS_STANDARD_CPP_LIBRARY 1\n#include \x22ace/config-win32.h\x22\n'" />
    <command name="file_manipulation" options="type=create file=ACE_wrappers/bin/MakeProjectCreator/config/default.features output='rwho=0\nqos=1\nzlib=1\nzzip=1\n'" />

    <command name="anonymous_shell"   options="scp -C bczar@naboo.dre.vanderbilt.edu:/web/users/isisbuilds/CIDLC/Win32/cidlc.exe ACE_wrappers/TAO/CIAO/bin" />
    <command name="anonymous_shell"   options="chmod 777 ACE_wrappers/TAO/CIAO/bin/cidlc.exe" />

    <command name="print_os_version" />
    <command name="check_compiler" options="msvc" />
    <command name="print_ace_config" />
    <command name="print_cidlc_version" />
    <command name="print_perl_version" />

    <command name="generate_workspace" options="-type vc71"/>

    <command name="win32make"         options="msvc_mpc_auto_compile -vc7 -ALL -Debug -k" />

    <command name="auto_run_tests"    options="-s \x22C:\ACE\autobuild\win32sandbox\sandbox.exe -t 5400000\x22" />

    <command name="log"               options="OFF" />
    <command name="process_logs"      options="move prettify clean=5 index" />
    <command name="status"            options="off" />
    <command name="shell"             options="c:\cygwin\bin\rsync --progress --recursive --delete -z --exclude /ACE_wrappers*/ --include *.txt --include *.html --exclude ** ./ bczar@naboo.dre.vanderbilt.edu:/web/users/isisbuilds/auto_compile_logs/blade49_vc71_full" />
</autobuild>
