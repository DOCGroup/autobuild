<!-- $Id$ -->
<autobuild>
 <configuration>

  <environment name="ACE_ROOT"         value="/home/dhinton/development/ace/autobuilds/no-rtcorba_no-messaging/ACE_wrappers/build/nrt" />
  <environment name="TAO_ROOT"         value="/home/dhinton/development/ace/autobuilds/no-rtcorba_no-messaging/ACE_wrappers/build/nrt/TAO"/>
  <environment name="LD_LIBRARY_PATH"  value="/usr/local/lib:/home/dhinton/development/ace/autobuilds/no-rtcorba_no-messaging/ACE_wrappers/build/nrt/lib" />
  <environment name="CVSROOT"          value=":ext:dhinton@cvs.doc.wustl.edu:/project/cvs-repository" />
  <environment name="CVS_RSH"          value="ssh" />

  <variable name="root"                value="/home/dhinton/development/ace/autobuilds/no-rtcorba_no-messaging" />
  <variable name="build_name"          value="nrt" />
  <variable name="project_root"        value="/home/dhinton/development/ace/autobuilds/no-rtcorba_no-messaging/ACE_wrappers/build/nrt" />
  <variable name="log_file"            value="build.txt" />
  <variable name="log_root"            value="/home/svn/www/autobuild_output/no-rtcorba_no-messaging" />
  <variable name="configs"             value="Linux" />

 </configuration>

 <command name="status"                options="on" />
 <command name="file_manipulation"     options="type=mustnotexist file=.disable" />
 <command name="file_manipulation"     options="type=create file=.disable output='RUNNING'" />
 <command name="log"                   options="on" />
 <command name="cvs"                   options="-z9 -q checkout ACE_wrappers" />

 <command name="create_ace_build"      options="" />

 <command name="file_manipulation"     options="type=create file=ACE_wrappers/build/nrt/ace/config.h output='#include \x22ace/config-linux.h\x22\n#define TAO_HAS_CORBA_MESSAGING 0\n#define TAO_HAS_RT_CORBA 0\n'" />

 <command name="file_manipulation"     options="type=create file=ACE_wrappers/build/nrt/include/makeinclude/platform_macros.GNU output='include $(ACE_ROOT)/include/makeinclude/platform_linux.GNU\ndebug=0\n'" />

 <command name="file_manipulation"     options="type=create file=ACE_wrappers/build/nrt/build.mwc output='workspace {\n ace\n ACEXML\n netsvcs\n tests\n apps\n examples\n performance-tests\n websvcs\n Kokyu\n}\n'" />

 <command name="file_manipulation"     options="type=create file=ACE_wrappers/build/nrt/TAO/build.mwc output='workspace {\n tao\n TAO_IDL\n tests\n orbsvcs\n examples\n performance-tests\n utils\n docs/tutorials/Quoter\n}\n'" />

 <command name="print_os_version" />
 <command name="check_compiler"        options="gcc" />
 <command name="check_linker"          options="ld" />
 <command name="print_ace_config" />

 <!-- clean up all the libs just in case they get moved around by an evolving MPC ;-) -->
 <command name="shell"                 options="find . -name \x22lib*\x22 -exec rm '{}' ';'" />

 <command name="generate_workspace"    options="build.mwc -make_coexistence" />
 <command name="generate_workspace"    options="TAO/build.mwc -make_coexistence" />

 <!-- ACE -->
 <command name="make"                  options="-k realclean rmcast=1" />
 <command name="make"                  options="-k rmcast=1" />

 <!-- TAO -->
 <command name="make"                  options="-k -C TAO realclean rmcast=1" />
 <command name="make"                  options="-k -C TAO rmcast=1" />

 <!--<command name="auto_run_tests"        options="" />-->

 <command name="log"                   options="off" />
 <command name="process_logs"          options="clean move prettify index" />
 <command name="file_manipulation"     options="type=delete file=.disable" />
 <command name="status"                options="off" />
</autobuild>
