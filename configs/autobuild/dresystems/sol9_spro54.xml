<autobuild>
 <configuration>

  <environment name="ACE_ROOT"         value="/export/home/dhinton/development/ace/autobuilds/sol9_spro54/ACE_wrappers/build/sol9_spro54_ace" />
  <environment name="LD_LIBRARY_PATH"  value="/usr/local/lib:/export/home/dhinton/development/ace/autobuilds/sol9_spro54/ACE_wrappers/build/sol9_spro54_ace/lib" />
  <environment name="CVSROOT"          value=":ext:dhinton@cvs.doc.wustl.edu:/project/cvs-repository" />
  <environment name="CVS_RSH"          value="ssh" />

  <variable name="root"                value="/export/home/dhinton/development/ace/autobuilds/sol9_spro54" />
  <variable name="build_name"          value="sol9_spro54_ace" />
  <variable name="project_root"        value="/export/home/dhinton/development/ace/autobuilds/sol9_spro54/ACE_wrappers/build/sol9_spro54_ace" />
  <variable name="log_file"            value="build.txt" />
  <variable name="log_root"            value="/export/home/dhinton/public_html/autobuild_output/sol9_spro54_ace" />
  <variable name="configs"             value="Linux" />

 </configuration>

 <command name="status"                options="on" />
 <command name="file_manipulation"     options="type=mustnotexist file=.disable" />
 <command name="file_manipulation"     options="type=create file=.disable output='RUNNING'" />
 <command name="log"                   options="on" />
 <command name="cvs"                   options="-z9 -q checkout ACE_wrappers" />

 <command name="create_ace_build"      options="" />

 <command name="file_manipulation"     options="type=create file=ACE_wrappers/build/sol9_spro54_ace/ace/config.h output='#include \x22ace/config-sunos5.9.h\x22\n'" />

 <command name="file_manipulation"     options="type=create file=ACE_wrappers/build/sol9_spro54_ace/include/makeinclude/platform_macros.GNU output='include $(ACE_ROOT)/include/makeinclude/platform_sunos5_sunc++.GNU\ndebug=0\nexceptions=1\noptimize=0\nrmcast=1'" />

 <command name="file_manipulation"     options="type=create file=ACE_wrappers/build/sol9_spro54_ace/build.mwc output='workspace {\n ace\n ACEXML\n netsvcs\n tests\n apps\n examples\n performance-tests\n websvcs\n Kokyu\n}\n'" />

 <command name="print_os_version" />
 <command name="check_compiler"        options="sun_cc" />
 <command name="print_ace_config" />

 <!-- clean up all the libs just in case they get moved around by an evolving MPC ;-) -->
 <command name="shell"                 options="find . -name \x22lib*\x22 -exec rm '{}' ';'" />

 <command name="generate_workspace"    options="build.mwc -make_coexistence" />

 <!-- ACE -->
 <command name="make"                  options="-k realclean" />
 <command name="make"                  options="-k" />

 <command name="log"                   options="off" />
 <command name="process_logs"          options="clean move prettify index" />
 <command name="file_manipulation"     options="type=delete file=.disable" />
 <command name="status"                options="off" />
</autobuild>

