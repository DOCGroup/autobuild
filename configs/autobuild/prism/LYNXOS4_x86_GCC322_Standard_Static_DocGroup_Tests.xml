<autobuild>
    <configuration>
        <environment name="ACE_ROOT" value="/home/tao/doc_scoreboard/lynxos_x86_gcc322/standard/static/ACE_wrappers" />
        <environment name="TAO_ROOT" value="/home/tao/doc_scoreboard/lynxos_x86_gcc322/standard/static/ACE_wrappers/TAO" />
        <environment name="MPC_ROOT" value="/home/tao/doc_scoreboard/lynxos_x86_gcc322/standard/static/ACE_wrappers/MPC" />
        <environment name="LD_LIBRARY_PATH" value="/home/tao/doc_scoreboard/lynxos_x86_gcc322/standard/static/ACE_wrappers/ace:/home/tao/doc_scoreboard/lynxos_x86_gcc322/standard/static/ACE_wrappers/lib:/usr/lib:/lib:." />
        <variable name="configs"  value="LynxOS STATIC NO_IFR DISABLE_ToFix_LynxOS_x86 PRISM_ONLY" />
        <environment name="PATH" value="/usr/i386-elf-lynxos/bin:/usr/local/bin:/usr/bin:/usr/local/sbin:." />
        <environment name="ENV_PREFIX" value="/usr/local/lynx/4.0.0/x86-322" />
        <environment name="ACE_RUNTEST_DELAY" value="2" />

        <variable name="root" value="/home/tao/doc_scoreboard/lynxos_x86_gcc322/standard/static" />
        <variable name="log_file" value="DOC_Group_test_results_run_on_lynxosx86.txt" />
        <variable name="project_root" value="/home/tao/doc_scoreboard/lynxos_x86_gcc322/standard/static/ACE_wrappers" />
    </configuration>

    <command name="file_manipulation" options="type=mustnotexist file=.disable" />
    <command name="file_manipulation" options="type=create file=.disable output='RUNNING'" />
    <command name="log"               options="ON" />

    <command name="print_os_version" />
    <command name="check_compiler" options="gcc" />
    <command name="print_ace_config" />

    <!-- The following auto_run_tests were split up due to resourcing issues -->
    <command name="auto_run_tests"    options=" -a" />
    <command name="auto_run_tests"    options=" -o" />
    <command name="auto_run_tests"    options=" -t" />

    <command name="file_manipulation" options="type=delete file=.disable" />
</autobuild>
