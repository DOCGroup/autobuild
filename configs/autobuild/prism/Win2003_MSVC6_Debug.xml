<!-- $Id$ -->
<autobuild>
    <configuration>
        <environment name="ACE_ROOT"  value="F:\Overnight_Builds\DOC_group\CL60\debug\ACE_wrappers" />
        <environment name="TAO_ROOT"  value="F:\Overnight_Builds\DOC_group\CL60\debug\ACE_wrappers\TAO"/>
        <environment name="CIAO_ROOT" value="F:\Overnight_Builds\DOC_group\CL60\debug\ACE_wrappers\TAO\CIAO"/>
        <environment name="Path"      value="F:\Overnight_Builds\DOC_group\CL60\debug\ACE_wrappers\bin;F:\Overnight_Builds\DOC_group\CL60\debug\ACE_wrappers\lib;C:\Perl\bin;C:\cygwin\bin" type="prefix" />
        <environment name="CVSROOT"   value=":ext:sma@cvs.doc.wustl.edu:/project/cvs-repository" />
        <environment name="CVS_RSH"   value="C:\cygwin\bin\ssh" />
        <environment name="RSYNC_RSH" value="C:\cygwin\bin\ssh" />

        <variable name="root"         value="F:\Overnight_Builds\DOC_group\CL60\debug" />
        <variable name="log_file"     value="log.txt" />
        <variable name="log_root"     value="T:\doc-scoreboard\windows\MSVC6_Debug" />
        <variable name="name"         value="WinXP DLL Debug" />
        <variable name="configs"      value="Win32 SANDBOX Exceptions" />
    </configuration>

    <command name="status"            options="on" />
    <command name="file_manipulation" options="type=mustnotexist file=.disable" />
    <command name="file_manipulation" options="type=create file=.disable output='RUNNING'" />

    <command name="win32make"         options="msvc_mpc_auto_compile -ALL -Debug -k -clean" />

    <command name="log"               options="ON" />

    <command name="shell"             options="find ACE_wrappers -iname '*.ds[pw]' -exec rm '{}' ';'" />
    <command name="cvs"               options="-z 9 -q checkout -P ACE_wrappers" />
    <command name="cvs"               options="-z 9 -q checkout -P ACE_MPC"/>

    <command name="file_manipulation" options="type=create file=ACE_wrappers/ace/config.h output='#define ACE_DISABLE_WIN32_ERROR_WINDOWS\n#include \x22ace/config-win32.h\x22\n'" />
    <command name="file_manipulation" options="type=create file=ACE_wrappers/bin/MakeProjectCreator/config/default.features output='cidl=0\nrwho=0\nqos=1\nmfc=1\nssl=0\n'" />

    <command name="print_os_version" />
    <command name="check_compiler" options="msvc" />
    <command name="print_ace_config" />
    <command name="print_perl_version" />

    <command name="generate_workspace" options="-exclude TAO/CIAO -recurse -type vc6" />

    <command name="win32make"         options="msvc_mpc_auto_compile -ALL -Debug -k" />

    <command name="auto_run_tests"    options="-s \x22F:\Overnight_Builds\DOC_group\autobuild\win32sandbox\sandbox.exe -t 5400000\x22" />

    <command name="log"               options="OFF" />
    <command name="process_logs"      options="move prettify clean=5 index" />
    <command name="file_manipulation" options="type=delete file=.disable" />
    <command name="status"            options="off" />
</autobuild>
