<autobuild>
    <configuration>
        <environment name="ACE_ROOT" value="/usr/users/tao/ultra5/doc_scoreboard/lynxos_x86_gcc322/standard/static/ACE_wrappers" />
        <environment name="TAO_ROOT" value="/usr/users/tao/ultra5/doc_scoreboard/lynxos_x86_gcc322/standard/static/ACE_wrappers/TAO" />
        <environment name="MPC_ROOT" value="/usr/users/tao/ultra5/doc_scoreboard/lynxos_x86_gcc322/standard/static/ACE_wrappers/MPC" />
        <environment name="LD_LIBRARY_PATH" value="/usr/local/gcc-3.2.2/lib:/usr/users/tao/ultra5/doc_scoreboard/native_gcc322/standard/static/ACE_wrappers/ace" />
        <environment name="CVSROOT"         value=":ext:sma@cvs.doc.wustl.edu:/project/cvs-repository" />
        <environment name="CVS_RSH"         value="ssh" />
        <environment name="PATH" value="/usr/users/tao/ultra5/doc_scoreboard/native_x86_gcc322/standard/static/ACE_wrappers/bin:/usr/local/lynx/4.0.0/x86/cdk/sunos-elf-x86/bin:/usr/local/lynx/4.0.0/x86/cdk/sunos-elf-x86/usr/bin:/usr/local/bin:/usr/bin:/usr/sbin:/opt/bin:/usr/openwin/bin:/usr/X/bin:/usr/ccs/bin:/usr/ucb:." />
        <environment name="ENV_PREFIX" value="/usr/local/lynx/4.0.0/x86-322" />
        <environment name="TAO_IDL" value="/usr/users/tao/ultra5/doc_scoreboard/native_gcc322/standard/static/ACE_wrappers/bin/tao_idl" />

        <variable name="root" value="/usr/users/tao/ultra5/doc_scoreboard/lynxos_x86_gcc322/standard/static" />
        <variable name="sam_root" value="/usr/users/tao/ultra5/doc_scoreboard/lynxos_x86_gcc322/standard/static/ACE_wrappers/samwise" />
        <variable name="log_file" value="build.txt" />
        <variable name="log_root" value="/var/tao1/taoperf/htdocs/doc-scoreboard/lynxos_x86/cross_compile_build" />
        <variable name="project_root" value="/usr/users/tao/ultra5/doc_scoreboard/lynxos_x86_gcc322/standard/static/ACE_wrappers" />

    </configuration>

    <command name="status"            options="on" />
    <command name="file_manipulation" options="type=mustnotexist file=.disable" />
    <command name="file_manipulation" options="type=create file=.disable output='RUNNING'" />
    <command name="log"               options="ON" />

    <!-- Delete all the Makefiles so the cvs checkout won't complain. -->
    <command name="shell"             options="find ACE_wrappers -name 'Makefile*' -exec rm '{}' ';'" />
    <command name="cvs"               options="-z 3 -q checkout -P ACE_wrappers" />
    <command name="cvs"               options="-z 3 -q checkout -P ACE_MPC"/>

    <!-- Now, delete all the Makefiles to insure a clean slate. -->
    <command name="shell"             options="find ACE_wrappers -name 'Makefile*' -exec rm '{}' ';'" />

    <command name="file_manipulation" options="type=create file=ACE_wrappers/ace/config.h output='#include \x22ace/config-lynxos.h\x22\n'" />
    <command name="file_manipulation" options="type=create file=ACE_wrappers/include/makeinclude/platform_macros.GNU output='OBJSFORMAT=elf\ndebug=0\nstatic_libs_only=1\ninclude $(ACE_ROOT)/include/makeinclude/platform_lynxos.GNU\ninline=0\nCXX=c++\nTAO_IDL:=/usr/users/tao/ultra5/doc_scoreboard/native_gcc322/standard/static/ACE_wrappers/bin/tao_idl\nTAO_IDLFLAGS+=-g /usr/users/tao/ultra5/doc_scoreboard/native_gcc322/standard/static/ACE_wrappers/bin/gperf\n'" />

    <command name="print_os_version" />
    <command name="check_compiler" options="gcc" />
    <command name="print_ace_config" />

    <!-- Uses ACE.mwc -->
    <command name="generate_workspace" options="-exclude TAO,build.mwc,prism" />
    <!-- Uses TAO/TAO.mwc -->
    <command name="generate_workspace" options="dirs=TAO -exclude TAO_*.mwc,CIAO,oldCIAO" />
    <!-- And the prism extras -->
    <!-- command name="generate_workspace" options="dirs=prism/tests" / -->

    <command name="make"              options="-k realclean" />
    <command name="make"              options="-k -C TAO realclean" />
    <!-- command name="make"              options="-k -C prism/tests realclean" / -->
    <command name="make"              options="-k" />
    <command name="make"              options="-k -C TAO" />
    <!-- command name="make"              options="-k -C prism/tests" / -->

    <command name="process_logs"      options="clean" />
    <command name="log"               options="OFF" />
    <command name="process_logs"      options="clean move prettify index" />
    <command name="file_manipulation" options="type=delete file=.disable" />
    <command name="status"            options="off" />

</autobuild>
