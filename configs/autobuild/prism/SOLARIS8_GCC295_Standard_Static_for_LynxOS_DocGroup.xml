<autobuild>
    <configuration>

        <environment name="ACE_ROOT" value="/usr/users/tao/ultra5/doc_scoreboard/TAO14/gcc295/standard/static/ACE_wrappers" />
        <environment name="MPC_ROOT" value="/usr/users/tao/ultra5/doc_scoreboard/TAO14/gcc295/standard/static/MPC" />
        <environment name="LD_LIBRARY_PATH" value="/usr/local/gcc-2.95.3/lib:/usr/users/tao/ultra5/doc_scoreboard/TAO14/gcc295/standard/static/ACE_wrappers/ace" />
        <environment name="CVSROOT"         value=":ext:sma@cvs.doc.wustl.edu:/project/cvs-repository" />
        <environment name="CVS_RSH"         value="ssh" />
        <environment name="PATH" value="/usr/local/gcc-2.95.3/bin:/usr/local/bin:/usr/bin:/opt/bin:/usr/openwin/bin:/usr/X/bin:/usr/ccs/bin:/usr/ucb:." />

        <variable name="root" value="/usr/users/tao/ultra5/doc_scoreboard/TAO14/gcc295/standard/static" />
        <variable name="sam_root" value="/usr/users/tao/ultra5/doc_scoreboard/TAO14/gcc295/standard/static/ACE_wrappers/samwise" />
        <variable name="log_file" value="build.txt" />
        <variable name="log_root" value="/var/tao1/taoperf/htdocs/scoreboard/ultra5/tao14/gcc295/standard_static" />
        <variable name="MAIL_ADMIN" value="sma@ultra5.prismtechnologies.com" />
        <variable name="project_root" value="ACE_wrappers" />

    </configuration>

    <command name="status"            options="on" />
    <command name="file_manipulation" options="type=mustnotexist file=.disable" />
    <command name="file_manipulation" options="type=create file=.disable output='RUNNING'" />
    <command name="log"               options="ON" />

    <command name="file_manipulation" options="type=rmtree file=ACE_wrappers" />
    <command name="file_manipulation" options="type=rmtree file=MPC" />
    <command name="cvs"               options="-z 3 -q checkout -P ACE_wrappers" />
    <command name="cvs"               options="-z 3 -q checkout -P MPC"/>
    <!-- Now, delete all the Makefiles to insure a clean slate. -->
    <command name="shell"             options="find ACE_wrappers -name 'Makefile*' -exec rm '{}' ';'" />

    <command name="file_manipulation" options="type=create file=ACE_wrappers/ace/config.h output='#include \x22ace/config-sunos5.8.h\x22\n'" />
    <command name="file_manipulation" options="type=create file=ACE_wrappers/include/makeinclude/platform_macros.GNU output='optimize=1\nexceptions=1\ndebug=0\nthreads=1\nstatic_libs_only=1\n\ninclude $(ACE_ROOT)/include/makeinclude/platform_sunos5_g++.GNU\ninline=0\n'" />

    <command name="print_os_version" />
    <command name="check_compiler" options="gcc" />
    <command name="print_ace_config" />

    <!-- Uses ace.mwc -->
    <command name="generate_workspace" options="-exclude TAO,vc7_nightly.mwc,build.mwc -hierarchy -make_coexistence" />
    <!-- Uses TAO/TAO.mwc -->
    <command name="generate_workspace" options="dirs=TAO -exclude TAOACE.mwc,CIAO -hierarchy -make_coexistence" />

    <command name="shell"             options="ACE_wrappers/bin/ace_components --orbsvcs --set Naming,ImplRepo,IFRService" />
    <command name="make"              options="-k -C ace" />
    <command name="make"              options="-k -C apps/gperf/src" />
    <command name="make"              options="-k -C TAO/TAO_IDL" />

    <command name="process_logs"      options="clean" />
    <command name="log"               options="OFF" />
    <command name="process_logs"      options="clean move prettify index" />
    <command name="file_manipulation" options="type=delete file=.disable" />
    <command name="status"            options="off" />

</autobuild>
