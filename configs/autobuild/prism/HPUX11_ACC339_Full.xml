<autobuild>
    <configuration>

        <environment name="ACE_ROOT"        value="/usr/users/tao/hp1/doc_scoreboard/Full/ACE_wrappers" />
        <environment name="LD_LIBRARY_PATH" value="/usr/users/tao/hp1/doc_scoreboard/Full/ACE_wrappers/ace" />
        <environment name="CVSROOT"         value=":ext:eas@cvs.doc.wustl.edu:/project/cvs-repository" />
        <environment name="CVS_RSH"         value="ssh" />
        <environment name="PATH"            value="/usr/users/tao/hp1/bin:/usr/local/opt/aCC/bin:/usr/bin:/opt/ansic/bin:/usr/ccs/bin:/usr/contrib/bin:/usr/local/bin" />

        <variable name="root"     value="/usr/users/tao/hp1/doc_scoreboard/Full" />
        <variable name="sam_root" value="/usr/users/tao/hp1/doc_scoreboard/Full/ACE_wrappers/samwise" />
        <variable name="log_file" value="build.txt" />
        <variable name="log_root" value="/var/tao1/taoperf/htdocs/doc-scoreboard" />
        <variable name="configs"  value="HPUX11.11 aCC 3.39" />
        <variable name="MAIL_ADMIN" value="eas@prismtechnologies.com" />

    </configuration>

    <command name="status"            options="on" />
    <command name="file_manipulation" options="type=mustnotexist file=.disable" />
    <command name="file_manipulation" options="type=create file=.disable output='RUNNING'" />
    <command name="log"               options="ON" />
    <command name="cvs"               options="-z 3 -q checkout -P ACE_wrappers" />

    <command name="file_manipulation" options="type=create file=ACE_wrappers/ace/config.h output='#include \x22ace/config-hpux-11.00.h\x22\n'" />
    <command name="file_manipulation" options="type=create file=ACE_wrappers/include/makeinclude/platform_macros.GNU output='include $(ACE_ROOT)/include/makeinclude/platform_hpux_aCC.GNU\n'" />
    <command name="print_os_version" />
    <command name="check_compiler" options="aCC" />
    <command name="print_ace_config" />

    <command name="make"              options="-k reverseclean" />
    <command name="make"              options="-k -C TAO reverseclean" />
    <command name="make"              options="-k" />
    <command name="make"              options="-k -C TAO" />

    <command name="auto_run_tests"    options="" />
    <command name="process_logs"      options="clean" />
    <command name="log"               options="OFF" />
    <command name="process_logs"      options="clean move prettify index" />
    <command name="file_manipulation" options="type=delete file=.disable" />
    <command name="status"            options="off" />

</autobuild>
