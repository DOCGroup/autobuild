<!-- $Id$ -->

<autobuild>
    <configuration>
        <environment name="ACE_ROOT" value="/usr/users/tao/doc_scoreboard/lynxos_ppc_gcc322/standard/dynamic/ACE_wrappers" />
        <environment name="TAO_ROOT" value="/usr/users/tao/doc_scoreboard/lynxos_pcc_gcc322/standard/dynamic/ACE_wrappers/TAO" />
        <environment name="MPC_ROOT" value="/usr/users/tao/doc_scoreboard/lynxos_pcc_gcc322/standard/dynamic/ACE_wrappers/MPC" />
        <environment name="LD_LIBRARY_PATH" value="/usr/users/tao/doc_scoreboard/lynxos_ppc_gcc322/standard/dynamic/ACE_wrappers/ace:/usr/users/tao/doc_scoreboard/lynxos_ppc_gcc322/standard/dynamic/ACE_wrappers/lib:/usr/lib:/lib:." />
        <variable name="configs"  value="LynxOS NO_IFR DISABLE_ToFix_LynxOS_PPC PRISM_ONLY" />
        <environment name="PATH" value="/usr/ppc-elf-lynxos/bin:/usr/local/bin:/usr/bin:/usr/local/sbin:/bin:." />
        <environment name="ENV_PREFIX" value="/usr/local/lynx/4.0.0/ppc-322" />
        <environment name="ACE_RUNTEST_DELAY" value="4" />

        <variable name="root" value="/usr/users/tao/doc_scoreboard/lynxos_ppc_gcc322/standard/dynamic" />
        <variable name="log_file" value="DOC_Group_test_results_run_on_rad1.txt" />
        <variable name="project_root" value="/usr/users/tao/doc_scoreboard/lynxos_ppc_gcc322/standard/dynamic/ACE_wrappers" />
    </configuration>

    <command name="file_manipulation" options="type=mustnotexist file=.disable" />
    <command name="file_manipulation" options="type=create file=.disable output='RUNNING'" />
    <command name="log"               options="ON" />

    <command name="print_os_version" />
    <command name="check_compiler" options="gcc" />
    <command name="print_ace_config" />

    <!-- The following auto_run_tests were split due to resourcing problems -->
    <command name="auto_run_tests"    options=" -a" />
    <command name="auto_run_tests"    options=" -o" />
    <command name="auto_run_tests"    options=" -t" />

    <command name="log"               options="OFF" />
    <command name="file_manipulation" options="type=delete file=.disable" />
</autobuild>
