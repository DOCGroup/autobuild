<!-- $Id$ -->
<autobuild>
    <configuration>
        <environment name="ACE_ROOT"     value="C:\ACE\VC71\ACE_wrappers" />
        <environment name="TAO_ROOT"     value="C:\ACE\VC71\ACE_wrappers\TAO"/>
        <environment name="CIAO_ROOT"     value="C:\ACE\VC71\ACE_wrappers\TAO\CIAO"/>
        <environment name="CVSROOT"      value=":ext:isisbuilds@cvs.doc.wustl.edu:/project/cvs-repository" />
        <environment name="CVS_RSH"      value="C:\bin\plink.exe" />
        <environment name="PATH"         value="C:\WINDOWS\system32;C:\WINDOWS;C:\WINDOWS\System32\Wbem;C:\Program Files\Microsoft Visual Studio .NET 2003\Common7\IDE;C:\Program Files\Microsoft Visual Studio .NET 2003\Vc7\bin;C:\ACE\VC71\ACE_wrappers\bin;C:\ACE\VC71\ACE_wrappers\lib;c:\xerces-c-2_5_0-win32-vc7\bin;C:\Perl\bin;c:\zzip\lib;c:\zlib\lib" type="prefix" />
        <environment name="XERCESCROOT"   value="C:\xerces-c-2_5_0-win32-vc7"/>
        <environment name="BOOST_ROOT"    value="C:\Boost-1.32"/>
        <environment name="BOOST_VERSION" value="boost-1_32"/>
        <environment name="BOOST_CFG"     value="-vc71-mt" />
        <environment name="BOOST_STATIC_LIB_PREFIX" value="lib" />
        <environment name="ZLIB_ROOT"       value="C:\zlib" />
        <environment name="ZZIP_ROOT"       value="C:\zzip" />
        <variable name="configs"  value="Win32 VC71 Exceptions" />
        <variable name="root"     value="C:\ACE\VC71\" />
        <variable name="log_file" value="log.txt" />
        <variable name="log_root" value="C:\ACE\VC71\" />
        <variable name="name"     value="WinXP VS.NET 2003" />
    </configuration>

    <command name="status"            options="on" />
    <command name="file_manipulation" options="type=mustnotexist file=.disable" />
    <command name="file_manipulation" options="type=create file=.disable output='RUNNING'" />

    <command name="win32make"         options="msvc_mpc_auto_compile -vc7 -ALL -Debug -k -clean" />

    <command name="log"               options="ON" />

    <command name="cvs"               options="-z9 -q checkout -P ACE_wrappers" />
    <command name="cvs"               options="-z9 -q checkout -P ACE_MPC"/>

    <command name="file_manipulation" options="type=create file=ACE_wrappers/ace/config.h output='#define ACE_DISABLE_WIN32_ERROR_WINDOWS\n#define ACE_HAS_STANDARD_CPP_LIBRARY 1\n#include \x22ace/config-win32.h\x22\n'" />
    <command name="file_manipulation" options="type=create file=ACE_wrappers/bin/MakeProjectCreator/config/default.features output='rwho=0\nqos=1\nboost=1\ncidl=1\nzlib=1\nzzip=1\n'" />

    <command name="print_os_version" />
    <command name="check_compiler" options="msvc" />
    <command name="print_ace_config" />
    <command name="print_perl_version" />

    <command name="generate_workspace" options="-type vc71"/>
    <command name="generate_workspace" options="dirs=TAO/CIAO/CIDLC -type vc71 CIDLC.mwc"/>

    <command name="win32make"         options="msvc_mpc_auto_compile -vc7 -dir TAO\CIAO\CIDLC -Debug -k" />
    <command name="win32make"         options="msvc_mpc_auto_compile -vc7 -ALL -Debug -k" />

    <command name="auto_run_tests"    options="-s \x22C:\ACE\autobuild\win32sandbox\sandbox.exe -t 5400000\x22" />

    <command name="log"               options="OFF" />
    <command name="process_logs"      options="move prettify clean=5 index" />
    <command name="file_manipulation" options="type=delete file=.disable" />
    <command name="status"            options="off" />
    <command name="shell"             options="rsync --progress --recursive --delete -z --exclude /ACE_wrappers/ --include *.txt --include *.html --exclude ** ./ bczar@naboo.dre.vanderbilt.edu://web/users/isisbuilds/auto_compile_logs/tao_win2003_full" />
</autobuild>
