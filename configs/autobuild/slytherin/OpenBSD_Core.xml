<!-- $Id$ -->
<autobuild>
    <configuration>

        <environment name="ACE_ROOT"        value="/home/acebuild/Core/ACE_wrappers" />
        <environment name="LD_LIBRARY_PATH" value="/home/acebuild/Core/ACE_wrappers/ace" />
        <environment name="CVSROOT"         value=":ext:fhunleth@cvs.doc.wustl.edu:/project/cvs-repository" />
        <environment name="CVS_RSH"         value="ssh" />

        <variable name="root"     value="/home/acebuild/Core" />
        <variable name="sam_root" value="/home/acebuild/Core/ACE_wrappers/samwise" />
        <variable name="log_file" value="build.txt" />
        <variable name="log_root" value="/home/acebuild/public_html/auto_compile_logs/slytherin_Core" />
        <variable name="configs"  value="OpenBSD" />
        <variable name="MAIL_ADMIN" value="frank@hunleth.com" />

    </configuration>

    <command name="status"            options="on" />
    <command name="file_manipulation" options="type=mustnotexist file=.disable" />
    <command name="file_manipulation" options="type=create file=.disable output='RUNNING'" />
    <command name="log"               options="on" />
    <command name="cvs"               options="-z 3 -q checkout -P ACE_wrappers" />

    <command name="file_manipulation" options="type=create file=ACE_wrappers/ace/config.h output='#include \x22ace/config-openbsd.h\x22\n'" />
    <command name="file_manipulation" options="type=create file=ACE_wrappers/include/makeinclude/platform_macros.GNU output='debug=0\nexceptions=0\nrt_corba=1\nsmart_proxies=1\ninterface_repo=1\ninterceptors=1\nthreads=1\n#ssl=1\ninclude $(ACE_ROOT)/include/makeinclude/platform_openbsd.GNU\n'" />
    <command name="print_os_version" />
    <command name="check_compiler" options="gcc" />
    <command name="print_ace_config" />

    <command name="make" options="-k reverseclean" />
    <command name="make" options="-k -C TAO reverseclean" />
    <command name="make" options="-C ace" />
    <command name="make" options="-C apps/gperf/src" />
    <command name="make" options="-C TAO/tao" />
    <command name="make" options="-C TAO/TAO_IDL" />
    <command name="make" options="-C TAO/orbsvcs/orbsvcs" />

    <command name="log"               options="off" />
    <command name="process_logs"      options="clean move prettify index" />
    <command name="file_manipulation" options="type=delete file=.disable" />
    <command name="status"            options="off" />
</autobuild>
