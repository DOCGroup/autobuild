<!-- $Id$ -->
<autobuild>
    <configuration>
        <variable name="root" value="/project/maedhros/bugzilla/Core/ACE_wrappers" />
        <variable name="ace_root" value="/project/maedhros/bugzilla/Core/ACE_wrappers" />
        <variable name="sam_root" value="/project/maedhros/bugzilla/Core/ACE_wrappers/samwise" />
        <variable name="log_file" value="/project/maedhros/bugzilla/Core/build_log.txt" />
    </configuration>
    <command name="log" options="ON" />
    <command name="file_manipulation" options="type=mustnotexist file=.disable" />
    <command name="file_manipulation" options="type=create file=.disable output='RUNNING'" />
    <command name="file_manipulation" options="type=create file=ace/config.h output='#include \x22ace/config-linux.h\x22\n'" />
    <command name="file_manipulation" options="type=create file=include/makeinclude/platform_macros.GNU output='include $(ACE_ROOT)/include/makeinclude/platform_linux.GNU\ndebug=1\ninline=0\n'" />
<!--    <command name="sam" options="clean" /> -->
    <command name="cvs" options="update" />
    <command name="print_ace_config" />
<!--    <command name="sam" options="create" /> -->
<!--    <command name="sam" options="sam" /> -->
    <command name="make" options="makeopts='-j 2 -k -C . realclean'" />
    <command name="make" options="makeopts='-j 2 -k -C TAO realclean'" />
    <command name="make" options="makeopts='-j 2 -k -C . Core'" />
    <command name="file_manipulation" options="type=delete file=.disable" />
    <command name="log" options="OFF" />
</autobuild>
