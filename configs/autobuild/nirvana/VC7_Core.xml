<!-- $Id$ -->
<autobuild>
    <configuration>
        <environment name="ACE_ROOT"     value="G:/bugzilla/new_VC7_Core/ACE_wrappers" />
        <environment name="TAO_ROOT"     value="G:/bugzilla/new_VC7_Core/ACE_wrappers/TAO"/>
        <environment name="TAO_ROOT"     value="G:/bugzilla/new_VC7_Core/ACE_wrappers/TAO" />
        <environment name="Path"         value="G:/bugzilla/new_VC7_Core/ACE_wrappers\bin;G:/bugzilla/VC7.NET/ACE_wrappers\lib;D:\ssl\openssl-0.9.6\out32dll" type="prefix" />

        <variable name="root"            value="G:/bugzilla/new_VC7_Core/" />
        <variable name="project_root"            value="ACE_wrappers" />
	<variable name="log_file"        value="vc7_debug.log" />
        <variable name="log_root" value="Z:/new_VC7_Core_Debug" />

        <variable name="name"            value="WinXP VC.NET Debug" />
    </configuration>

    <command name="status"            options="on" />
    <command name="file_manipulation" options="type=mustnotexist file=.disable" />
    <command name="file_manipulation" options="type=create file=.disable output='RUNNING'" />

    <command name="log"               options="ON" />

    <command name="cvs"               options="-z9 checkout -P ACE_wrappers" />
    <command name="cvs"               options="-z9 checkout -P ACE_MPC"/>
    <command name="file_manipulation" options="type=create file=ACE_wrappers/ace/config.h output='#define ACE_DISABLE_WIN32_ERROR_WINDOWS\n#define TAO_HAS_SMART_PROXIES 1\n#define TAO_HAS_AMI 1\n#define TAO_HAS_INTERFACE_REPOSITORY 1\n#define TAO_HAS_RT_CORBA 1\n#include \x22ace/config-win32.h\x22\n'" />
    <command name="file_manipulation" options="type=create file=ACE_wrappers/bin/MakeProjectCreator/config/default.features output='rwho=0\nqos=1\n'" /> 

    <command name="print_os_version" />
    <command name="check_compiler" options="msvc" />
    <command name="print_ace_config" />
    <command name="print_perl_version" />

    <command name="vc7make"           options="/clean debug TAO/TAOACE.sln" />
    <command name="vc7make"           options="/clean debug TAO/CIAO/CIAOTAO.sln" />
    <command name="generate_workspace" options="-type vc71 -recurse"/>

    <command name="vc7make"            options="/rebuild debug TAO/TAOACE.sln" />
    <command name="vc7make"            options="/rebuild debug TAO/CIAO/CIAOTAO.sln" />

    <command name="log"               options="OFF" />
    <command name="process_logs"      options="move prettify clean index" />
    <command name="file_manipulation" options="type=delete file=.disable" />
    <command name="status"            options="off" />
</autobuild>
